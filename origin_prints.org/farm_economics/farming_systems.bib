@misc{orgprints27618,
            year = {2008},
          author = {Tiago Jos{\'e} Bertuol and Carla Galbiati and M{\^o}nica Josene Barbosa Pereira and Anderson Marques do Amaral},
         journal = {Revista Brasileira de Agroecologia},
           title = {Avalia{\c c}{\~a}o de mutualismo entre acacia mangium wild (Mimosaceae) e formigas (Hymenoptera: Formicidae).},
           month = {May},
          volume = {3},
           pages = {41--47},
          number = {1},
             url = {https://orgprints.org/id/eprint/27618/},
        keywords = {Mirmec{\'o}fitas, Formigas predadoras, Nect{\'a}rios-extraflorais, Herb{\'i}voria.
Mirmecafitas, Ants predator, Extrafloral nectary e Herbivorous.},
        abstract = {O mutualismo entre formigas e plantas consiste na prote{\c c}{\~a}o da {\'a}rvore pela formigas contra herb{\'i}voros, que recebem em troca alimento ou local para nidifica{\c c}{\~a}o. O objetivo desta pesquisa foi verificar a rela{\c c}{\~a}o entre Acacia mangium Willd e formigas visitantes. O estudo foi realizado em C{\'a}ceres, MT, em 14 plantas de A. mangium escolhidas ao acaso em um sistema agroflorestal. Em cada planta foi mensurada a {\'a}rea foliar consumida por herb{\'i}voros atrav{\'e}s de um gabarito, em quatro folhas/planta. A agressividade das formigas em rela{\c c}{\~a}o aos herb{\'i}voros foi medida pela observa{\c c}{\~a}o da retirada de cupins fixados nas plantas por uma hora. A rela{\c c}{\~a}o entre a desfolha de A. mangium e a abund{\^a}ncia de formigas e ao tempo de retirada do cupim foi analisada atrav{\'e}s de regress{\~a}o m{\'u}ltipla. As formigas encontradas foram Crematogaster, Brachymyrmecini e Camponotus. N{\~a}o houve rela{\c c}{\~a}o entre a desfolha m{\'e}dia e o n{\'u}mero de formigas encontradas sobre as plantas. Por{\'e}m, foi constatada rela{\c c}{\~a}o negativa entre a desfolha e o tempo para a retirada do cupim. Devido {\`a} exist{\^e}ncia de agressividade das formigas perante herb{\'i}voros na A. mangium conclui-se que esta esp{\'e}cie ex{\'o}tica abriga formigas nativas com comportamento defensivo. Entretanto, {\'e} necess{\'a}rio investigar se estas formigas recebem em troca fonte de alimento ou local para nidifica{\c c}{\~a}o para caracterizar mutualismo.}
}

