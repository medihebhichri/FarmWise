@misc{orgprints22748,
           pages = {45--62},
          volume = {43},
           month = {January},
             doi = {http://dx.doi.org/10.1590/S0103-20032005000100003},
          number = {1},
         journal = {Revista de Economia e Sociologia Rural},
            year = {2005},
          author = {Maria Aparecida Silva  Oliveira and Erly Cardoso  Teixeira},
           title = {Pol{\'i}tica de estabiliza{\c c}{\~a}o de renda para a agricultura familiar: uma an{\'a}lise de risco.},
             url = {https://orgprints.org/id/eprint/22748/},
        keywords = {Estabiliza{\c c}{\~a}o de renda, Agricultura familiar, Risco.
Income stabilization policy, Family farm, Risk.},
        abstract = {Os produtores familiares foram prejudicados pelas pol{\'i}ticas de substitui{\c c}{\~a}o de importa{\c c}{\~o}es e de moderniza{\c c}{\~a}o da agricultura, sofrendo com elevadas transfer{\^e}ncias de renda para fora do setor. Uma forma de ressarci-los seria implementando uma pol{\'i}tica de estabiliza{\c c}{\~a}o de renda que asseguraria rentabilidade est{\'a}vel aos produtores. O objetivo deste artigo {\'e} determinar os benef{\'i}cios de uma pol{\'i}tica de estabiliza{\c c}{\~a}o de renda implementada na agricultura familiar para agricultores e consumidores de arroz, feij{\~a}o, milho e mandioca, e os custos para agricultores comerciais nesses mercados. Utilizou-se o modelo de Newbery e Stiglitz, que considera a redu{\c c}{\~a}o do risco com a pol{\'i}tica de estabiliza{\c c}{\~a}o de renda. A pol{\'i}tica implicaria aumento de receita para produtores familiares. Os benef{\'i}cios de transfer{\^e}ncia superariam os de efici{\^e}ncia para produtores familiares e consumidores. Os benef{\'i}cios para a agricultura familiar superariam os custos da agricultura comercial com a pol{\'i}tica nos mercados de feij{\~a}o, mandioca e milho, e os benef{\'i}cios totais superariam os custos totais nos mercados de feij{\~a}o e milho, ao contr{\'a}rio ocorreria nos mercados de arroz e mandioca, embora neste fosse pequena a diferen{\c c}a. Conclui-se que a implementa{\c c}{\~a}o da pol{\'i}tica seria vi{\'a}vel nos mercados onde a participa{\c c}{\~a}o da agricultura familiar fosse maior que a da comercial e naqueles importadores.}
}

@misc{orgprints25602,
           title = {Experi{\^e}ncias agroecol{\'o}gicas brasileiras: uma an{\'a}lise {\`a} luz do desenvolvimento local.},
