        keywords = {Agricultura convencional e ecol{\'o}gica, Otimiza{\c c}{\~a}o, Simula{\c c}{\~a}o.
Conventional and ecologic agriculture, Optimization, Simulation.},
        abstract = {O processo de obten{\c c}{\~a}o de novas metodologias agr{\'i}colas, com o intuito de acelerar a produ{\c c}{\~a}o de alimento para atender o crescimento populacional, segue no sentido, dentre outros, da maximiza{\c c}{\~a}o da produtividade, do lucro e da conserva{\c c}{\~a}o do meio ambiente. Da{\'i} a import{\^a}ncia das pesquisas em mecanismos que resultem em um equil{\'i}brio sustent{\'a}vel entre aspectos financeiros, sociais e ambientais. Dentro deste contexto os modelos de otimiza{\c c}{\~a}o podem vir a proporcionar uma melhor solu{\c c}{\~a}o para os processos que ocorrem neste setor produtivo. O objetivo do trabalho foi a compara{\c c}{\~a}o atrav{\'e}s de simula{\c c}{\~o}es de cen{\'a}rios com diferentes fun{\c c}{\~o}es objetivo entre a agricultura convencional e a ecol{\'o}gica, de uma {\'a}rea irrigada no munic{\'i}pio de Boqueir{\~a}o, PB, tendo como base de compara{\c c}{\~a}o benef{\'i}cios econ{\^o}micos (receita l{\'i}quida), sociais (gera{\c c}{\~a}o de m{\~a}o-de-obra) e ambientais (uso de produtos qu{\'i}micos). As compara{\c c}{\~o}es foram efetuadas usando-se a ferramenta matem{\'a}tica e computacional CISDERGO. Os resultados mostraram que o manejo ecol{\'o}gico gera uma maior rentabilidade que o convencional.}
}

@misc{orgprints28506,
         journal = {Bragantia},
            year = {2012},
          author = {Daniel Rodr{\'i}guez Caicedo and Jose Miguel Cotes Torres and Jos{\'e} Ricardo Cure},
           title = {Comparison of eight degree-days estimation methods in four agroecological regions in Colombia.

 },
          volume = {71},
           pages = {299--307},
           month = {March},
          number = {2},
             doi = {http://dx.doi.org/10.1590/S0006-87052012005000011 },
             url = {https://orgprints.org/id/eprint/28506/},
        keywords = {Graus-dia, Tempo t{\'e}rmico, Temperatura basal, Modelagem estat{\'i}stica.
Degree-days, Thermal time, Temperature threshold, Statistical modeling.},
        abstract = {Eight methods were used to estimate degree-days in four Colombian localities. Four methods have been previously proposed in literature: Simple Sine, Double Sine, Simple Triangle, and Double Triangle methods. The other four methods are proposed in this research: Simple Logistic, Double Logistic, Simple Normal, and Double Normal. The estimation of the degree-days through hourly temperature values was used as the reference standard method, and the four localities from where the temperature values were taken were the municipalities of Cajic{\'a} (Cundinamarca), Santa Elena (Antioquia), Carepa (Urab{\'a} Antioque{\~n}o), and Ciudad Bolivar (Zona cafetera Antioque{\~n}a). Degree-days obtained by all methods under study were compared through linear regression to those obtained by the reference standard method. There were differences in the correlation of each method to the reference when compared within each region and among regions.  The Simple Logistic and Double Logistic methods showed the best performance with acceptable R2 values and considerably lower bias than the other methods.  The poorest fit was found in Cajic{\'a}, where the average R2 was 0.571.  For the regions of Santa Elena and Carepa, the average R2 was 0.756 and 0.733.  The best fit was found in Ciudad Bolivar, with an average R2 of 0.826.}
}

