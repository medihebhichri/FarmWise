            year = {2014},
           title = {Avalia{\c c}{\~a}o produtiva de rabanete Raphanus sativus L. submetido a preparados homeop{\'a}ticos de tiririca Cyperus rotundus L.
},
          volume = {9},
           pages = {151--159},
           month = {June},
          number = {2},
        keywords = {Dinamiza{\c c}{\~a}o, Homeopatia vegetal, Hortali{\c c}a.
Dinamization, Homeopathy plant, Vegetable.},
        abstract = {O objetivo deste trabalho foi avaliar a produ{\c c}{\~a}o de rabanete, sob diversos par{\^a}metros, submetidos a preparados homeop{\'a}ticos de Cyperus rotundus. Os tratamentos foram obtidos atrav{\'e}s da tintura dos bulbos de C. rotundus na propor{\c c}{\~a}o de 1:10, dinamizados {\`a} 5 CH, 20 CH, 30 CH, comparados a testemunha. A aplica{\c c}{\~a}o dos tratamentos foi feita em canteiros com 1 m? contendo quatro repeti{\c c}{\~o}es, no delineamento em blocos ao acaso. Os tratamentos foram aplicados nos dias 0, 7, 14, 21, 28, com concentra{\c c}{\~a}o de 2\%. Foram avaliados a massa de ra{\'i}zes comest{\'i}veis, ra{\'i}zes, parte a{\'e}rea, largura longitudinal, largura transversal e n{\'u}meros de rachados. Os tratamentos 5 e 30 CH apresentaram maior massa das ra{\'i}zes comest{\'i}veis e folhas frescas. A massa das ra{\'i}zes n{\~a}o diferiu estatisticamente entre os tratamentos. Os preparados homeop{\'a}ticos diferiram da testemunha na largura longitudinal. O tratamento 20 CH apresentou maior largura transversal. A utiliza{\c c}{\~a}o dos preparados homeop{\'a}ticos pode ser considerada uma pr{\'a}tica promissora na produ{\c c}{\~a}o de rabanete em agricultura de base ecol{\'o}gica.},
             url = {https://orgprints.org/id/eprint/27400/}
}

@misc{orgprints20582,
          volume = {52},
           pages = {116--120},
           title = {BLACK POLYPROPYLENE MULCH TEXTILE IN ORGANIC AGRICULTURE},
            year = {2010},
          author = {Petr Dvorak and Jana Haj{\v s}lov{\'a} and Karel Hamouz and V{\v e}ra Schulzov{\'a} and Perla Kuchtova and Jaroslav Tomasek},
         journal = {Lucr{\u a}ri {\c S}tiin{\c t}ifice},
        keywords = {black polypropylene textile, potato, yield, quality, microclimate},
        abstract = {Black polyethylene mulch is used for weed control in a range of crops under the organic system. The use of black polypropylene mulch is usually restricted to perennial crops. The trial was conducted at Experimental station of Department of Crop Production of the Czech University of Life Science Prague-Uh{\v r}{\'i}n{\v e}ves in Czech Republic. For the experiments were used black polypropylene woven mulch (comparison wit bare soil), two varieties of early potatoes Finka and Katka. Black polypropylene textile was used in potatoes by organic agriculture and it had positive effect on soil temperature (in the depth of 100 mm). Slightly higher soil temperatures under black polypropylene mulch in the vegetation period after planting had favourable influence on earlier stands emergence. The soil water potential (in the depth of 250 mm) and also the soil water content have been beneficial for black polypropylene mulch. Significantly lower values of the soil water potentials have been found in the period after planting and at the end of vegetation. Black polypropylene mulch provided favourable temperatures and soil moisture. Post harvest analyses were focused on the determination of the yield and quality tubers from each variant.},
             url = {https://orgprints.org/id/eprint/20582/}
}

@misc{orgprints21796,
          volume = {51},
           pages = {121--125},
           title = {Influence of grass mulch application on tubers size and yield of ware potatoes },
         journal = {Lucr{\u a}ri {\c S}tiin{\c t}ifice, Seria Agronomie},
            year = {2009},
          author = {Petr Dvorak and Jana Hajslova and Karel Hamouz and V{\v e}ra Schulzov{\'a} and Perla Kuchtov{\'a} and Jaroslav Tomasek},
        keywords = {grass mulch, potatoes, yield, quality, weed biomass},
        abstract = {The aim of the experiment was to investigate the effect of grass mulch and the term of its application on the yield and quality of potatoes and weed biomass under organic system. The experiment with 2 varieties of early potatoes (Finka and Katka), 2 row spacings (28 000 and 38 000 tubers per hectare) and 2 terms of grass mulch application (after planting and after second hoeing) was carried out in the Czech Republic in 2008. Results showed that the yield was influenced mostly by mulching (55.9 \%), then by variety (39.2 \%) and by growth structure (4.9 \%). The highest yield was reached in the variant with grass mulch applicated after planting. The yield of this variant increased statistically about 9.3 t/ha in comparison with control variant (bare soil). Results of the weed control showed a positive effect of grass mulch on weed biomass, where the lowest weed biomass was found out in variant with grass mulch applicated after planting (weed biomass was lower by 67.6 \% in comparison with the control variant - bare soil).},
             url = {https://orgprints.org/id/eprint/21796/}
}

