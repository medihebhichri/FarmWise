@misc{orgprints54545,
           pages = {1--90},
           month = {January},
         address = {Tingvoll},
          series = {NORS{\O}K rapport},
            year = {2025},
          author = {Juni Rosann E. Johanssen},
       publisher = {NORS{\O}K},
           title = {Holdninger til systemer for hold av melkeku og kalv - En sp{\o}rreunders{\o}kelse blant 1000 nordmenn},
             url = {https://orgprints.org/id/eprint/54545/},
        keywords = {Dyrevelferd, forbrukerkukalv, ku-kalv kontakt, melkeproduksjon, sp{\o}rreunders{\o}kelse, Animal welfare, cow-calf contact, dairy farming, citizen survey},
        abstract = {Den vanlige praksisen i melkeproduksjon, der ku og kalv skilles umiddelbart eller innen kort tid etter f{\o}dsel, har f{\r a}tt {\o}kende kritikk de siste {\r a}rene. Samtidig har interessen for ku-kalv kontakt-systemer, hvor ku og kalv f{\r a}r v{\ae}re sammen i en periode etter f{\o}dsel, {\o}kt. En nylig unders{\o}kelse blant borgere i USA og Canada kartla holdninger til ulike systemer for {\r a} holde ku og kalv. Respondentene var like negative til alle tre systemene som innebar tidlig separasjon (der kalvene ble holdt alene, i grupper med andre kalver eller med ammetanter) (Sirovica et al. 2022). Derimot var holdningene positive til systemet hvor kalven fikk v{\ae}re med sin egen mor. I det norske SUCCEED-prosjektet antydet fokusgruppeintervjuer at norske borgere har st{\o}rre tillit til b{\o}nder og landbruket i Norge enn borgere i andre land, noe som kan f{\o}re til h{\o}yere aksept for systemer med tidlig separasjon. Respondenter fra den norske dagligvarekjeden mente at det trolig ikke finnes et marked for meieriprodukter fra systemer med ku-kalv-kontakt i Norge, da kundene neppe er villige til {\r a} betale h{\o}yere priser (Kvam \& Logstein 2023). M{\r a}let med v{\r a}rt prosjekt var {\r a} unders{\o}ke nordmenn sin kjennskap til norsk melkeproduksjon, deres holdninger til ulike systemer for kalvehold f{\o}r og etter tilleggsinformasjon, samt deres villighet til {\r a} betale for melk fra systemer der kalven har v{\ae}rt sammen med sin egen mor. Dette ble gjort gjennom en sp{\o}rreunders{\o}kelse med 1 000 norske borgere, hvor vi ogs{\r a} samlet inn
bakgrunnsinformasjon om respondentene for {\r a} kunne analysere hvilke faktorer som eventuelt p{\r a}virket svarene deres. Flertallet av respondentene i unders{\o}kelsen oppga at de var ?ikke noe s{\ae}rlig? eller ?litt? kjent med norsk melkeproduksjon. N{\r a}r det gjaldt holdninger til ulike systemer for hold av ku og kalv, viste respondentene st{\o}rst negativitet til systemet ?kalv alene?, etterfulgt av ?kalv i gruppe?, deretter ?kalv med ammetante?, og mest positivitet til ?kalv med mor?. Etter {\r a} ha mottatt tilleggsinformasjon endret holdningene seg noe, men rekkef{\o}lgen p{\r a} hvilke systemer som ble vurdert mest negativt og mest positivt forble uendret. Tilleggsinformasjonen f{\o}rte til at respondentene ble noe mindre negativt til systemer som inneb{\ae}rer tidlig separasjon av ku og kalv, samtidig som de ble litt mindre positive til systemer der kalven f{\r a}r v{\ae}re med mor. Selv om forskjellene mellom systemene fortsatt var tydeligere, ble gapet mellom holdningene til tidlig separasjon og samv{\ae}r mellom kalv og mor mindre etter at informasjonen ble gitt. De fleste respondentene oppga at de var villige til {\r a} betale det samme for melk fra systemer der kalven f{\r a}r v{\ae}re med sin egen mor (CCC-melk) som for vanlig melk i dag. Litt over en fjerdedel svarte at de var villige til {\r a} betale mer for CCC-melk, noe som indikerer at det kan finnes et marked for denne typen meieriprodukter i Norge. Bakgrunnsvariabler hadde en betydelig effekt p{\r a} svarene, og s{\ae}rlig tydelig var forskjellen mellom menn og kvinner. Kvinner var generelt mer negative til systemer som inneb{\ae}rer tidlig separasjon av ku og kalv, mer positive til systemet ?kalv med mor? og hadde h{\o}yere betalingsvilje for CCC-melk. Resultatene fra studien tyder p{\r a} at {\o}kt informasjon kan bidra til at flere aksepterer praksiser som inneb{\ae}rer tidlig separasjon av ku og kalv. Likevel er det fortsatt flest som foretrekker systemet der kalven f{\r a}r v{\ae}re med sin egen mor.}
}

@misc{orgprints43022,
            year = {2021},
          author = {Juni Rosann E. Johanssen and Kristin S{\o}rheim},
          series = {NORS{\O}K Rapport},
         address = {Tingvoll},
           title = {Ku og kalv sammen i melkeproduksjon - Intervjuer med melkeprodusenter},
       publisher = {NORS{\O}K},
           month = {December},
           pages = {1--42},
             url = {https://orgprints.org/id/eprint/43022/},
        keywords = {Ku-kalv kontakt, melkeproduksjon, dyrevelferd, 
husdyrmilj{\o}, kvalitative intervjuer, SUCCEED, GrazyDaisy
Cow-calf contact, dairy farming, animal welfare, 
animal housing environment, qualitative 
interviews},
        abstract = {Tradisjonelt har det v{\ae}rt vanlig {\r a} skille ku og kalv innen kort tid etter f{\o}dsel i melkeproduksjon. Det er {\o}kende debatt og oppmerksomhet rundt det etiske ved et driftsopplegg hvor ku og kalv skilles tidlig. Forbrukerunders{\o}kelser fra andre land viser at de fleste forbrukere {\o}nsker at ku og kalv skal f{\r a} v{\ae}re sammen. Samtidig ser vi at flere melkeprodusenter, ogs{\r a} i Norge, er interessert i dette. Samv{\ae}r mellom ku og kalv kan fremme dyrevelferd og naturlig atferd, samtidig er det en del 
utfordringer som stress ved separasjon og mindre levert melk fra kyr som g{\r a}r med kalv. 

Denne rapporten ble skrevet i forbindelse med et PhD-studie og som en del av to prosjekter: ?Melkeku og kalv sammen p{\r a} beite ? et driftsopplegg for bedre dyrevelferd? (2020-2021) 
finansiert av dyrevernfondet og prosjektet ?SUCCEED ? Funksjonelle l{\o}sninger for kontakt mellom melkeku og kalv? (2020-2023) finansiert av Forskningsmidlene for jordbruk og matindustri (FFL/JA), NFR prosjektnummer 310728.

Vinteren 2020-2021 intervjuet NORS{\O}K og Ruralis 13 melkeprodusenter med ku og kalv sammen. For denne rapporten har vi valgt ut 10 av disse intervjuene hvor vi har skrevet om de 10 og hvordan de gj{\o}r det med ku og kalv sammen p{\r a} sine g{\r a}rder, med bilder. Deretter kommer en oppsummering av hva melkeprodusentene ser som fordeler og utfordringer med {\r a} ha ku og kalv sammen. I forbindelse med SUCCEED-prosjektet og PhD-studiet hadde vi ogs{\r a} et fors{\o}k med kyr og kalver p{\r a} beite sommeren 2021, og i slutten av resultatene i denne rapporten har vi skrevet litt om fors{\o}ket og et intervju med g{\r a}rdbrukerne vi hadde fors{\o}ket hos. 

I diskusjonen sammenligner vi resultater fra v{\r a}re intervjuer med tidligere studier p{\r a} ku og kalv, blant annet nyere studier med intervjuer av b{\o}nder i flere ulike europeiske land og et med intervjuer av b{\o}nder fra New Zealand. Deretter har vi skrevet om v{\r a}re perspektiver rundt hvordan 
det kan tilrettelegges for {\r a} ha ku og kalv lenger sammen i melkeproduksjon n{\r a}r det gjelder hva som er best for kalven, hva som er best for kua, hva som er best for bonden og hvordan en kan etablere et driftssystem som kan fungere for alle parter. 

Fra kua og kalvens perspektiv viser det overveiende materialet fra litteraturgjennomgang og intervjuer at det er fordeler for begge med en lengst mulig dieperiode og en gradvis separasjon. Fra bondens perspektiv er det b{\r a}de fordeler og ulemper. Det avgj{\o}rende er hvordan det praktisk kan tilrettelegges i fj{\o}set og p{\r a} beitet for b{\r a}de dyr og r{\o}kter, og der {\o}konomien ogs{\r a} spiller en viktig rolle. {\O}konomi handler f{\o}rst og fremst om reduserte inntekter fra levert melk n{\r a}r kalven dier mora, og det kan handle om investeringskostnader for {\r a} tilrettelegge i fj{\o}set og p{\r a} beite for {\r a} ha ku og kalv sammen. 

Generelt er det fortsatt behov for mer kunnskap og praktisk r{\r a}dgivning for b{\r a}de fj{\o}sl{\o}sninger, metoder for separasjon og annen kunnskap om kort- og langtidseffekter for dyr og bonde i slike driftsopplegg. Dette belyses i p{\r a}g{\r a}ende forskningsprosjekt (bl.a. SUCCEED), og det vil fortl{\o}pende bli publisert artikler med ny kunnskap de n{\ae}rmeste par {\r a}rene, som vil v{\ae}re med og bygge opp et bedre kunnskapsgrunnlag.}
}

@misc{orgprints35418,
          author = {Juni Rosann E. Johanssen and Emma Brunberg and Kristin S{\o}rheim},
            year = {2019},
          series = {NORS{\O}K Rapport},
         address = {Tingvoll},
           title = {Separasjon av ku og kalv - Atferd hos ku og kalv i melkeproduksjon etter separasjon ved to ulike metoder},
       publisher = {NORS{\O}K},
           pages = {1--36},
           month = {May},
        abstract = {I konvensjonell melkeproduksjon er det ingen krav om at ku og kalv skal v{\ae}re sammen, og tradisjonelt blir ku og kalv separert umiddelbart eller kort tid etter f{\o}dsel. I {\o}kologisk melkeproduksjon skal kalven die kua i minst tre dager, og den skal ha naturlig melk i minst tre m{\r a}neder. Forbrukerne er stadig mer opptatt av dyrevelferd, og det er et {\o}kende fokus p{\r a} det {\r a} ha ku og kalv sammen lenger i melkeproduksjon. Det er mange fordeler med {\r a} ha ku og kalv sammen lenger, men det er ogs{\r a} utfordringer. En av utfordringene er at dyra kan vise en sterkere stressrespons etter separasjon sammenlignet med om man separerer de raskt etter f{\o}dsel. Det finnes ulike metoder for br{\r a} og trinnvis separasjon av ku og kalv, og forskning viser at noen metoder kan gi mindre stressreaksjon enn andre. 

Form{\r a}let med v{\r a}rt fors{\o}k p{\r a} Tingvoll gard var {\r a} sammenligne stressrelatert atferd hos kyr og kalver ved to ulike separasjonsmetoder etter en forlenget periode p{\r a} tre til fem uker sammen i en kalvingsbinge. Metode 1: Br{\r a} separasjon hvor kalvene ble flyttet til et annet fj{\o}s 300 meter unna, og metode 2: Totrinns-separasjon med fenceline hvor kalvene ble flyttet til en binge inne i kyrnes l{\o}sdrift hvor de fikk v{\ae}re i fire dager f{\o}r de ble flyttet til en annen del av fj{\o}set. Atferden til kyrne og kalvene ved dag 1, 2 og 3 etter separasjonsdagen (dag 0) ble registrert ved kontinuerlige observasjoner og {\o}yeblikksobservasjoner. 

Resultatene fra fors{\o}ket viste forskjeller i atferd hos ku og kalv ved de ulike dagene etter separasjon ved at b{\r a}de kyrne og kalvene vokaliserte signifikant mer ved dag 1 sammenlignet med dag 2 og 3. Kyrne l{\r a} signifikant mindre p{\r a} dag 1 sammenlignet med dag 3, bevegede seg signifikant mer ved dag 1 sammenlignet med dag 2 og 3, og de brukte signifikant mindre tid p{\r a} {\r a} spise/drikke ved dag 1 og 2 sammenlignet med dag 3. Det var ingen signifikant forskjell for atferden ?st{\r a}r? (tre atferder samlet) hos kyrne, men for ?st{\r a}r, {\r a}rv{\r a}ken? var det signifikant mer av atferden p{\r a} dag 1 og 2 sammenlignet med dag 3. Den eneste atferden hos kyrne som ga signifikant forskjell for metode var ?spiser kraftf{\^o}r? hvor kyr ved metode 1 brukte mer tid p{\r a} {\r a} spise kraftf{\^o}r sammenlignet med kyr ved metode 2, men her var det f{\r a} registreringer. Kalvene brukte det meste av tiden sin p{\r a} {\r a} ligge, men resultatene viste at de bevegde seg signifikant mer ved dag 2 sammenlignet med dag 3, og at kalver ved metode 1 brukte signifikant mer tid p{\r a} {\r a} spise/drikke sammenlignet med kalver ved metode 2, men her var det ogs{\r a} f{\r a} registreringer. 

Resultatene fra fors{\o}ket v{\r a}rt viste forskjeller som tyder p{\r a} at kyrne og kalvene var mer stresset ved dag 1 etter separasjon sammenlignet med dag 2 og 3, men for de to ulike separasjonsmetodene fikk vi ingen tydelige forskjeller som tydet p{\r a} at dyra var mer stresset ved den ene metoden sammenlignet med den andre.},
        keywords = {Melkeproduksjon, ku-kalv, dyrevelferd, stress separasjon av ku og kalv, separasjonsmetoder, KuKalvTG,

Dairy farming, cow-calf, animal welfare, stress, separation of cow and calf, separation methods, grazydaisy},
             url = {https://orgprints.org/id/eprint/35418/}
}

@misc{orgprints34990,
            year = {2019},
          author = {Juni Rosann E. Johanssen and Kristin S{\o}rheim},
          series = {NORS{\O}K Rapport},
         address = {Tingvoll},
           title = {Driftssystemer for slaktekylling som ivaretar helse og dyrevelferd, krav om {\o}kt andel egenprodusert f{\^o}r og utfasing av konvensjonelle proteinf{\^o}rmidler i {\o}kologisk fj{\o}rfeproduksjon},
       publisher = {NORS{\O}K},
           pages = {1--42},
           month = {March},
             url = {https://orgprints.org/id/eprint/34990/},
        abstract = {NORS{\O}K fikk i oppdrag fra Regelverksutvalget for {\o}kologisk landbruk {\r a} unders{\o}ke om det er forhold i {\o}kologiske driftssystemer for slaktekyllingproduksjon som kan bidra til {\r a} redusere risikoen for sykdom med p{\r a}f{\o}lgende antibiotikabehandling, og samtidig im{\o}tekomme krav om {\o}kt andel egenprodusert f{\^o}r og utfasing av konvensjonelle proteinf{\^o}rmidler. I Norge er det liten {\o}kologisk fj{\o}rfeproduksjon, men fj{\o}rfe er husdyret som har st{\o}rst forskjell i dyrevelferd n{\r a}r man sammenligner {\o}kologisk og konvensjonell produksjon. NORS{\O}K har vurdert velferd hos produsenter med {\o}kologisk slaktekylling, intervjuet produsenter, sendt ut sp{\o}rreskjema til andre akt{\o}rer og gjennomf{\o}rt litteraturstudie. Saktevoksende raser, milj{\o}berikelser, lav dyretetthet og tilgang til et attraktivt uteomr{\r a}de er veldig positivt for kyllingers velferd. Det {\r a} ha hjemmeklekking, det {\r a} bruke kunstige kyllingm{\o}dre, samt ha enda mer attraktive uteomr{\r a}der vil antagelig kunne gi enda bedre velferd for {\o}kologiske kyllinger. Det kan dukke opp sykdom som m{\r a} behandles med antibiotika ogs{\r a} i {\o}kologisk slaktekyllingproduksjon, men forbruket av medikamenter er sv{\ae}rt lavt. Et krav om {\o}kt andel egenprodusert f{\^o}r til fj{\o}rfe vil kunne bli utfordrende {\r a} tilfredsstille fordi vi i Norge har lite {\o}kologisk kornareal, og avlingene kan variere mye p{\r a} grunn av blant annet klima. Ved d{\r a}rlige avlinger her kan det gjerne ogs{\r a} v{\ae}re d{\r a}rlige avlinger i n{\ae}rliggende omr{\r a}der i Norges naboland. F{\o}r et eventuelt krav om {\o}kt andel egenprodusert f{\^o}r i {\o}kologisk fj{\o}rfeproduksjon kommer, b{\o}r det v{\ae}re mer {\o}kologisk kornareal. Kravet til {\o}kt andel egenprodusert f{\^o}r til dr{\o}vtyggere kan komme f{\o}rst da det er lettere {\r a} tilfredsstille. Ang{\r a}ende utfasing av konvensjonelle proteinf{\^o}rmidler slik situasjonen er i dag vil det bli sv{\ae}rt vanskelig {\r a} optimere et ern{\ae}ringsmessig godt nok kraftf{\^o}r for {\o}kologiske fj{\o}rfes velferd og produksjon om man ikke kan fortsette med {\r a} bruke 5 \% konvensjonelle proteinr{\r a}varer i kraftf{\^o}ret. F{\o}r et eventuelt krav om 100 \% {\o}kologiske proteinr{\r a}varer i {\o}kologisk kraftf{\^o}r til fj{\o}rfe kommer m{\r a} en vite at det finnes et godt nok alternativ(er) til den andelen p{\r a} 5 \% som i dag er konvensjonelle r{\r a}varer. Et alternativ kan v{\ae}re {\r a} tillate aminosyren metionin som syntetisk i f{\^o}r til fj{\o}rfe, eventuelt {\r a} etter hvert bruke proteinr{\r a}varer som ikke krever kornareal.},
        keywords = {{\O}kologisk husdyrhold, regelverk, fj{\o}rfeproduksjon, slaktekylling, dyrevelferd, dyrehelse, egenprodusert f{\^o}r, proteinf{\^o}rmidler}
}

