@misc{orgprints20634,
           pages = {1--12},
          volume = {6},
             doi = {10.1017/S175173111100187X},
         journal = {Animal},
            year = {2012},
          author = {R.L.M. Schils and J. Eriksen and S. Ledgard and T.V. Vellinga and P.J. Kuikman and J. Lou and S.O. Petersen and G.L. Velthof},
           title = {Strategies to mitigate nitrous oxide emissions from herbivore production systems},
        abstract = {Herbivores are a significant source of nitrous oxide (N2O) emissions. They account for a large share of manure-related N2O emissions, as well as soil-related N2O emissions through the use of grazing land, and land for feed and forage production. It is widely acknowledged that mitigation measures are necessary to avoid an increase in N2O emissions while meeting the growing global food demand. The production and emissions of N2O are closely linked to the efficiency of nitrogen (N) transfer between the
major components of a livestock system, that is, animal, manure, soil and crop. Therefore, mitigation options in this paper have been structured along these N pathways. Mitigation technologies involving diet-based intervention include lowering the CP content or increasing the condensed tannin content of the diet. Animal-related mitigation options also include breeding for improved N conversion and high animal productivity. The main soil-based mitigation measures include efficient use of fertilizer and manure, including the use of nitrification inhibitors. In pasture-based systems with animal housing facilities, reducing grazing time is an effective option to reduce N2O losses. Crop-based options comprise breeding efforts for increased N-use efficiency and the use of pastures with N2-fixing clover. It is important to recognize that all N2O mitigation options affect the N and carbon cycles of livestock systems. Therefore, care should be taken that reductions in N2O emissions are not offset by unwanted
increases in ammonia, methane or carbon dioxide emissions. Despite the abundant availability of mitigation options,
implementation in practice is still lagging. Actual implementation will only follow after increased awareness among farmers and greenhouse gases targeted policies. So far, reductions in N2O emissions that have been achieved are mostly a positive side effect of other N-targeted policies.},
             url = {https://orgprints.org/id/eprint/20634/}
}

@unpublished{orgprints730,
           title = {Forsknings- og udviklingsaktiviteter p{\r a} omr{\r a}det reduceret jordbearbejdning},
          author = {P. Schj{\o}nning and M.H. J{\o}rgensen and J.E. Olesen and G. Rasmussen and L.J. Munkholm and B. Melander},
            year = {2001},
           month = {February},
        abstract = {Der er fra det danske landbrugserhverv formuleret {\o}nske om, at der iv{\ae}rks{\ae}ttes en bedre r{\r a}dgivning omkring gennemf{\o}relse af reduceret jordbearbejdning, herunder direkte s{\r a}ning (FRDK, bl.a. ved m{\o}det i Forskningskontoret 18. januar 2001). Problemstillingen har interesse for s{\r a}vel konventionelle som {\o}kologiske jordbrugere, idet reduceret bearbejdning af jorden som udgangspunkt er positivt for jordkvaliteten. Der er gennemf{\o}rt en lang r{\ae}kke unders{\o}gelser omkring reduceret jordbearbejdning og direkte s{\r a}ning i Danmark. En r{\ae}kke forhold har dog til dels gjort disse resultater for{\ae}ldede. Det skyldes bl.a. den politisk bestemte udvikling i de dyrkede afgr{\o}der, herunder i s{\ae}rlig grad s{\ae}dskiftet, som har stor betydning for reduceret jordbearbejdning. De gennemf{\o}rte fors{\o}g koncentrerede sig endvidere prim{\ae}rt om effekten p{\r a} udbyttet og med mindre fokus p{\r a} milj{\o}m{\ae}ssige aspekter. Samtidig har den maskintekniske udvikling bevirket, at der i dag findes maskiner, der i h{\o}jere grad l{\o}ser nogle af de problemer, der tidligere blev konstateret i forbindelse med reduceret jordbearbejdning.
Reduceret jordbearbejdning ? herunder direkte s{\r a}ning ? har interessante milj{\o}m{\ae}ssige effekter. Det drejer sig dels om et reduceret energiforbrug ved afgr{\o}deetablering. Videre p{\r a}virkes landbrugets indflydelse p{\r a} m{\ae}ngden af drivhusgasser ved reduceret bearbejdning. I forhold til pl{\o}jning vil reduceret bearbejdning {\o}ge lagringen af kulstof i jorden (og dermed fjerne CO2 fra luften). Samtidig er der dog en effekt p{\r a} afgivelsen af luftformige kv{\ae}lstofforbindelser, hvoraf N2O ogs{\r a} er en drivhusgas. De milj{\o}m{\ae}ssige effekter best{\r a}r ogs{\r a} i en reduktion af overfladeafstr{\o}mning og erosion. Til geng{\ae}ld opst{\r a}r tilsyneladende en {\o}get fare for ?by-pass? transport af fosfor og pesticider til dybere jordlag via kontinuerte, vertikale bioporer i jorden ved pl{\o}jefri dyrkning.
Der kan konstateres et f{\ae}lles behov for myndigheder og landbrugserhverv i en kvantificering af muligheder og problemer ved reduceret jordbearbejdning og direkte s{\r a}ning i Danmark. Der er behov for unders{\o}gelser, der gennemf{\o}res i i{\o}vrigt realistiske dyrkningssystemer. S{\r a}danne v{\ae}rkstedsarealer vil give mulighed for belysning af b{\r a}de agronomiske og milj{\o}m{\ae}ssige problemstillinger. Der vil videre i et vist omfang v{\ae}re behov for belysning af mere ad-hoc pr{\ae}gede problemstillinger. S{\r a}danne opgaver b{\o}r dog ogs{\r a} i s{\r a} stor udstr{\ae}kning som muligt gennemf{\o}res i fastliggende fors{\o}g og i realistiske s{\ae}dskifter.
},
        keywords = {tillage; direct drilling; reduced tillage; no-till},
             url = {https://orgprints.org/id/eprint/730/}
}

