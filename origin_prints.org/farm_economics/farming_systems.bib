        abstract = {Vidensynteseprojektet blev indledt med interviews af dyrl{\ae}ger og konsulenter for at indkredse veterin{\ae}re problemstillinger, som er forbundet med oml{\ae}gning til {\o}kologisk drift. En oml{\ae}gning af bes{\ae}tningen er en del af oml{\ae}gningen af hele bedriften og b{\o}r som s{\r a}dan ikke ses isoleret. Konsulentens fokus er relativt bredt p{\r a} hele bedriften, dyrl{\ae}gens er mere sn{\ae}vert p{\r a} den enkelte ko og gruppen af k{\o}er/kvier/kalve. Dette perspektiv b{\o}r ikke st{\r a} alene i og med, at det er hele bedriften, som l{\ae}gges om.  Beslutninger i bes{\ae}tningen griber ind i hele bedriften, ligesom beslutninger
p{\r a} bedriftsniveau griber ind i rutiner og rammer i bes{\ae}tningen. Landmanden ? driftslederen
? og hans eller hendes beslutninger st{\r a}r som omdrejningspunkt for oml{\ae}gningen. Driftslederen ser det indefra. En beskrivelse af oml{\ae}gningen, inklusive landmandens oplevelse af hvad der sker med bes{\ae}tningen, er n{\o}dvendig for at kunne diskutere oml{\ae}gningsprocessen.}
}

@misc{orgprints39655,
       publisher = {{\O}kologisk Landsforening},
           title = {K{\o}er med kalve - Erfaringer, overvejelser og inspiration fra danske og udenlandske {\o}kologiske malkekv{\ae}gbes{\ae}tninger},
         address = {Aarhus},
            year = {2020},
          author = {Mette Vaarst and Iben Alber Christiansen},
           month = {December},
           pages = {1--64},
        abstract = {De senere {\r a}r har mange landm{\ae}nd investeret i nye kalvestalde, kalvek{\o}kkener og m{\ae}lketildelingsudstyr for at sikre hygiejne og gode managementrutiner. Mange forbrugere og organisationer stiller samtidig flere og flere sp{\o}rgsm{\r a}lstegn ved hvorfor ko og kalv ikke er sammen i l{\ae}ngere end 24 timer i den {\o}kologiske m{\ae}lkeproduktion. Generelt eftersp{\o}rges ?naturlighed? som en vigtig del af dyrenes velf{\ae}rd, og mange forbinder naturlighed med {\o}kologi. Der kan argumenteres for, at det er i tr{\r a}d med de {\o}kologiske principper, som er formuleret i 2005 i IFOAM-regi, at lade ko og kalv f{\r a} opfyldt deres naturlige behov for at v{\ae}re sammen. At indf{\o}re et ko-kalv-system vil ikke desto mindre vende op og ned p{\r a} de fleste rutiner i de malkekv{\ae}gsbes{\ae}tninger, de fleste nu betragter som ?almindelige? og vil p{\r a} mange m{\r a}der kr{\ae}ve anderledes systemer og rutiner. Dette lille h{\ae}fte udspringer af erfaringer om og nedskrevne studier i kokalv systemer, og skal betragtes som et ?id{\'e}katalog? til inspiration, som kan rejse nogle vigtige diskussioner og overvejelser i forbindelse med etablering af et ko-kalv-system.},
             url = {https://orgprints.org/id/eprint/39655/}
}

@misc{orgprints39654,
          number = {1},
             doi = {10.3220/LBF1596195636000},
           pages = {49--57},
          volume = {70},
           title = {Cow calf contact in dairy herds viewed from the
perspectives of calves, cows, humans and the
farming system. Farmers? perceptions and
experiences related to dam-rearing systems},
            year = {2020},
          author = {Mette Vaarst and Florence Hellec and Cynthia Verwer and Juni Rosann E. Johanssen and Kristin S{\o}rheim},
         journal = {J.Sustainable Organic Agric.Systems},
             url = {https://orgprints.org/id/eprint/39654/},
        keywords = {organic dairy farming, mother-bonded calf rearing, extended suckling systems, cow-calf contact, farmer practices, farmer experiences},
        abstract = {A common practice in dairy farming is to remove the calf
from its mother a few hours after birth. The public debate
on the subject has increased, and views on whether the calf
should be allowed to stay with its dam for weeks are debated
among citizens, farmers, and advisors. The aim of this article is to present, analyse, and discuss experiences and arguments on dam-rearing of calves through interviews with actors, primarily farmers, involved in organic dairy farming in four European countries. The interviews showed that dam-rearing is practiced in a wealth of different systems, and four main points of view should be considered: that of the calf, the cow, the farmer, and the farming system. Three important qualities of cow calf contact systems are described from the animals? perspective: 1) nutrition, 2) care, and 3) learning. The discussion included ethical considerations referring to the principle of fairness as expressed by the International Federation of Organic Agriculture Movements (IFOAM). Wellbalanced and managed dam-rearing systems are suggested to contribute significantly to the physiological development and natural behaviour of mother cows and calves. The calves obtain capacities and skills through learning from the dam and others in the system. Major efforts are required when organising suitable calf- and cow-friendly dam-rearing systems, and farmer observations must be more careful because they take place in a group and therefore need to account for complex situations. In doing this, the farmer shows animals
respect, and treats them justly as part of the ethical alliance between animals and humans cohabiting on a farm. Farmers? trust in the capabilities of the animals ? such as the cow?s ability to look after the calf and the calf?s capability to live in a complex dairy system ? seems to partly break with some of the animal husbandry qualities that are often considered important when taking care of cows and calves in a system with early separation. ?Being in control? in new ways than previously was identified as a key for human learning in these systems as a part of the shifting focus when observing animals and spending time with cows and calves differently. In a cow calf contact system, the humans need relies to a higher degree on being able to observe and judge a complex situation than, for example, on giving the calves exactly the same amount of milk of a specific temperature at the same times every day.}
}

@misc{orgprints13755,
            year = {2000},
          author = {Mette Vaarst and Erik Steen Kristensen},
         journal = {Sundhed, velf{\ae}rd og medicinanvendelse ved oml{\ae}gning til {\o}kologisk m{\ae}lkeproduktion},
          series = {F{\O}JO-rapport},
