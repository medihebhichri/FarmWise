        keywords = {Insetos-praga, Agricultura org{\^a}nica, Solanum tuberosum, Aduba{\c c}{\~a}o silicatada agricultura familiar.
Insect pests, Organic agriculture, Solanum tuberosum, Silicate fertilization.},
        abstract = {O controle de insetos-praga exclusivamente com inseticidas sint{\'e}ticos ocasiona diversos efeitos colaterais. A indu{\c c}{\~a}o de resist{\^e}ncia de plantas {\'e} uma estrat{\'e}gia em potencial para prevenir danos de insetos na cultura. Os objetivos deste trabalho foram avaliar os efeitos de fontes de sil{\'i}cio na indu{\c c}{\~a}o de resist{\^e}ncia aos principais insetos-praga, no desenvolvimento e produtividade da cultura de batata inglesa em cultivo org{\^a}nico. As plantas foram submetidas aos seguintes tratamentos: 1- aduba{\c c}{\~a}o via solo com agrosil{\'i}cio na dosagem correspondente a 8 t/ha; 2- aduba{\c c}{\~a}o foliar com solu{\c c}{\~a}o de {\'a}cido sil{\'i}cico a 0,8\%; 3- aduba{\c c}{\~a}o foliar com solu{\c c}{\~a}o de cinza de eucalipto a 5\% e 4- testemunha. As avalia{\c c}{\~o}es dos insetos-praga foram realizadas aos 20, 40 e 60 dias ap{\'o}s a emerg{\^e}ncia das plantas, contando-se o n{\'u}mero de pulg{\~o}es, inj{\'u}rias foliares provocadas por Diabrotica spp. e fol{\'i}olos minados por Liriomyza spp. Foi determinado a altura, o di{\^a}metro e a produtividade, bem como os teores de tanino e lignina das plantas. O {\'a}cido sil{\'i}cico atuou como indutor da s{\'i}ntese de tanino na planta e a cinza e o agrosil{\'i}cio agiram como promotores do desenvolvimento vegetativo das plantas.}
}

@misc{orgprints24474,
          author = {Victor Maur{\'i}cio da Silva and Paulo Henrique Ribeiro and Alex Fabian Rabelo Teixeira and Jacimar Luis de Souza},
