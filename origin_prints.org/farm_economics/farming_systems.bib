@misc{orgprints45730,
       publisher = {Norsk senter for {\o}kologisk landbruk},
           title = {Jord, avling og n{\ae}ringsbalanse gjennom 30 {\r a}r med {\o}kologisk drift. Langtidsstudier p{\r a} Tingvoll gard 1991-2021},
         address = {Tingvoll},
          series = {NORS{\O}K Rapport},
            year = {2022},
          author = {Maria B{\r a}tnes and Martha Ebbesvik and Tatiana Rittl},
           pages = {1--46},
           month = {December},
        keywords = {{\O}kologisk drift, jordanalyser, engavling, belgvekstandel, n{\ae}ringsbalanser},
        abstract = {Omlegging til {\o}kologisk drift p{\r a} Tingvoll gard startet i 1987. Arealene var ferdig omlagt i 1992, husdyrholdet i 1994. Rapporten belyser de langsiktige effektene {\o}kologisk drift har hatt p{\r a} jordegenskaper, engavlinger og n{\ae}ringsbalanser fra 1991 til 2021. 
Det har blitt tatt ut jordpr{\o}ver for analyse seks ganger med fem til syv {\r a}rs mellomrom i l{\o}pet av 30-{\r a}rsperioden. Engavlinger har blitt registrert p{\r a} en del av de fulldyrka {\o}kologiske skiftene siden 1991. Samtidig med avlingsregistreringene har pr{\o}ver blitt tatt og analysert for n{\ae}ringsinnhold. Fra 2008 ble det ogs{\r a} tatt ut pr{\o}ver for {\r a} bestemme andelen gras, belgvekster og ugras i forbindelse med avlingsregistreringene. F{\o}r Tingvoll gard fikk klimastasjon i 1995, ble nedb{\o}r og temperatur hentet fra en klimastasjon som l{\r a} 15 km lenger inn i landet. {\r A}rlige n{\ae}ringsbalanser for nitrogen, fosfor og kalium har blitt beregnet p{\r a} bakgrunn av mengder innkj{\o}pte og solgte produkter i gardsregnskapene. 
Innholdet av mold og plantetilgjengelig fosfor (P-AL), kalsium og magnesium i matjordlaget for garden sett under ett er redusert i l{\o}pet av perioden 1991-2021. Innholdet av fosfor (P-AL) kategoriseres likevel som ?godt? i fulldyrka jord i 2021. P{\r a} de permanente beitene var P-AL-innholdet karakterisert som ?middels/optimalt?, som er {\o}nskelig niv{\r a}. Plantetilgjengelig kalium har {\o}kt litt i perioden. Kaliumreservene har vist en nedgang, men er likevel store. N{\ae}rings- og moldinnholdet i undergrunnsjorda, 20-40 cm dybde, p{\r a} fulldyrka arealer har v{\ae}rt stabilt over tid. 
Gjennomsnittstemperaturen i vekstsesongen har {\o}kt med ca. 0,05 o C {\r a}rlig i l{\o}pet av 30-{\r a}rs perioden. Dette har f{\o}rt til tidligere h{\o}sting av enga og ofte tre sl{\r a}tter istedenfor to. Engavlingene fra f{\o}rste sl{\r a}tt har blitt litt redusert fra 1991 til 2021, men innholdet av r{\r a}protein, PBV og AAT har hatt en svak {\o}kning som tyder p{\r a} tidligere h{\o}sting. Andelen belgvekster har variert fra 15 til 50 \% i {\r a}rene fra 2008 til 2021, og har v{\ae}rt h{\o}yere i andresl{\r a}tten enn i f{\o}rstesl{\r a}tten. Denne andelen har holdt seg bra helt til femte eng{\r a}r. 
N{\ae}ringsbalansen viser at det har v{\ae}rt litt h{\o}yere overskudd av nitrogen, fosfor og kalium de siste {\r a}rene. {\r A}rsaken er st{\o}rre mengde innkj{\o}pt kraftf{\^o}r og noe grovf{\^o}r etter 2010 enn f{\o}r. Dette kan f{\o}re til h{\o}yere niv{\r a} av disse n{\ae}ringsstoffene i jorda i {\r a}rene som kommer.},
             url = {https://orgprints.org/id/eprint/45730/}
}

@misc{orgprints54165,
             doi = {10.25518/1780-4507.19928},
          volume = {26},
           pages = {261--274},
           title = {Decision support tools for grass-based fodder management on Walloon dairy farms: current adoption and perspectives},
         journal = {Biotechnol. Agron. Soc. Environ.},
            year = {2022},
          author = {Caroline Battheu-Noirfalise and Eric Froidmont and Micha{\"e}l Mathot and Didier Stilmant},
        abstract = {Description of the subject. Many decision support tools (DSTs) have been developed to help dairy farmers optimally manage the high variability in the quality and availability of grass-based fodder, but their adoption rate remains low.

Objectives. The objective was to characterize and understand the adoption rate of DSTs related to using grass-based fodder.

Methodology. A sample of 61 Walloon (Belgium) dairy farmers responded to an online survey concerning their current use of 23 DSTs related to using grass-based fodder either directly (pasture or grassland) or indirectly (feeding or techno-economic), as well as barriers to and incentives for adopting them, their current interest in DSTs, and satisfaction with the guidance on using these DSTs.

Results. Pasture management DSTs were used the least, even though farmers were the most interested in them. Farmers used simple indicators rather than software or automated tools. Farmers indicated that DSTs were too expensive and time consuming, even if they could ultimately save them time and money. Continuing education is lacking. Four types of users were identified who influence the use of DSTs: high user no grazing (H-NG), high user traditional or technical grazing (H-T/TG), low user traditional grazing (L-TG), and moderate user organic (M-ORG).

Conclusions. Communicating with end-users during each step of DST development would help (1) identify the specific needs of a diverse set of dairy farmers and (2) develop DSTs that better correspond to their practices. More long-term guidance is required to inform farmers about existing DSTs and to transfer the knowledge required to use them.},
        keywords = {dairy farming, grassland management, farm surveys, grazing, precision agriculture},
             url = {https://orgprints.org/id/eprint/54165/}
}

@misc{orgprints28504,
