@misc{orgprints22932,
           title = {A intera{\c c}{\~a}o humano-animal e o uso de homeopatia no manejo sanit{\'a}rio de rebanhos leiteiros em pequenas propriedades no Sul do Brasil.},
         journal = {Revista Brasileira de Agroecologia},
          author = {Luciana Aparecida  Honorato and Maria Jos{\'e}  H{\"o}tzel},
            year = {2012},
          number = {2},
           pages = {77--86},
          volume = {7},
             url = {https://orgprints.org/id/eprint/22932/},
        keywords = {Atitudes, Comportamento, Vacas, Homeopatia, Agricultura familiar, Sul do Brasil.
Attitudes, Behaviour, Cows, Homeopathy, Family farm, Southern of Brazil.},
        abstract = {O comportamento de vacas leiteiras pode ser influenciado pela natureza do tratamento que recebem dos humanos. Neste trabalho foram comparados 20 estabelecimentos de agricultores familiares que usavam terapia convencional (n=11) ou homeop{\'a}tica (n=9) como principal terap{\^e}utica em rebanhos leiteiros. Foram feitas entrevistas com os agricultores, medidas de comportamento humano (como nomear e falar com os animais, contatos positivos ou negativos, condu{\c c}{\~a}o para a sala de ordenha), e de comportamento animal. Foram encontrados escores de comportamento humano similares entre os dois grupos. A m{\'e}dia de dist{\^a}ncia de fuga foi menor nos rebanhos sob uso de homeopatia do que no convencional (P=0,04). Nos 9 estabelecimentos homeop{\'a}ticos o medicamento era colocado na ra{\c c}{\~a}o, reduzindo a freq{\"u}{\^e}ncia de manejos aversivos, o que pode ter influenciado o comportamento dos animais. A semelhan{\c c}a entre os dois grupos sugere que h{\'a} um padr{\~a}o de rotina de manejo, onde predominam os comportamentos neutros e positivos dos manejadores, o que pode refletir em atitudes geralmente positivas em rela{\c c}{\~a}o aos animais. As diferen{\c c}as no comportamento dos animais observadas neste estudo podem, portanto, ser explicadas principalmente pela forma de administra{\c c}{\~a}o dos medicamentos, que possivelmente reduzem de uma forma geral a ocorr{\^e}ncia de intera{\c c}{\~o}es negativas entre manejadores e animais.}
}

@misc{orgprints27405,
          number = {2},
           pages = {60--69},
          volume = {9},
           month = {April},
           title = {Produ{\c c}{\~a}o de leite org{\^a}nico e convencional no Oeste de Santa Catarina: caracteriza{\c c}{\~a}o e percep{\c c}{\~a}o dos produtores.},
         journal = {Revista Brasileira de Agroecologia},
          author = {Luciana Aparecida  Honorato and Isabella Dias Barbosa Silveira and Luiz Carlos Pinheiro Machado Filho},
            year = {2014},
        abstract = {{\'E} crescente o n{\'u}mero de produtores interessados no sistema de produ{\c c}{\~a}o org{\^a}nico, especialmente dentro da Agricultura Familiar. Por{\'e}m, pouco se sabe sobre a estrutura{\c c}{\~a}o das unidades produtivas, tecnologias e pr{\'a}ticas que est{\~a}o sendo utilizadas nesse sistema. O objetivo desse trabalho foi identificar o perfil de produtores de leite org{\^a}nico atrav{\'e}s da caracteriza{\c c}{\~a}o das fam{\'i}lias e unidades produtivas, investigar as motiva{\c c}{\~o}es, as principais dificuldades e aspectos positivos e negativos da produ{\c c}{\~a}o org{\^a}nica, al{\'e}m de comparar caracter{\'i}sticas demogr{\'a}ficas, produtivas e manejos utilizados nos sistemas org{\^a}nico e convencional. Foram selecionadas 17 unidades leiteiras org{\^a}nicas (ORG) e 17 unidades leiteiras convencionais (CONV), de agricultores familiares, no Oeste de Santa Catarina, Brasil. Foram realizadas entrevistas com os agricultores em mar{\c c}o e setembro de 2010 sobre suas opini{\~o}es a respeito da produ{\c c}{\~a}o org{\^a}nica, e sobre as diferen{\c c}as entre as tecnologias e manejos adotados. O tamanho da propriedade, idade, escolaridade e tempo na atividade foram similares entre os sistemas. O ORG teve rebanhos menores e menor produ{\c c}{\~a}o por vaca, o que foi relacionado {\`a} menor porcentagem de sangue holand{\^e}s no rebanho e n{\~a}o ao sistema em si. Esse sistema apresentou uma produ{\c c}{\~a}o leiteira menor do que o CONV com m{\'e}dias de 3342 e 7912 litros/m{\^e}s, respectivamente. O ORG teve um perfil de pessoas mais ativas no seu meio social, tendo maior acesso a informa{\c c}{\~o}es, o que pode ter influenciado na op{\c c}{\~a}o pelo sistema de produ{\c c}{\~a}o. A melhor explora{\c c}{\~a}o dos recursos forrageiros e menor utiliza{\c c}{\~a}o de antibi{\'o}ticos fazem com que os produtores org{\^a}nicos percebam melhorias no agroecossistema e na qualidade de vida. O maior entrave percebido pelos produtores org{\^a}nicos {\'e} a falta de reconhecimento econ{\^o}mico pelo mercado e de assist{\^e}ncia t{\'e}cnica especializada no assunto. A adequa{\c c}{\~a}o dos produtores aos padr{\~o}es normativos {\'e} limitada pela falta de planejamento alimentar e de sa{\'u}de animal.},
        keywords = {Leite org{\^a}nico, Agricultura familiar, Processo de transi{\c c}{\~a}o.
Organic dairy, Family farms, Conversion process.},
             url = {https://orgprints.org/id/eprint/27405/}
}

