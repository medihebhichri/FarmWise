@misc{orgprints43303,
            year = {2021},
           month = {June},
           title = {XXIX SEAE Conference: "One health. A planet. A diverse rural world?},
          editor = {SEAE Spanish Society of Organic Farming and Agroecology},
        abstract = {The Spanish Society of Organic Agriculture and Agroecology (SEAE) have organized a Technical Conference (JT), under the slogan ?One health. A planet. A diverse rural world ?: because people's health is inseparable from the health of our planet. In this symbiosis, the rural world plays a fundamental role as a guarantor of our food and of many other services that sustain life and preserve the health of our ecosystems. And if something balances the three aspects - health, planet and rural world - it is ecological production and agroecology which, in times of pandemic and climate emergency, are configured as the best strategy to demand a comprehensive response to this socio-economic crisis. ecological current, addressing all those links between agriculture and health.
Thus, under the gaze of experts and initiatives in the agroecological field that range from local to international, it is expected to explore, from a transdisciplinary and multisectoral approach, different issues involved in this single health concept: the role of organic production and short channels, human and animal health as interdependent and linked to ecosystems or public policies that should favor agroecological strategies for the transition of our food systems.},
             url = {https://orgprints.org/id/eprint/43303/}
}

@misc{orgprints41851,
       publisher = {SEAE},
           title = {Proceedings of XXVIII SEAE Conference:"Health of agroecosystems and human well-being: Indicators of organic production"},
          editor = {SEAE Spanish Society of Organic Farming and Agroecology},
           month = {May},
            year = {2021},
             url = {https://orgprints.org/id/eprint/41851/},
        keywords = {producci{\'o}n ecol{\'o}gica, agroecolog{\'i}a, desarrollo rural, sistemas agroalimentarios, salud del agroecosistema, salud humana, indicadores},
        abstract = {The situation of the environmental collapse and emergency (climatic, economic and health), to that our life and economic model has led us, requires urgent and effective solutions.
So that we need tools to evaluate and measure the impact of our actions and the solutions that we propose, as well as for the projection of these impacts in the medium and long term.
The uncontrolled release of toxins, the loss of biodiversity, the shortage of energy and materials, the emission of greenhouse gases, etc., derived from human activity has caused an exponential increase in temperatures at all levels, the proliferation of meteorological effects increasingly intense and frequent adverse events, loss of soil and genetic resources, with very serious consequences on health and social justice. Each of these consequences turn into causes that lead to other effects, generating feedback chains.
If we do not address the bases of the problems as they are and at the same time we are able to analyze them and understand them as a whole, we will not be able to act with the effectiveness, rigor and efficiency that this
situation requires.
For the first time, international institutions are requiring indicators of impact and are conditioning the aids, such as the Common Agricultural Policy, to the achievement of environmental, social and economic objectives.
SEAE wants to contribute to the debate and the generation of indicators that show and assess the impact of our food production activity on our
soil, climate, biodiversity, water and human well-being. It is a challenge for researchers,administrations, productive sector and society in general.}
}

