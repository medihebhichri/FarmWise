@misc{orgprints361,
          volume = {209},
           pages = {8},
          author = {J{\o}rgen E. Olesen},
            year = {2000},
         journal = {{\O}kologisk Jordbrug},
           title = {International forskning i {\o}kologiske s{\ae}dskifter},
       publisher = {{\O}kologisk Landsforening},
             url = {https://orgprints.org/id/eprint/361/},
        abstract = {I juni m{\r a}ned sidste {\r a}r samledes fyrre forskere fra Europa og USA p{\r a} Landbrugsskolen i Borris i tre dage. M{\o}det var arrangeret af Forskningscenter for {\O}kologisk Jordbrug (F{\O}JO) og havde til form{\r a}l at diskutere status og fremtidig retning for forskning i {\o}kologiske s{\ae}dskifter.

Form{\r a}let med forskning i s{\ae}dskifter er at {\o}ge forst{\r a}elsen af {\r a}rsagerne til de fordele, som gode s{\ae}dskifter indeb{\ae}rer. Dette sker med henblik p{\r a} at forbedre r{\r a}dgivningen vedr{\o}rende den praktiske h{\r a}ndtering af s{\ae}dskifter, afgr{\o}der og jord. Problemstillingerne unders{\o}ges med en bred vifte af metoder, som bl.a. omfatter langvarige s{\ae}dskiftefors{\o}g, g{\r a}rdstudier og matematiske modeller.

Der er en r{\ae}kke igangv{\ae}rende langvarige fors{\o}g i Europa med forskellige s{\ae}dskifter under {\o}kologisk dyrkning. P{\r a} m{\o}det blev resultater fra 10 s{\r a}danne fors{\o}g pr{\ae}senteret. Nogle af fors{\o}gene omfatter ogs{\r a} sammenligninger med konventionel dyrkning, flere g{\o}dningsniveauer eller forskellig h{\r a}ndtering af efterafgr{\o}der.

B{\r a}de klima og jordtype varierer betydeligt i Europa. Dette afspejler sig i de s{\ae}dskifter, der indg{\r a}r i fors{\o}gene. Under de k{\o}lige og forholdsvis fugtige klimaforhold i Norge og Skotland domineres s{\ae}dskifterne af kl{\o}vergr{\ae}s, foder- og v{\r a}rs{\ae}dsafgr{\o}der. Klimaet i de baltiske lande er mere kontinentalt og derfor koldere om vinteren og varmere om sommeren, og generelt lidt t{\o}rre. Disse klimaforhold muligg{\o}r en r{\ae}kke forskellige afgr{\o}der, men den t{\o}rre sommer og tidlig kulde om efter{\r a}ret vanskeligg{\o}r etablering af efterafgr{\o}der. Central- og {\O}steuropa er ogs{\r a} pr{\ae}get af det kontinentale klima, som dog generelt er varmere. Det muligg{\o}r dyrkning af majs til modenhed, hvilket indg{\r a}r i mange af s{\ae}dskifterne. I Middelhavslandene er sommert{\o}rke ofte et stort problem. Her indg{\r a}r ofte majs, solsikke, durum-hvede og {\ae}rter i s{\ae}dskifterne. P{\r a} grund af den tidlige h{\o}st er der her store muligheder for at udnytte efter{\r a}rs- og vinters{\ae}sonen til dyrkning af kv{\ae}lstoffikserende efterafgr{\o}der.

I den {\o}vrige del af Vesteuropa, som omfatter bl.a. Danmark, England og Tyskland, er der en stor variation i de afgr{\o}der, der indg{\r a}r i s{\ae}dskifterne. Her er de klimatiske begr{\ae}sninger ikke helt s{\r a} afg{\o}rende som i visse af de {\o}vrige regioner. Til geng{\ae}ld kan forskelle i jordtypen spille en stor rolle.

M{\o}det viste, at der p{\r a} trods af de store regionale forskelle, ogs{\r a} er en r{\ae}kke f{\ae}lles problemstillinger, der vedr{\o}rer s{\ae}dskiftet. Det drejer sig is{\ae}r om s{\ae}dskiftets virkning p{\r a} jordens frugtbarhed, forsyning af afgr{\o}derne med n{\ae}ringsstoffer og betydning for forekomst af ukrudt, sygdomme og skadedyr. Der er et stort behov for en bedre koordinering af de igangv{\ae}rende aktiviteter i Europa. Dette kan EU's forskningsprogrammer forh{\r a}bentlig v{\ae}re medvirkende til at sikre.

Alle indl{\ae}ggene og konklusionerne fra m{\o}det er samlet i en rapport, som kan k{\o}bes ved henvendelse til F{\O}JO. Rapporten er p{\r a} engelsk.
}
}

@misc{orgprints363,
           month = {August},
           pages = {8},
            year = {1999},
          author = {J{\o}rgen E. Olesen},
         journal = {{\O}kologisk jordbrug},
           title = {Synlige forskelle i s{\ae}dskiftefors{\o}get},
             url = {https://orgprints.org/id/eprint/363/},
        abstract = {S{\r a} kom nedb{\o}ren. Efter det meget varme og t{\o}rre for{\r a}r er det helt rart med lidt almindeligt k{\o}ligt regnvejr. P{\r a} sandjorden p{\r a} Jyndevad er der allerede vandet tre gange. Det skyldes dog delvist at vi i {\r a}r har {\ae}ndret vandingspraksis. 
	I de tidligere {\r a}r har vi benytte g{\ae}ngs praksis med at vande op til fuld markkapacitet. Nyere forskningsresultater viser dog, at det p{\r a} sandjord er meget vanskeligt at fugte jorden op igen efter en t{\o}r periode. Vandet vil have en tendens til at str{\o}mme ned i nogle enkelte "fingre" i jorden. Her kan der samtidigt ske udvaskning af is{\ae}r kv{\ae}lstof.
	Vi har derfor i {\r a}r valgt kun at vande med ca. 15 mm ved et vandunderskud p{\r a} 30 mm. Erfaringer fra andre fors{\o}g p{\r a} Jyndevad viser, at det totalt set vil kunne spare vand, uden at det g{\r a}r ud over udbyttet. Det er vores h{\r a}b, at det ogs{\r a} vil kunne medvirke til at hindre nitratudvasking i v{\ae}ksts{\ae}sonen. Denne nitratudvaskning er en af {\r a}rsagerne til at udnyttelsen af kv{\ae}lstof i husdyrg{\o}dningen her er betydeligt lavere end p{\r a} de {\o}vrige fors{\o}gssteder.

Ukrudtsbek{\ae}mpelse

Regnvejret har drillet lidt p{\r a} lerjorden p{\r a} Holeby. Her er roerne ved udgangen af maj endnu ikke radrenset. Jorden er simpelt hen for v{\r a}d. S{\r a} roerne er heller ikke blevet tyndet ud endnu. Vi har uds{\r a}et dobbelt plantetal, s{\r a} vi kan t{\r a}le at miste nogle planter, f.eks. ved skadedyrsangreb.
	P{\r a} Foulum er der meget ukrudt i v{\r a}rs{\ae}d med udl{\ae}g. Her er der ikke foretaget mekanisk ukrudtsbek{\ae}mpelse, fordi vi valgte at s{\r a} udl{\ae}gget samtidigt med kornet. Sidste {\r a}r blev udl{\ae}gget p{\r a} grund af vejret f{\o}rst s{\r a}et noget senere. Det gav en meget svag etablering af is{\ae}r kl{\o}ver, som blev udsat for h{\r a}rd konkurrence fra b{\r a}de ukrudt og afgr{\o}de. Udl{\ae}gget er i {\r a}r etableret betydeligt bedre. Der vil dog stadig v{\ae}re en del konkurrence fra ukrudtet.
	P{\r a} Jyndevad er udl{\ae}gget f{\o}rst s{\r a}et den 2. maj efter en blindstrigling og to efterf{\o}lgende ukrudtsharvninger. Her er udl{\ae}gget dog ogs{\r a} spiret godt frem, og afgr{\o}derne p{\r a} Jyndevad er generelt ikke helt s{\r a} konkurrencest{\ae}rke som p{\r a} Foulum, s{\r a} her f{\r a}r udl{\ae}gget alligevel en chance. Ogs{\r a} p{\r a} Flakkebjerg er udl{\ae}gget etableret godt og har taget ordentligt fat efter, at der er kommet regn.
	P{\r a} Jyndevad dyrkes vinterhvede og v{\r a}rhvede p{\r a} dobbelt r{\ae}kkeafstand for at g{\o}re det muligt at radrense. Det er ogs{\r a} lykkedes godt nok i vinterhveden, men i v{\r a}rhveden st{\r a}r der stadig en del hvidmelet g{\r a}sefod i kornr{\ae}kkerne. Hvor der er givet g{\o}dning ser det ud til at dette ukrudt vil have gode muligheder for at overvokse kornet. Husdyrg{\o}dningen er udbragt i s{\r a}bedet inden s{\r a}ningen. Det har desv{\ae}rre den effekt, at det g{\o}der ukrudtet mindst lige s{\r a} meget som afgr{\o}den.

Virkning af efterafgr{\o}der

Sidste {\r a}r var der alle steder en god virkning af efterafgr{\o}der p{\r a} udbytterne i v{\r a}rbyg. Merudbyttet for efterafgr{\o}der varierede fra 9 til 11 hkg/ha. P{\r a} sandjorden i Jyndevad ser der igen i {\r a}r ud til at blive en god virkning af efterafgr{\o}den. Derimod synes der at v{\ae}re en mindre effekt p{\r a} Foulum end sidste {\r a}r. Det kan h{\ae}nge sammen med, at gr{\ae}sset sidste {\r a}r blev fr{\ae}set inden pl{\o}jningen. I {\r a}r har vi sparet denne behandling og pl{\o}jet gr{\ae}sset ned direkte.
	Fr{\ae}sningen kan have haft en positiv effekt p{\r a} mineraliseringen af kv{\ae}lstof i gr{\ae}sset. Dermed kan tidspunktet for kv{\ae}lstoffets tilg{\ae}ngelighed have passet bedre til afgr{\o}dens behov. Fors{\o}g p{\r a} Foulum har vist, at fr{\ae}sning af afgr{\ae}sningsmarker forud for pl{\o}jning om for{\r a}ret kan {\o}ge udbytte og kv{\ae}lstofoptagelse i v{\r a}rbyg. Derimod har fors{\o}g med tilsvarende behandlinger p{\r a} Jyndevad ikke givet forskel. Her er m{\r a}ske igen et sted, hvor jordtypen spiller en rolle.
}
}

@unpublished{orgprints3857,
          author = {J{\o}rgen E. Olesen and Margrethe Askegaard},
            year = {2003},
           title = {Nitrate leaching from arable crop rotations in organic farming},
        abstract = {The crop rotation is a crucial and integral part of organic farming systems. It must be designed to maintain and exploit soil fertility, and to minimise the impacts on environment, e.g. through nitrate leaching. Crop rotations in organic farming typically include legume crops to provide nitrogen fixation and improve soil fertility. Catch crops are also commonly used, if demands for soil tillage to control perennial weeds allow. This paper presents results on nitrate leaching from a field experiment with comparison of three different crop rotations with and without catch crops designed for organic farming.},
             url = {https://orgprints.org/id/eprint/3857/}
}

@inproceedings{orgprints3859,
          editor = {D.J. Hatch and D.R. Chadwick and S.C. Jarvis and J.A. Roker},
           pages = {389--396},
           title = {Nitrate leaching from arable crop rotations in organic farming},
       publisher = {Wageningen Academic Publishers},
         journal = {Controlling nitrogen flows and losses},
            year = {2004},
          author = {J{\o}rgen E. Olesen and Margrethe Askegaard and J{\o}rgen Berntsen},
        abstract = {Nitrate leaching from crop rotations for organic grain production were investigated in a field experiment on different soil types in Denmark from 1997 to 2002. Three experimental factors were included in the experiment in a factorial design: 1) proportion of grass-clover and pulses in the rotation, 2) cover crop (with and without), and 3) manure (with and without). Two four-course rotations were compared. They had one year of grass-clover as a green manure crop, either followed by spring wheat or by winter wheat. The nitrate leaching was measured using ceramic suction cells. The nitrate leaching did not differ between the rotations, as a change in leaching following the grass-clover was compensated by a reverse effect in the grain crops. Use of cover crops reduced N leaching by 23 to 38\% at crop rotation level with the highest reduction on the coarse sandy soil. Simulation of N leaching using the FASSET model showed that a practice of using part of the summer period in the grass-clover as a bare fallow to control couch grass could increase leaching substantially, in particular on the sandy soil.},
             url = {https://orgprints.org/id/eprint/3859/}
}

