            year = {2010},
          author = {Marney Aparecida de Oliveira Paulino and Fl{\'a}vio Pimenta de Figueiredo and Rodrigo Carvalho Fernandes and Fl{\'a}via Silva  Barbosa},
           title = {Caracter{\'i}stica da produ{\c c}{\~a}o agr{\'i}cola em comunidades rurais da regi{\~a}o do Alto Rio Pacu{\'i}.},
          volume = {5},
           pages = {165--173},
          number = {2},
        abstract = {As condi{\c c}{\~o}es edafoclim{\'a}ticas do Norte de Minas Gerais, em quase todos os meses do ano s{\~a}o desfavor{\'a}veis {\`a} produ{\c c}{\~a}o agr{\'i}cola. Muitas vezes, para a realiza{\c c}{\~a}o dessa atividade {\'e} necess{\'a}rio {\`a} utiliza{\c c}{\~a}o de t{\'e}cnicas de produ{\c c}{\~a}o que quando utilizadas inadequadamente tornam-se desfavor{\'a}veis ao meio ambiente. Por esse motivo, esse trabalho teve como objetivo averiguar as condi{\c c}{\~o}es de produ{\c c}{\~a}o de quatro comunidades rurais da regi{\~a}o do Alto Rio Pacu{\'i}, localizada no Norte de Minas Gerais. Para essa avalia{\c c}{\~a}o, utilizaram-se de question{\'a}rios pr{\'e}determinados, os quais apresentavam quest{\~o}es sobre a utiliza{\c c}{\~a}o do solo, a {\'a}rea de produ{\c c}{\~a}o, o tipo de cultura, os sistemas de irriga{\c c}{\~a}o, a fonte de abastecimento de {\'a}gua, entre outras. Doze propriedades rurais pertencentes a quatro comunidades, foram visitadas. Conclui-se nessa pesquisa que a atividade econ{\^o}mica que se destaca na regi{\~a}o {\'e} o cultivo de oler{\'i}colas, as quais, irrigadas por sistemas de irriga{\c c}{\~a}o diversos, ocasionando, segundo os produtores rurais, redu{\c c}{\~a}o do fluxo de {\'a}gua do rio Pacu{\'i} e dos seus afluentes.},
        keywords = {Produ{\c c}{\~a}o Agr{\'i}cola, Meio Ambiente, Levantamento.
Agricultural Production, Environment, Survey.},
             url = {https://orgprints.org/id/eprint/24547/}
}

@misc{orgprints26040,
