         journal = {Ambiente \& Sociedade},
           title = {A agroecologia como perspectiva de sustentabilidade na agricultura familiar.},
        keywords = {Agroecologia, Sustentabilidade, Espa{\c c}o rural.
Agroecology, Sustainability, Espa{\c c}o rural.},
        abstract = {O presente trabalho avalia aspectos de ordem social, econ{\^o}mica e ambiental advindos das pr{\'a}ticas dos agricultores familiares da APROFAM (Associa{\c c}{\~a}o dos Produtores e Produtoras Agroecol{\'o}gicas de Mossor{\'o}), que comercializam semanalmente sua produ{\c c}{\~a}o na Feira Agroecol{\'o}gica, de modo a analisar como proporcionam o desenvolvimento sustent{\'a}vel da agricultura de base familiar. Para tanto, foram realizadas entrevistas dialogadas com seis desses produtores. Os dados foram obtidos atrav{\'e}s de visita in loco, registro fotogr{\'a}fico e levantamento de dados qualitativos e quantitativos no per{\'i}odo de janeiro a mar{\c c}o de 2012. A cria{\c c}{\~a}o da Feira possibilitou o acesso ao mercado local, organiza{\c c}{\~a}o por parte dos produtores para adquirir a certifica{\c c}{\~a}o participativa, produ{\c c}{\~a}o mais diversificada, aumento na renda familiar, colaborando com a perman{\^e}ncia das fam{\'i}lias no campo. Observou-se, ainda, que existem desafios a serem superados, pois 75\% dos entrevistados n{\~a}o conseguem contabilizar seus custos de produ{\c c}{\~a}o, nem mensurar as quantidades comercializadas. Ainda, existe insufici{\^e}ncia de assessoria t{\'e}cnica, disponibilidade h{\'i}drica, entre outros, que impossibilitam o fortalecimento dessa forma de organiza{\c c}{\~a}o social.},
             url = {https://orgprints.org/id/eprint/28545/}
}

@misc{orgprints22979,
          number = {1},
           pages = {139--144},
          volume = {7},
           title = {Extrato aquoso de alho (Allium sativum) sobre nemat{\'o}ides gastrintestinais de ovinos.},
         journal = {Revista Brasileira de Agroecologia},
            year = {2012},
          author = {Fernanda Carlini Cunha Santos and Silvia Gionzalez  Monteiro and Fernanda Silveira Flores  Vogel},
        keywords = {Endoparasitas, Fitoterapia, Plantas medicinais, Terapias alternativas.
Alternative therapies, Endoparasites, Phytoterapic, Medicinal plants.},
        abstract = {O parasitismo gastrintestinal em ovinos acarreta em in{\'u}meros preju{\'i}zos aos produtores rurais, sendo que seu controle {\'e} realizado basicamente atrav{\'e}s da administra{\c c}{\~a}o de anti-helm{\'i}nticos sint{\'e}ticos que estimula a sele{\c c}{\~a}o de parasitas resistentes. Com o objetivo de determinar a efic{\'a}cia do extrato aquoso de alho sobre parasitas gastrintestinais de ovinos in vivo, 21 animais foram divididos em 3 tratamentos. Os animais do Tratamento I e II receberam, respectivamente, 60g e 90g de alho na forma de extrato aquoso em dose {\'u}nica, e o terceiro grupo foi considerado controle n{\~a}o tratado. Amostras fecais foram coletadas nos dias 0, 3, 8, 16 e 24 para realiza{\c c}{\~a}o dos exames de contagem de ovos por grama de fezes (OPG) e coprocultura. N{\~a}o foi observada redu{\c c}{\~a}o do OPG assim como nenhuma altera{\c c}{\~a}o dos g{\^e}neros larvais identificados na coprocultura. Diante disso, conclui-se que o extrato aquoso de alho n{\~a}o apresentou efeito anti-helm{\'i}ntico in vivo nas condi{\c c}{\~o}es deste experimento.},
             url = {https://orgprints.org/id/eprint/22979/}
}

@misc{orgprints23002,
           pages = {176--181},
          volume = {6},
          number = {3},
         journal = {Revista Brasileira de Agroecologia},
            year = {2011},
          author = {Fernanda Carlini Cunha  Santos and Fernanda Silveira Flores  Vogel and Silvia Gonzales  Monteiro},
           title = {Efeito do suco de alho (Allium sativum L.) sobre endoparasitas gastrintestinais de ovinos.},
             url = {https://orgprints.org/id/eprint/23002/},
        abstract = {Os parasitas gastrintestinais de ovinos causam enormes preju{\'i}zos a pecu{\'a}ria, tendo como principais sinais cl{\'i}nicos: redu{\c c}{\~a}o na produ{\c c}{\~a}o de carne, leite e l{\~a}, anemia, diarr{\'e}ia, anorexia e morte s{\'u}bita. O tratamento preconizado {\'e} basicamente atrav{\'e}s da administra{\c c}{\~a}o de produtos qu{\'i}micos, sendo que estes estimulam o aparecimento da resist{\^e}ncia parasit{\'a}ria. Nesse contexto, a fitoterapia {\'e} uma modalidade de tratamento medicinal que vem crescendo muito nestes {\'u}ltimos anos. Com o intuito de avaliar a efic{\'a}cia do suco de alho (Allium sativum) sobre infec{\c c}{\~o}es helm{\'i}nticas foram utilizados 18 ovinos divididos em tr{\^e}s tratamentos. Nos grupos I e II, os animais receberam respectivamente, 15g e 30g de alho em forma de suco, uma vez ao dia durante tr{\^e}s dias consecutivos, e o grupo III foi considerado controle, sem tratamento. Amostras fecais foram coletadas nos dias 0, 1, 2, 7 e 13 para realiza{\c c}{\~a}o da contagem do n{\'u}mero de ovos por grama de fezes (OPG) e coprocultura. Durante todo per{\'i}odo experimental n{\~a}o houve redu{\c c}{\~a}o do n{\'u}mero m{\'e}dio de ovos nem altera{\c c}{\~a}o significativa dos g{\^e}neros dos parasitas encontrados nas coproculturas, assim como nenhum efeito colateral foi observado nos animais tratados com alho. O suco de alho n{\~a}o foi efetivo na redu{\c c}{\~a}o da carga parasit{\'a}ria dos ovinos in vivo.},
        keywords = {Fitoter{\'a}picos, Helmintos, Ovinocultura, Plantas medicinais.
Breeding sheep, Helminth, Medical plants, Phytotherapic.}
}

@misc{orgprints28544,
