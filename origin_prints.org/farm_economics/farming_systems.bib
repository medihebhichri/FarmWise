@misc{orgprints27505,
          author = {Susana Andrea Stupino and Jorge Luis Frangi and Santiago Javier Sarand{\'o}n and Marcelo Fabi{\'a}n Arturi and Ana Clara Ferreira},
            year = {2008},
         journal = {Revista Brasileira de Agroecologia},
           title = {Plant diversity in two horticultural farms under organic and conventional management in La Plata, Argentina: A case study.},
           month = {August},
           pages = {24--35},
          volume = {3},
          number = {3},
             url = {https://orgprints.org/id/eprint/27505/},
        abstract = {Cultivated and associated plant diversity, were compared (the last at 3 scales: alpha, beta and gamma), in 2 farms under conventional and organic management in La Plata. The relev{\'e} method was used for associated species recording in spring and summer. At farm level we assessed total spontaneous specific richness ({\ensuremath{\gamma}} diversity), genus and family richness. Richness ? species total per crop- was used as index of {\ensuremath{\alpha}} diversity. We also calculated the median richness per crop sample unit. A randomization method was used to assess the degree for which differences between the observed and expected median richness per crop in a management style are attributable to chance. {\ensuremath{\beta}} diversity values were compared to a random framework, separately for each farm. Results indicate that the organic farm had higher associated diversity at all levels; also, higher number of cultivated plots and species, and proportion of exclusive, perennials, native and utilitarian species. In the organic farm crops with higher or lower median richness than expected, and higher {\ensuremath{\beta}}, indicated higher spatial and seasonal heterogeneity. Conventional management limited spontaneous plants to a reduced, homogeneously distributed group. Other factors may influence associated plant diversity.},
        keywords = {Horticultura, Agrobiodiversidad, Diversidad asociada, Agroecosistemas.
Horticulture, Agrobiodiversity, Associated diversity, Agroecosystems.}
}

@unpublished{orgprints5401,
            year = {2005},
          author = {M. Styczen and R.N. Poulsen and J. Gudbjerg and B. Gjettermann},
           month = {August},
           title = {Simulering af DOC og DON i indsatsomr{\r a}de "S{\o}nders{\o}" p{\r a} Fyn: Endelig rapport for Arbejdspakke 6 i projektet "Regional Grundvandsbeskyttelse ved hj{\ae}lp af optimerede {\O}kologiske Dyrkningssystemer"},
             url = {https://orgprints.org/id/eprint/5401/},
        abstract = {Der er gennemf{\o}rt en beregning af udvaskning af nitrat, ammonium, DOC og DON for en konventionel og en {\o}kologisk situation i et indsatsomr{\r a}de ved ?S{\o}nders{\o}? p{\r a} Fyn. Beregningerne er udf{\o}rt med DAISY-modellen, indeholdende et nyudviklet DOC/DON-modul og MIKE SHE-modellen. 

Modelops{\ae}tningen, der d{\ae}kker et noget st{\o}rre omr{\r a}de end selve indsatsomr{\r a}det,  baserer sig p{\r a} den eksisterende arealanvendelse og landbrugssituation for en del af omr{\r a}det, og ekstrapoleret i den nordvestlige del. Ekstrapolationen d{\ae}kker kun en lille del af det faktiske indsatsomr{\r a}de. Modelops{\ae}tningerne bygger p{\r a} tidligere modelleringsstudier i samme omr{\r a}de.

Koncentrationerne af DOC og DON er stort set ens i de to scenarier p{\r a} grund af, at den styrende faktor er jordens organiske pulje. P{\r a} grund af st{\o}rre forekomst af gr{\ae}smarker i {\o}kologisk landbrug ville man forvente en opbygning af den organiske pulje, men DAISY-modellen synes at undervurdere denne faktor. Forskellen i DOC/DON-produktion mellem konventionelt og {\o}kologisk jordbrug forventes derfor ogs{\r a} at v{\ae}re undervurderet. For at beskrive DOC/DON-dynamikken i grundvandet, er den udvaskede m{\ae}ngde opdelt i forskellige puljer, med forskellige C/N-forhold og nedbrydningstid.

M{\ae}ngden af uorganisk nitrat udvasket fra rodzonen er v{\ae}sentligt lavere i indsatsomr{\r a}det efter oml{\ae}gning til {\o}kologisk brug, nemlig 29 kg N/ha i forhold til 70 kg N/ha i gennemsnit f{\o}r oml{\ae}gningen. 

Effekten af oml{\ae}gningen er noget forsinket. Nitratkoncentrationen til dr{\ae}n falder fra 13.2 kg nitrat-N/ha (2003-2012) i det konventionelle scenarie til 9.5 kg nitrat-N/ha i det {\o}kologiske scenarie, opgjort som gennemsnit for de f{\o}rste 10 {\r a}r af oml{\ae}gningen (samme klimaserie og periode). For koncentrationerne i det {\o}verste grundvandsmagasin bliver koncentrations{\ae}ndringerne f{\o}rst rigtigt synlige mod slutningen af simuleringsperioden, men ikke i alle beregningspunkter. I 2013 er forskellen mellem den gennemsnitlige nitratkoncentration i det konventionelle og {\o}kologiske scenarie 9 procent (af koncentrationen for det konventionelle scenarie med redoxzone og DOC-nedbrydning implementeret), men det skylles at kun en lille del af det {\o}verste magasin har en alder p{\r a} 0-10 {\r a}r. Forts{\ae}ttes beregningerne derfor i yderligere 30 {\r a}r, vil den lavere udvaskning i det {\o}kologiske scenarie ogs{\r a} sl{\r a} v{\ae}sentligt kraftigere igennem p{\r a} grundvandskoncentrationerne. Men man kan alts{\r a} ikke forvente en umiddelbar effekt af en dyrkningsoml{\ae}gning p{\r a} en nitrattruet grundvandsindvinding.

Omkring 52 \% af nitraten, der forlader rodzonen i indsatsomr{\r a}det, reduceres i redoxzonen. Da dette g{\ae}lder for begge scenarier, er der i de p{\r a}g{\ae}ldende omr{\r a}der ingen effekt af oml{\ae}gning. DOC-betinget denitrifikation har fjernet yderligere ca 6 \% af nitraten i det konventionelle scenarie. Da den nedvaskede DOC-m{\ae}ngde er lille i beregningerne, kan dette tal v{\ae}re undervurderet, is{\ae}r i det {\o}kologiske scenarie. P{\r a} den anden side er det antaget, at alt DOC-nedbrydning medvirker til denitrifikation, hvilket er en overvurdering. Forskellen mellem denne beregning og det {\o}kologiske scenarie er kun yderligere 4 procent i 2013 (konservativ transport og  konventionelt scenarie = 100 \%).

I denne rapport er gjort en r{\ae}kke simple antagelser om nedbrydning, nitrifikation, denitrifikation og sorption i grundvandszonen. Antagelserne f{\o}rer til en udvikling i DOC og DON, der nogenlunde svarer til generelle observationer vedr{\o}rende koncentrationer og C/N-forhold, men der foreligger ikke m{\r a}ledata eller detailunders{\o}gelser, der kan anvendes til en mere detaljeret parameterisering og procesforst{\r a}else. For at opn{\r a} en bedre forst{\r a}else af betydningen af DOC-produktion p{\r a} denitrifikation, ikke mindst i {\o}kologisk produktion, kr{\ae}ves derfor yderligere unders{\o}gelser.
},
        keywords = {DOC, DON, groundwater protection, nitrogen leaching}
}

