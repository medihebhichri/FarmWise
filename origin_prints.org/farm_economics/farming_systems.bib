@misc{orgprints394,
           title = {Nyt fra de {\o}kologiske s{\ae}dskiftefors{\o}g},
       publisher = {{\O}kologisk Landsforening},
            year = {2001},
          author = {Margrethe Askegaard},
         journal = {{\O}kologisk Jordbrug},
           month = {September},
           pages = {6},
          volume = {247},
        abstract = {Nyt fra de {\o}kologiske s{\ae}dskiftefors{\o}g.
Af Margrethe Askegaard, Danmarks JordbrugsForskning, Afd. for Plantev{\ae}kst og Jord 

Aktiviteter i marken lige nu
P{\r a} Jyndevad er alle afgr{\o}derne h{\o}stet og p{\r a} Foulum mangler kun lupinerne. P{\r a} Flakkebjerg er kornafgr{\o}derne modne og der ventes kun p{\r a} t{\o}rvejr. Den sidste nedb{\o}r var godt for b{\r a}de udl{\ae}g, kl{\o}vergr{\ae}s og roer p{\r a} Flakkebjerg. Der har som forventet v{\ae}ret store variationer mellem afgr{\o}der og behandlinger p{\r a} de enkelte lokaliteter og mellem lokaliteterne. N{\r a}r korn og fr{\o} er t{\o}rret og vejet kan vi f{\r a} sat tal p{\r a} forskellene. 

Fra misv{\ae}kst til god v{\ae}kst i havren   
P{\r a} Foulum har havren hvert {\r a}r udviklet sig godt og kraftigt, is{\ae}r i g{\o}dede parceller. P{\r a} Flakkebjerg har v{\ae}ksten derimod ikke v{\ae}ret s{\r a} god. I {\r a}r ser det helt galt ud i havren og is{\ae}r i systemet med fangafgr{\o}de hvor havren er s{\r a}et i opfr{\ae}sede b{\r a}nd i hvidkl{\o}ver. {\r A}rsagen til den ringe v{\ae}kst er sandsynligvis en sen s{\r a}ning som f{\o}lge af det v{\r a}de for{\r a}r, en efterf{\o}lgende lang og t{\o}r periode samt en h{\r a}rd konkurrence fra hvidkl{\o}veren. 

Problemukrudt
Ud over kvik og tidsler som vi sl{\r a}s med p{\r a} flere af lokaliteterne er vi ogs{\r a} ved at f{\r a} problemer med svinem{\ae}lk p{\r a} Foulum og Holeby, f{\o}lfod p{\r a} Jyndevad og agerpadderokke p{\r a} Flakkebjerg. De n{\ae}vnte ukrudtsarter optr{\ae}der meget massivt i nogle f{\r a} parceller. Vi kan dog frygte at de vil brede sig.  Et problem er, at vi mangler viden om hvordan disse ukrudtsarter bedst kan rammes. En hel{\r a}rsbrak kunne sikkert l{\o}se en del af problemerne. En v{\ae}sentlig ulempe ved en l{\ae}ngere brakperiode er imidlertid at vi mister b{\r a}de en produktionsafgr{\o}de og en masse kv{\ae}lstof gennem udvaskning. Erfaringer fra l{\ae}sere modtages meget gerne.

Lupinerne driller 
I s{\ae}dskifternes anden rotation som startede i {\r a}r byttede vi {\ae}rterne ud med lupin med henblik p{\r a} at undg{\r a} opformering af s{\ae}dskifteb{\r a}rne {\ae}rtesygdomme. Lupinerne har generelt v{\ae}ret d{\r a}rlige i konkurrencen med ukrudtet og mange af parcellerne er derfor meget beskidte. N{\ae}ste {\r a}r vil lupinerne blive s{\r a}et p{\r a} dobbelt r{\ae}kkeafstand s{\r a} vi kan radrense. P{\r a} Jyndevad har vi i {\r a}r haft kraftige angreb af gr{\r a}skimmel p{\r a} lupinerne. Det har medf{\o}rt en alt for tidlig nedvisning og modning af en i {\o}vrigt meget flot afgr{\o}de. Gr{\r a}skimmel er en svamp som findes alle steder. D{\o}dt plantemateriale fra affaldende kronblade og fugtigt vejr fremmer svampen. En medvirkende faktor til det h{\r a}rde angreb kan v{\ae}re vanding. 

S{\r a}tid af hvede p{\r a} Foulum
For at f{\r a} en kraftigere for{\r a}rsv{\ae}kst i vinterhveden p{\r a} Foulum valgte vi sidste {\r a}r at s{\r a} hveden i starten af september. I f{\o}rste rotation l{\r a} s{\r a}tiden omkring 1. oktober. Hveden udviklede sig godt efter den tidlige s{\r a}ning, men p{\r a} grund af d{\r a}rlig s{\r a}s{\ae}d som gav uens og tynd fremspiring samt en fugtig jord m{\r a}tte vi af hensyn til kornet droppe de tidlige ukrudtsstriglinger i forbindelse med fremspiringen. Det sker ikke ustraffet p{\r a} en jord med kamille. Selv i vores systemer uden fangafgr{\o}de hvor vi kunne radrense b{\r a}de efter{\r a}r og for{\r a}r har kamillerne udviklet sig kraftigt udfra kornr{\ae}kkerne. P{\r a} trods af de mange kamiller har vi valgt at bibeholde det tidlige s{\r a}tidspunkt. I {\r a}r har vi selv lavet s{\r a}s{\ae}den, og efter kontrol for uds{\ae}dsb{\r a}rne sygdomme samt frarensning af sm{\r a} kerner h{\r a}ber vi at kunne opn{\r a} en t{\ae}t og ensartet fremspiring i r{\ae}kkerne s{\r a} kornet kan holde til en kraftig ukrudtsstrigling.   
},
             url = {https://orgprints.org/id/eprint/394/}
}

@misc{orgprints397,
          volume = {241},
           pages = {6},
           month = {May},
       publisher = {{\O}kologisk Landsforening},
           title = {Nyt fra de {\o}kologiske s{\ae}dskiftefors{\o}g (Nyt fra fors{\o}g med de {\o}kologiske s{\ae}dskifter)},
         journal = {{\O}kologisk Jordbrug},
          author = {Margrethe Askegaard},
            year = {2001},
        abstract = {Nyt fra de {\o}kologiske s{\ae}dskiftefors{\o}g.
Af Margrethe Askegaard, Danmarks JordbrugsForskning, Afd. for Plantev{\ae}kst og Jord 


Aktiviteter i marken lige nu

Der kan v{\ae}re langt mellem planer og virkelighed, is{\ae}r hvis  vejret driller. Det har v{\ae}ret s{\r a} v{\r a}dt, at v{\r a}rs{\ae}den f{\o}rst nu, i skrivende stund d. 2. maj, er blevet s{\r a}et p{\r a} Flakkebjerg. Gyllen blev k{\o}rt ud tidligere p{\r a} t{\o}r overflade, men som f{\o}lge af vejrforholdene var der desv{\ae}rre en stor fugtighed i pl{\o}jelaget. Det gav grimme spor som sandsynligvis vil kunne ses resten af v{\ae}ksts{\ae}sonen.  Jorden p{\r a} Flakkebjerg er i forvejen meget kompakt og vi har tidligere set v{\ae}ksth{\ae}mning af afgr{\o}derne fordi r{\o}dderne har haft sv{\ae}rt ved at udvikle sig i jorden. I dyrkningssystemer med lavt input af n{\ae}ringsstoffer er god rodudvikling en foruds{\ae}tning for et godt udbytte.
P{\r a} den grovsandede jord p{\r a} Jyndevad blev v{\r a}rs{\ae}den s{\r a}et den 9. april. Normalt tager fremspiringen kun 7-10 dage, men det k{\o}lige vejr medf{\o}rte at v{\r a}rs{\ae}den f{\o}rst spirede frem efter 17-18 dage. Nu har v{\r a}rs{\ae}den udviklet 1-1? blad og lupinens f{\o}rste l{\o}vblad er ved at folde sig ud. Havren er m{\o}rkegr{\o}n mens v{\r a}rbyggen er lidt mere gullig. Den ser ud til at mangle varme. Graver man nogle af de fremspirende planter op ses et flot og op til 10 cm dybt rodnet. P{\r a} Foulum blev v{\r a}rs{\ae}den s{\r a}et den 12. april, og i skrivende stund ses begyndende og ensartet fremspiring.

Kl{\o}vergr{\ae}sset 
Man skal ikke lade sig snyde af en d{\r a}rlig kl{\o}verbestand sidst p{\r a} vinteren. Hvor der n{\ae}sten ikke kunne ses hvidkl{\o}ver i februar st{\r a}r der nu flere steder en fin udviklet kl{\o}verbestand. Andelen af kl{\o}ver i kl{\o}vergr{\ae}s som er udlagt i g{\o}det v{\r a}rbyg er ikke s{\r a} stor som i de marker hvor v{\r a}rbyggen har v{\ae}ret ug{\o}det. Det skyldes en st{\ae}rkere konkurrence fra den g{\o}dede v{\r a}rbyg. P{\r a} Foulum har vi fors{\o}gt at reparere kl{\o}vergr{\ae}sset i det tidlige for{\r a}r i de omr{\r a}der hvor kl{\o}verbestanden var svag , men fremspiringen af det is{\r a}ede ser ikke overbevisende ud. Vi h{\r a}ber p{\r a} at den etablerede hvidkl{\o}ver vil fylde hullerne ud i l{\o}bet af de n{\ae}ste m{\r a}neder.       

Ukrudtet
Skraber man i det {\o}verste jordlag ses m{\ae}ngder af lange hvide tr{\r a}de. Det er de kommende ukrudtsplanter som er p{\r a} vej. P{\r a} Foulum har vi haft sv{\ae}rt ved at kontrollere ukrudtet i tilstr{\ae}kkelig omfang i de afgr{\o}der hvor der blandt andet etableres kl{\o}vergr{\ae}sudl{\ae}g. Nu fors{\o}ger vi os med en strategi hvor 2/3 af kl{\o}vergr{\ae}s-uds{\ae}den blandes og s{\r a}s sammen med v{\r a}rbyggen. Dette giver mulighed for at udf{\o}re en ukrudtsharvning efter fremspiring til at tage toppen af ukrudtsproblemet. Efterf{\o}lgende uds{\r a}r vi  igen 2/3 af normal uds{\ae}dsm{\ae}ngde af kl{\o}vergr{\ae}sfr{\o}et. Resultatet er et overforbrug af fr{\o} i forhold til normalen, men vi tager ingen chancer n{\r a}r det drejer sig om at etablere kl{\o}vergr{\ae}smarken som jo st{\r a}r for en stor del af N-forsyningen til s{\ae}dskiftet.    
   
Tidligt s{\r a}et vinters{\ae}d st{\r a}r godt
Vinters{\ae}den p{\r a} Jyndevad og Foulum blev s{\r a}et tidligere end vi plejer; rugen p{\r a} Jyndevad sidst i august og vinters{\ae}den p{\r a} Foulum i starten af september. Resultatet er meget veletablerede planter som har busket sig godt. Men hvor der er fordele er der som regel ogs{\r a} ulemper. Ulempen er at ukrudtet ogs{\r a} f{\r a}r gode muligheder for at udvikle sig. For at reducere problemet blev der allerede i efter{\r a}ret blev der udf{\o}rt flere striglinger p{\r a} Jyndevad og radrensninger p{\r a} Foulum. Det er tydeligt, at det har hjulpet meget. Kamillerne i r{\ae}kkerne kan vi dog ikke stille noget op med, og de vil senere p{\r a} s{\ae}sonen s{\ae}tte deres hvide sk{\ae}r over kornet. 
},
             url = {https://orgprints.org/id/eprint/397/}
}

@misc{orgprints395,
           title = {Nyt fra s{\ae}dskiftefors{\o}g},
       publisher = {{\O}kologisk Landsforening},
            year = {2000},
          author = {Margrethe Askegaard},
         journal = {{\O}kologisk Jordbrug},
           month = {September},
          volume = {223},
           pages = {8},
             url = {https://orgprints.org/id/eprint/395/},
        abstract = {Nyt fra s{\ae}dskiftefors{\o}g.
Af Margrethe Askegaard, forsker ved Danmarks JordbrugsForskning


Marken nu
I skrivende stund er h{\o}sten i hus p{\r a} Foulum og Holeby og p{\r a} Jyndevad mangler kun lupin. Udbytterne er ikke gjort op, men meget tyder p{\r a} at niveauet i Foulum og Jyndevad bliver det samme som sidste {\r a}r mens Holeby melder om h{\o}je udbytter, i vinterhvede op over 80 hkg kerne/ha. De nye udl{\ae}g af kl{\o}vergr{\ae}s og efterafgr{\o}der st{\r a}r generelt p{\ae}nt. P{\r a} Foulum ligger den snittede halm visse steder i et tykt lag over udl{\ae}gget. Det bliver derfor n{\o}dvendigt med en ekstra overk{\o}rsel med snitter for undg{\r a} kv{\ae}lning af udl{\ae}gget. 

Problemer med kl{\o}veren 
Sidste {\r a}rs udl{\ae}g af kl{\o}vergr{\ae}s i v{\r a}rbyggen p{\r a} Foulum resulterede i meget forskellig udvikling af afgr{\o}den, fra meget kl{\o}verrige parceller til parceller n{\ae}sten uden kl{\o}ver. V{\ae}rst s{\r a} det ud i  de g{\o}dede parceller hvor v{\r a}rbyggen sammen med en stor ukrudtsm{\ae}ngde ud{\o}vede en alt for h{\r a}rd konkurrence overfor kl{\o}veren. I et fors{\o}g p{\r a} at reparere p{\r a} skaden opfr{\ae}sede vi, midt juni, 14 cm brede b{\r a}nd i 5-6 cm dybde. I disse striber blev is{\r a}et en blanding af fodervikke og persisk kl{\o}ver. Fremspiringen var nogenlunde i orden, men efterf{\o}lgende har desv{\ae}rre kun f{\r a} planter udviklet sig. 
Resultatet af n{\ae}sten rene rajgr{\ae}s-parceller som forfrugt til vinterhvede vil sandsynligvis kunne m{\ae}rkes i form af ringere udbytter til n{\ae}ste {\r a}r. 

P{\r a} Jyndevad observerede vi sidste efter{\r a}r, i fors{\o}gsled uden husdyrg{\o}dning, nogle kl{\o}vergr{\ae}s-parceller med meget ringe kl{\o}verbestand. Mistanken gik p{\r a} kalium-mangel, men kl{\o}veren havde ikke de karakteristiske hvide pletter p{\r a} bladene og analyser af kalium-indhold i de tiloversblevne kl{\o}verplanter tydede heller ikke p{\r a} at der var tale om kalium-mangel. I for{\r a}ret blev der anlagt et kalium-g{\o}dningsfors{\o}g. Fors{\o}get er endnu ikke opgjort, men der ses tydeligt mere kl{\o}ver i udl{\ae}gget i de kalium-g{\o}dede parceller end i de parceller som ikke er tilf{\o}rt kalium.

Tidlig s{\r a}ning af vinters{\ae}den
Efter en fuld rotation p{\r a} 4 {\r a}r med s{\ae}dskifterne har vi foretaget nogle justeringer med henblik p{\r a} den n{\ae}ste rotation. Blandt andet erstattes vinterhveden p{\r a} Jyndevad med rug, og s{\r a}tidspunkterne af vinters{\ae}den rykkes frem p{\r a} b{\r a}de Jyndevad og Foulum. 

P{\r a} Jyndevad blev rugen s{\r a}et midt i august og den var spiret frem 5 dage senere. Rugen s{\r a}s p{\r a} normal r{\ae}kkeafstand med 250 planter/m2. Begrundelsen for at s{\r a} tidligt p{\r a} Jyndevad er {\o}nsket om at udnytte rugen som en effektiv fangafgr{\o}de efter kl{\o}vergr{\ae}sset. Det vurderes, at ukrudtsharvninger i kombination med rugens gode konkurrenceevne kan klare ukrudtet s{\r a}ledes at radrensning kan undv{\ae}res.  

P{\r a} Foulum skal vi have etableret b{\r a}de rug og hvede i dette efter{\r a}r. Rugen og hveden s{\r a}s i f{\o}rste uge af september p{\r a} dobbelt r{\ae}kkeafstand og med 250 planter/m2. Begrundelsen for at s{\r a} tidligt p{\r a} Foulum er prim{\ae}rt {\o}nsket om en tidligere v{\ae}kststart i for{\r a}ret. Foulum ligger i et koldt omr{\r a}de hvilket medvirker til at planterne kommer sent i gang med v{\ae}ksten om for{\r a}ret. Da vejrforholdene kan drille mere p{\r a} Foulum end i Jyndevad med hensyn til at kunne k{\o}re i marken anvendes dobbelt r{\ae}kkeafstand i vinters{\ae}den. Dette muligg{\o}r radrensning i b{\r a}de efter{\r a}r og for{\r a}r.

P{\r a} Flakkebjerg og Holeby er klimaet noget mildere end i Foulum, og der er ikke {\r a}benlyse fordele ved en v{\ae}sentlig fremrykning af s{\r a}ningen. Tv{\ae}rtimod kan der v{\ae}re den ulempe at ukrudtet i fugtige efter{\r a}r ikke kan reguleres p{\r a} grund af det h{\o}jere lerindhold. Vi rykker dog s{\r a}tiden en lille smule frem til omkring den 25. september. P{\r a} Flakkebjerg anvendes fortsat dobbelt r{\ae}kkeafstand, s{\r a} vi kan fors{\o}ge at bek{\ae}mpe tidsler med radrensning.
}
}

@misc{orgprints396,
           pages = {8},
          volume = {217},
           month = {May},
       publisher = {{\O}kologisk Landsforening},
           title = {Nyt fra Forskningscenter for {\O}kologisk Jordbrug ? fra det {\o}kologiske s{\ae}dskiftefors{\o}g (For{\r a}r uden problemer)},
         journal = {{\O}kologisk Jordbrug},
          author = {Margrethe Askegaard},
            year = {2000},
             url = {https://orgprints.org/id/eprint/396/},
        abstract = {Nyt fra Forskningscenter for {\O}kologisk Jordbrug ? fra det {\o}kologiske s{\ae}dskiftefors{\o}g.

Af Margrethe Askegaard, forsker ved Danmarks JordbrugsForskning


Lige nu i marken
Vi er kommet rigtig godt fra start p{\r a} alle de fire fors{\o}gslokaliteter. Afgr{\o}derne udvikler sig godt og ensartet. Der har endnu ikke vist sig problemer med sygdomme og skadedyr, heller ikke i sukkerroerne p{\r a} Flakkebjerg og Holeby. Dog ser vi p{\r a} alle lokaliteterne bladrandbillernes tydelige visitkort langs bladrandene af {\ae}rteplanterne. Det syn har vi dog v{\ae}nnet os til ? s{\r a}dan ser {\ae}rterne bare ud. Stankelbenlarver har vi heldigvis ikke set noget til siden 1998 p{\r a} Jyndevad, hvor de {\o}delagde flere af vores parceller med v{\r a}rhvede efter kl{\o}vergr{\ae}s. Vi ser begyndende tegn p{\r a} kv{\ae}lstofmangel i vinterhvede i de systemer som ikke tilf{\o}res gylle, ogs{\r a} hvor forfrugten er kl{\o}vergr{\ae}s. 

Etablering af udl{\ae}g
Udl{\ae}ggene, b{\r a}de kl{\o}vergr{\ae}s og efterafgr{\o}de, spirer meget fint frem. Vores etableringsmetode afh{\ae}nger af jordtypen. P{\r a} de lette jorde i Jyndevad og Foulum s{\r a}r vi i 1 cm dybde og uden brug af efterharve. Det betyder, at fr{\o}ene efter s{\r a}ning ligger i en fors{\ae}nket rille i jorden. Denne metode har alle {\r a}r givet en meget sikker fremspiring n{\r a}r de sm{\r a}fr{\o}ede kl{\o}verarter indg{\r a}r i blandingerne. P{\r a} lerjorden i Flakkebjerg er s{\r a}bedet meget knoldet. Her anvendes i stedet s{\r a}ning med l{\o}ftede s{\r a}tude og brug af efterharven for at ryste fr{\o}ene ned mellem jordknoldene.

Kl{\o}ver i kl{\o}vergr{\ae}smarkerne p{\r a} Foulum
P{\r a} Foulum ser vi i {\r a}r en markant forskel i kl{\o}verbestanden i kl{\o}vergr{\ae}smarkerne, fra h{\o}j bestand i nogle parceller til n{\ae}sten ingen kl{\o}ver i andre. Etableringen af kl{\o}verudl{\ae}gget sidste for{\r a}r fulgte den normale procedure. Dog drillede vejret s{\r a} s{\r a}ningen af udl{\ae}gget f{\o}rst skete 11 dage efter s{\r a}ning af d{\ae}ks{\ae}den. Men p{\r a} trods af vejrdrillerier var fremspiringen god. De store forskelle h{\ae}nger sammen med de grundl{\ae}ggende fors{\o}gsbehandlinger, nemlig gylle (med og uden) og efterafgr{\o}de (med og uden) som begge p{\r a}virker forsyningen af kv{\ae}lstof til afgr{\o}den. Og faktorer som p{\r a}virker forsyningen af kv{\ae}lstof til afgr{\o}den p{\r a}virker som bekendt ogs{\r a} konkurrenceforholdet mellem d{\ae}ks{\ae}d og udl{\ae}g. Hvor d{\ae}ks{\ae}den (v{\r a}rbyg) ikke havde en efterafgr{\o}de som forfrugt og heller ikke blev tildelt gylle finder vi i {\r a}r den h{\o}jeste kl{\o}verbestand. Her ydede v{\r a}rbyggen sidste {\r a}r kun 19 hkg kerne pr. ha p{\r a} Foulum. I parceller med efterafgr{\o}de og med gylle, hvor v{\r a}rbyggen gav over dobbelt s{\r a} meget nemlig 43 hkg kerne pr. ha, finder vi i {\r a}r en meget lille bestand af kl{\o}ver i kl{\o}vergr{\ae}sset. Vores vurdering er, at vi i systemer med h{\o}jt N-input til afgr{\o}den vil opleve kl{\o}vergr{\ae}smarker med en mindre kl{\o}verbestand og dermed mindre kv{\ae}lstof-fiksering. Kl{\o}vergr{\ae}smarkerne vil p{\r a} denne m{\r a}de kunne virke som en kv{\ae}lstof-st{\o}dpude i systemerne. Det bliver meget sp{\ae}ndende, at f{\r a} denne v{\ae}ksts{\ae}son med idet vi s{\r a} har den f{\o}rste fulde fuld rotation og dermed bliver istand til at sammenligne de forskellige s{\ae}dskifte-systemer, blandt andet med hensyn til kv{\ae}lstof, kl{\o}verbestande og udbytter.

Vores erfaring med bek{\ae}mpelse af stankelbenlarver
I et tidligere nummer af {\O}kologisk Jordbrug omtales fr{\ae}sning af jorden som et middel til at bek{\ae}mpe stankelbenlarver. Vores erfaringer fra Jyndevad er imidlertid, at fr{\ae}sning ikke kan klare problemet. Derimod fik vi et godt resultat ved at anvende en traktormonteret ?bredbr{\ae}nder? med varmeskjold. En efterf{\o}lgende opt{\ae}lling fandt vi op til 160 d{\o}de laver pr. m2. Br{\ae}ndingen blev udf{\o}rt sen aften og tidlig morgen hvor larverne typisk befinder sig oven p{\r a} jorden for at {\ae}de.
}
}

@misc{orgprints391,
          volume = {18},
           pages = {84--90},
          author = {Margrethe Askegaard and J{\o}rgen Eriksen},
            year = {2002},
         journal = {Soil Use and Management},
           title = {Exchangeable potassium in soil as indicator of potassium status in an organic crop rotation on loamy sand.  },
        abstract = {In organic agriculture, where K may be a limited resource, reliable tools are important in the assessment of K availability in the soil in order to avoid K deficiency.  We investigated the effect of four organic farming systems on the exchangeable K in the plough layer of a six-course crop rotation from 1994 to 1997. The accumulated K balances over the four years varied between 
-49 and  +120 kg K ha-1 and the corresponding exchangeable K (0-20 cm) in autumn 1997 was 7.1 and 9.6 mg K 100 g soil-1, respectively, as an average of the crop rotation. The exchangeable K fraction responded to the K application in manure and to the crop in the rotation. In an additional experiment, no yield response to K was found, despite a low level of exchangeable K. The exchangeable K was a useful indicator of changes in the K status in the farming system with the largest positive K balance whereas this K fraction was insufficient as an indicator in the other three farming systems. The considerable variation of exchangeable K through the crop rotation makes the soil test method most suitable at the crop rotation level where fluctuations caused by crop and management are smoothed out.
},
             url = {https://orgprints.org/id/eprint/391/}
}

@misc{orgprints390,
          volume = {16},
           pages = {200--205},
           title = {Potassium retention and leaching in an organic crop rotation on loamy sand as affected by contrasting potassium budgets},
         journal = {Soil Use and Management},
            year = {2000},
          author = {Margrethe Askegaard and J{\o}rgen Eriksen},
        keywords = {potassium, leaching, organic farming, potassium retention, potassium balances},
        abstract = {. In organic farming, potassium (K) deficiency may become a significant problem due to import restrictions. Knowledge about potential K leaching in systems with different K budgets is therefore important for effective agricultural management. We investigated the effect of four organic farming systems (two livestock densities in combination with two types of organic manure) on crop yields, K leaching and K balances in a six course crop rotation from 1993/94 to 1997/98. Average K concentrations in soil water extracted by means of ceramic suction cups at 1 m depth were 0.6 mg K l-1 corresponding to a K leaching loss of 1.5 kg ha-1 yr-1,which was less than expected from values reported in the literature. Variation in K budgets from ?12 to +30 kg ha-1 yr-1 did not affect K leaching. In an additional experiment with application of 988 kg K ha-1 as KCl, K leaching accounted for only 0.2\% of the applied K although 40\% of the accompanying Cl was leached. The main part of the applied K was retained in the topsoil. It was concluded that K leaching was a result of the fertiliser history rather than of the current K budget.  

},
             url = {https://orgprints.org/id/eprint/390/}
}

