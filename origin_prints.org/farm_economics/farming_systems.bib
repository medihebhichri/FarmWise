             doi = {http://dx.doi.org/10.1590/S1806-66902014000100022},
          number = {1},
           pages = {186--196},
          volume = {45},
           month = {March},
           title = {Turbina e{\'o}lica para agricultura familiar do semi{\'a}rido com inova{\c c}{\~o}es tecnol{\'o}gicas para baixas velocidades de vento.},
