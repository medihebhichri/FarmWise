}

@inproceedings{orgprints356,
       publisher = {F{\O}JO},
           title = {Udnyttelse af husdyrg{\o}dning i s{\ae}dskifter til {\o}kologisk planteavl},
            year = {2000},
          author = {J{\o}rgen E. Olesen and Margrethe Askegaard and Ilse Ank{\ae}r Rasmussen},
         journal = {F{\O}JO rapport},
          editor = {Sven G. Sommer and J{\o}rgen Eriksen},
          volume = {7},
           pages = {75--82},
        abstract = {P{\r a} planteavlsbrug er mulighederne for indk{\o}b af husdyrg{\o}dning ofte begr{\ae}nset til indk{\o}b af konventionel husdyrg{\o}dning inden for gr{\ae}nsen p{\r a} 25\% af kv{\ae}lstofbehovet. Muligheder for at tilgodese afgr{\o}dernes n{\ae}ringsstofbehov p{\r a} denne m{\r a}de afh{\ae}nger af bortf{\o}rsel af n{\ae}ringsstoffer med afgr{\o}derne og hvor store is{\ae}r udvaskningstabene er. Disse aspekter unders{\o}ges i et fors{\o}g med forskellige s{\ae}dskifter p{\r a} fire forskellige jordtyper. S{\ae}dskifterne repr{\ae}senterer systemer med forskellige andele af korn og kv{\ae}lstoffikserende afgr{\o}der. S{\ae}dskifterne afpr{\o}ves p{\r a} fire forskellige m{\r a}der, henholdsvis med og uden brug af efterafgr{\o}der og med og uden brug af husdyrg{\o}dning. I de g{\o}dede parceller blev s{\ae}dskiftet g{\o}det med gylle svarende til 40\% af de ikke-fikserende afgr{\o}der kv{\ae}lstofbehov.

Udbyttet i v{\r a}rbyg viste en nedadg{\r a}ende tendens over de f{\o}rste tre {\r a}r af fors{\o}get i systemet uden g{\o}dning og efterafgr{\o}de. Det mest stabile og h{\o}jeste udbytter opn{\r a}edes i systemet med fangafgr{\o}de, og is{\ae}r hvor der ogs{\r a} anvendtes g{\o}dning. Udvaskningen af b{\r a}de kv{\ae}lstof og kalium viste store forskelle mellem fors{\o}gsstederne. Udvaskningen af kv{\ae}lstof var beskeden og kaliumudvaskningen kunne ignoreres p{\r a} den sandblandende lerjord p{\r a} Sj{\ae}lland og lerjorden p{\r a} Lolland. Derimod forekom der stor udvaskning af b{\r a}de kv{\ae}lstof og kalium p{\r a} den grovsandede jord p{\r a} Jyndevad og nogen udvaskning p{\r a} den lerblandede sandjord ved Foulum. Udvaskningen af kv{\ae}lstof kan begr{\ae}nses ved anvendelse af efterafgr{\o}der og kompenseres gennem dyrkning af kv{\ae}lstoffikserende afgr{\o}der. Der vil derimod v{\ae}re behov for yderligere g{\o}dskning med godkendte kaliumg{\o}dninger p{\r a} den grovsandede jord p{\r a} Jyndevad. Det er endnu for tidligt at afg{\o}re om der ogs{\r a} vil v{\ae}re et yderligere behov for g{\o}dskning med kalium p{\r a} Foulum.

Der var betydelig variation mellem fors{\o}gssteder og {\r a}r i de opn{\r a}ede udnyttelsesprocenter af kv{\ae}lstof i den tilf{\o}rte gylle. Dette kan h{\ae}nge sammen med at gyllen blev udlagt med sl{\ae}beslanger om for{\r a}ret, og ikke nedf{\ae}ldet eller nedpl{\o}jet. En s{\r a}dan nedf{\ae}ldning eller nedpl{\o}jning vil muligvis ogs{\r a} kunne medvirke til at {\o}ge afgr{\o}dens konkurrenceevne over for ukrudtet.
},
             url = {https://orgprints.org/id/eprint/356/}
}

