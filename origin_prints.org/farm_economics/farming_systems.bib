@misc{orgprints36300,
            year = {2019},
          author = {Kristin S{\o}rheim and Johan Barstad and Ildri (Rose) Kristine Bergslid and Annik Magerholm Fet and Bj{\o}rn Egil Fl{\o} and Unni St{\o}bet Lande and Liv Solemdal and H{\r a}vard Steinshamn and Liv Guri Velle},
         address = {Tingvoll, Norway},
          series = {NORS{\O}K Rapport},
           title = {B{\ae}rekraftig verdiskaping i verdsarvomr{\r a}de},
       publisher = {Norwegian centre of organic agriculture},
           month = {July},
           pages = {1--52},
             url = {https://orgprints.org/id/eprint/36300/},
        abstract = {Lokal verdiskaping basert p{\r a} utmarka i verdsarvomr{\r a}da Vestlandsk fjordlandskap kan ikkje sj{\r a}ast isolert, men er tett knytt til heile samfunnsutviklinga p{\r a} desse stadene. Det tradisjonelle landbruket i desse omr{\r a}da har skapt verdfulle naturgode som biologisk mangfald, opne turomr{\r a}de med h{\o}g rekreasjonsverdi, turstiar og anna. Fleire av desse naturgoda er viktige for reiselivsn{\ae}ringa. Kulturlandskapet er ogs{\r a} viktig for verdsarvstatusen i seg sj{\o}lv, ettersom dette er ein viktig del av kjerneverdiane til omr{\r a}det (OUV). 
Utviklinga i landbruket gjer at tungdrivne areal ogs{\r a} i verdsarvomr{\r a}da g{\r a}r ut av bruk. Opph{\o}yr eller l{\r a}g bruksintensitet g{\r a}r spesielt ut over utmarksressursane og dei seminaturlege naturtypane. Utmarka i Geirangerfjordomr{\r a}det er s{\ae}rleg trua, da tradisjonell bruk med beiting, sl{\r a}tt og hausting no i stor grad har teke slutt, og naturtypane gror att med buskar og kratt. Gjengroing f{\o}rer til tap av variasjon, artar og {\o}kologiske funksjonar. Intensivering av landbruket vil ogs{\r a} forringe naturverdiane og landskapskvalitetane.
For at landbruket ikkje berre skal vere eit ?kulisselandbruk?, men vere del av eit berekraftig driftssystem, m{\r a} tilgjengelege arealressursar p{\r a} innmark og utmark nyttast til matproduksjon og andre livsviktige {\o}kosystemtenester. Gjeldande landbrukspolitikk, med m{\r a}l om stordrift og effektivisering av matproduksjonen, er ei sterkare drivkraft enn dei tiltaka som er sett inn for {\r a} bremse og motverke at tungdrivne areal g{\r a}r ut av bruk i verdsarvomr{\r a}da.
Den lokale turist- og reiselivsn{\ae}ringa kan ogs{\r a} hamne i noko av same marginaliserte situasjonen som landbruksn{\ae}ringa fordi dei ikkje lenger er dei mest sentrale akt{\o}rane. Eksterne akt{\o}rar representert ved cruiseindustri, turbussar og storskala reisearrang{\o}rar er ofte i enda st{\o}rre grad premissleverand{\o}rar.
Det trengs ei heilskapleg tiln{\ae}rming for {\r a} l{\o}yse utfordringane. Ein prosess som forskyv maktbalansen fr{\r a} eksterne akt{\o}rar innan masseturisme og i retning lokalt n{\ae}ringsliv og verdiskaping og som inkluderer innbyggjarane, krev nytenking, utvikling av nye forretningsmodellar og evne til gjennomf{\o}ring gjennom demokratiske planprosessar.  
Av ulike konkrete verkemiddel som kan pr{\o}vast ut, men da helst som element i ein st{\o}rre endringsprosess, vil vi peike p{\r a} s{\ae}rskilde landbrukspolitiske verkemiddel i retning sveitsisk modell, turistskatt, anna betaling for fellesgode og bygdeutviklingsprosjekt som kan gi auka tilflytting og ny n{\ae}ringsaktivitet. Dialog og partnarskapsbygging kan fremme ansvarleg medverknad.
Forskingsbehova for verdsarvomr{\r a}da og tilsvarande omr{\r a}de med store og unytta ressursar i utmarka er mange, og arbeidsgruppa si vurdering er summert og konkretisert i denne rapporten.},
        keywords = {World heritage areas, sustainable agriculture and tourism, business models; ecosystem based management}
}

