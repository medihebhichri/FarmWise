@misc{orgprints44606,
          editor = {Turid Str{\o}m},
           month = {November},
           pages = {1--55},
       publisher = {Norwegian Centre of Organic Agriculture},
           title = {{\O}kologisk svinehold og nytt regelverk},
          series = {NORS{\O}K Rapport},
         address = {Tingvoll},
          author = {Kristin S{\o}rheim and Berit Marie Blomstrand},
            year = {2022},
             url = {https://orgprints.org/id/eprint/44606/},
        abstract = {Fra 25.juni 2022 er regelverket for {\o}kologisk svinehold endra. Selvforsyningsgraden er {\o}kt, fra 20\% til 30\% egenprodusert f{\^o}r. Det er gitt et unntak for bruk av konvensjonelle proteinf{\^o}rkilder p{\r a} inntil 5\% fram til 31.12.2025, men bare for unge svin under 35 kg. Nytt i regelverket er krav til fast areal b{\r a}de innend{\o}rs og utend{\o}rs og krav til utforming av utearealet. Utearealet skal gj{\o}res attraktivt for grisene, og en skal helst velge arealer med tr{\ae}r eller skogsarealer. Grisene m{\r a} ha tilgang til leskur og kunne regulere kroppstemperaturen ved forskjellig utetemperatur. Tilrettelegging av et egna uteareal og beiteareal med et mangfold av plantevekster og et godt planlagt vekstskifte vil ogs{\r a} bidra til at selvforsyningsgraden fra egen g{\r a}rd kan {\o}kes b{\r a}de for purker og slaktegris. Vi har i denne rapporten beskrevet eksempler b{\r a}de fra litteraturen og g{\r a}rdsbes{\o}k, som oppfyller kravene i det nye regelverket. Det er viktig at storparten av utearealet kan holdes t{\o}rt, enten ved at
det er plassert h{\o}gt i terrenget og/eller er godt drenert. Det m{\r a} v{\ae}re b{\r a}de t{\o}rre og lune liggeplasser, men ogs{\r a} tilgang til gj{\o}rmebad og avkj{\o}ling. Arealet m{\r a} v{\ae}re stort nok til at grisen kan utfolde seg med roting og l{\o}ping og ogs{\r a} ha mulighet til {\r a} trekke seg unna for {\r a} unng{\r a} sl{\r a}ssing og aggresjon. Purker med unger m{\r a} ha nok plass. De eksemplene vi viser fra g{\r a}rder vi har bes{\o}kt hadde ca. 200 m2 per dyr, noe som er langt over minimumskravet til areal. Dette ga veldig gode l{\o}sninger med tilgang til beite og god plass til aktivitet og dermed ogs{\r a} redusert smittepress. Vegetasjonsbelter mot vassdrag er n{\o}dvendig for {\r a} hindre forurensning. Det er viktig {\r a} velge tr{\ae}r, busker og planter som hindrer erosjon og samler opp n{\ae}ringsstoffer. Isolerte hytter b{\r a}de for enkeltdyr og flere dyr sammen er gode l{\o}sninger, likeens flyttbare f{\^o}ringsautomater eller f{\^o}ringstroer med tak. Hyttene b{\o}r flyttes rundt p{\r a} beitet for {\r a} hindre at det blir for opptr{\r a}kket og bl{\o}tt. Innhengning for {\r a} samle dyr n{\r a}r det er behov for det er ogs{\r a} n{\o}dvendig. Alternativet til flyttbare hytter er {\r a} anlegge beite i n{\ae}rheten av fj{\o}sbygninger der dyra kan ha tilgang til ly og varme, eller avkj{\o}ling, om det trengs. Inngjerding av grisene fungerer godt med elektrisk gjerde, med to eller tre tr{\r a}der i riktig h{\o}yde for {\r a} holde gris av ulik alder og st{\o}rrelse innenfor. Grisen reagerer sterkt p{\r a} str{\o}m. Gjerdet m{\r a} kontrolleres daglig. Det er usikkert hvor godt elektriske gjerder vil holde villsvin ute. Andre former for inngjerding kan v{\ae}re sm{\r a}maska nettinggjerder, nedgravd 50-60 cm for {\r a} hindre dyr i {\r a} grave seg ned under gjerdet. Ved mistanke eller varsel om villsvin eller annen risiko for smittsom sykdom, b{\o}r dyra kunne settes inn og sikres for en periode. Generelt er det viktig {\r a} bruke rikelig med str{\o}. Halm er best for {\r a} holde varmen og som rotemateriale, men det kan ogs{\r a} brukes skogsflis og kombinasjoner av ulike materialer, selv om disse har noe d{\r a}rligere isolerende egenskaper. Det er mulig {\r a} legge til rette for utegris og {\o}kologisk produksjon i samsvar med nye regler de fleste steder i Norge, ogs{\r a} om vinteren. N{\o}dvendige investeringer er lavere med hytter og enkle fj{\o}s enn med konvensjonelle grisefj{\o}s, men driftsforma er noe mer arbeidskrevende. Det m{\r a} planlegges mer tid til tilsyn og stell. Vaksinasjon og systematisk parasittbehandling og vekstskifte er viktig for {\r a} unng{\r a} sykdom. Det samme er mer omtanke i avlsarbeidet, der en legger mer vekt p{\r a} robuste dyr i det milj{\o}et de skal v{\ae}re. Det er sv{\ae}rt f{\r a} som driver med {\o}kologisk gris i Norge. I intervju sier de som allerede driver med det at de {\o}nsker mer kunnskap og forskning p{\r a} {\o}kt selvforsyningsgrad av protein, alternativer til antibiotika og parasittmidler, egne avlsm{\r a}l for utegris, grovf{\^o}ropptak og mer kunnskap om beiterotasjon.},
        keywords = {Animal welfare, animal health, organic production
Utegris, husdyrvelferd, dyrehelse}
}

@misc{orgprints42788,
            year = {2021},
           month = {August},
          author = {Kristin S{\o}rheim and Juni Rosann E. Johanssen and Steffen A. Adler},
           pages = {1--9},
           title = {Short report Norway - Kalvelykke \& GrazyDaiSy},
        keywords = {Cow - calf grazing strategies; nurse cows},
        abstract = {In this pilot study we investigated weight gain, health and behaviour of five calf pairs (own calf plus foster calf) that spent about nine weeks on pasture together with their respective (foster) mothers. Neither the cows nor the calves were given concentrates in the period. The pasture was of generally poor quality, and the results could have been even more in favour of weight gain in calves and with less weight loss of cows on a high-quality pasture.
The cows were Limousin crosses. They were not milked, only suckled by their calves. Three foster calves were removed from their foster mothers because they obviously did not get enough milk. The behaviour observations showed that these calves did not manage to suckle neither their foster mother nor the other cows to some particular degree. The other calves showed good weight gain and health, and the cows? body condition remained fairly stable even though the pasture quality was poor. Weaning was abrupt, but cows and calves stopped bawling after 3-4 days.
The control-group calves kept indoors had a higher weight gain, but these were also fed more intensively. 
We tested a collar with an accelerometer to see if it is possible to use the data to distinguish between different types of behaviour. Preliminary results show that it is easy to tell the difference between being ?active? and ?passive?, and there are indications that it is possible to distinguish suckling from other activities. These results can contribute to developing systems for monitoring grazing livestock.},
             url = {https://orgprints.org/id/eprint/42788/}
}

@misc{orgprints43315,
           title = {Innovative driftssystemer for {\o}kologisk kylling},
       publisher = {Norwegian centre of organic agriculture},
            year = {2021},
          author = {Kristin S{\o}rheim and Juni Rosann E. Johanssen and Susanne Friis Pedersen and Steffen A. Adler},
         address = {Tingvoll},
          series = {NORS{\O}K Rapport},
           month = {December},
           pages = {1--101},
        keywords = {Organic broiler production, integrated meat and plant production, animal welfare, livestock building
{\O}kologisk kyllingproduksjon, husdyrvelferd},
        abstract = {Gjennom dette prosjektet, finansiert av Regionalt forskingsfond Midt-Norge og Regionalt forskingsfond M{\o}re og Romsdal, Tr{\o}ndelag fylkeskommune, M{\o}re og Romsdal fylkeskommune, Talg{\o} M{\o}reTre AS, {\O}kologisk Norge og Nors{\o}k har vi gjennom litteraturstudium, sp{\o}rreunders{\o}kelse og feltfors{\o}k sett p{\r a} muligheter for {\r a} utvikle et mer kretsl{\o}psbasert driftssystem for {\o}kologisk slaktekylling med tilrettelagt uteomr{\r a}de.
Vi har sammenlignet to ulike saktevoksende hybrider, rowan ranger og sasso, p{\r a} uteomr{\r a}de med bare grasvekst eller mer beplanta uteomr{\r a}de og studert tilvekst, f{\^o}rforbruk og atferd. Sasso er en enda mer saktevoksende hybrid enn rowan ranger. Videre har vi planlagt kyllinghus med tilrettelagt uteomr{\r a}de og sett p{\r a} {\o}konomi, milj{\o} og muligheter for sirkul{\ae}re verdikjeder knyttet til {\o}kologisk kyllingproduksjon.
I fors{\o}ket fant vi at rowan ranger n{\r a}dde h{\o}yere vekt ved slaktalder p{\r a} 72 dager enn sasso. Beplantning hadde positiv effekt p{\r a} vekta p{\r a} dag 64, men det var ingen forskjell ved slaktetidspunktet. Kraftf{\^o}ropptaket var ikke forskjellig for de to hybridene og vegetasjonen hadde ikke effekt p{\r a} f{\^o}ropptaket. Rowan ranger hadde h{\o}yere tilvekst og f{\^o}reffektivitet enn sasso. Sasso oppholdt seg mer ute, var mer aktiv og brukte st{\o}rre del av utearealet enn rowan ranger. Beplantning stimulerte bruk av uteomr{\r a}de for begge hybridene. Kyllingene hadde st{\o}rst f{\^o}rrelatert aktivitet p{\r a} uteomr{\r a}de om kvelden, deretter om morgenen, mens de var mye i ro midt p{\r a} dagen n{\r a}r det var sol og varmt.
Beplantning og tilrettelegging av uteareal b{\o}r skje i god tid f{\o}r arealet tas i bruk til kyllingene slik at plantene f{\r a}r etablert seg og gir skjerming, ly og muligheter for f{\^o}ropptak. Det er beskrevet plantetyper, plantem{\o}nster og nytteverdi av de ulike plantene.
Kortikosteron er et hormon som sier noe om stress og p{\r a}kjenninger hos et dyr, og vi m{\r a}lte kortikosteron i fj{\ae}r p{\r a} kyllingene dagen f{\o}r slakting. Niv{\r a}et var h{\o}yere hos hanene enn hos h{\o}nene og det var tendens til h{\o}yere niv{\r a} hos rowan ranger enn hos sasso.
Modulbaserte kyllinghus i tre synes {\r a} v{\ae}re godt egna og er konkurransedyktige i pris for b{\r a}de {\o}kologisk og konvensjonell kyllingproduksjon og i ulik skala. Kyllinghus produsert i tre har et lavere milj{\o}avtrykk enn hus av st{\r a}l og betong.
Det konseptet vi beskriver i rapporten er tilrettelagt for bruk av uteareal, og utearealet er planlagt slik at det annethvert {\r a}r f{\r a}r hvile for kyllingene og kan brukes til planteproduksjon, som korn eller gr{\o}nnsaker og rotvekster.
V{\r a}re {\o}konomiske beregninger fra fors{\o}ket viser at dekningsbidraget (inkl. faste kostnader) er p{\r a} ca. 15,50 kr/kylling for rowan ranger og ca. 8 kr/kylling for sasso i en produksjon p{\r a} 72 000 kyllinger/{\r a}r. I mindre skala (10 800 kyllinger/{\r a}r) og med et enklere bygg, er dekningsbidraget ca. 14 kr/kylling for rowan ranger og ca. 7 kr/kylling for sasso. Dekningsbidraget er h{\o}yere enn for konvensjonell kylling, noe som skyldes merpris for {\o}kologisk kylling og h{\o}yere slaktevekt.
Sp{\o}rreunders{\o}kelser viser at forbrukere er stadig mer opptatt av dyrevelferd og de sier de er villige til {\r a} betale merpris for god dyrevelferd og milj{\o}vennlig produksjon. En sp{\o}rreunders{\o}kelse vi gjorde blant 2000 forbrukere viste at 65\% var villige til {\r a} betale en merpris p{\r a} 20\% eller mer for bedre dyrevelferd i kyllingproduksjonen. Over 70\% av de som ble spurt syntes fj{\o}rfe skal ha tilgang til uteomr{\r a}de. Samtidig sier markedsakt{\o}rene at det er vanskelig {\r a} kommunisere at det er behov for merpris for {\o}kologisk kylling.
Integrert {\o}kologisk kylling- og planteproduksjon kan gi bedre {\o}konomi, bedre ressursutnyttelse og bedre milj{\o}profil n{\r a}r en tar i betraktning flere milj{\o}parametere enn klimagassutslipp, samt inkluderer bruken av arealet til produksjon av eget f{\^o}r eller matvekster. F{\^o}ret utgj{\o}r en vesentlig del av klimagassutslipp fra produksjonen og ved {\r a} disponere utearealet som tilh{\o}rer produksjonsenheten til lokalprodusert f{\^o}r kan det erstatte litt av det innkj{\o}pte/importerte {\o}kologiske f{\^o}ret.},
             url = {https://orgprints.org/id/eprint/43315/}
}

@misc{orgprints43314,
       publisher = {Norsk senter for {\o}kologisk landbruk},
           title = {Enklere, billigere og automastiserte fj{\o}sl{\o}sninger i melkeproduksjonen},
          series = {NORS{\O}K Rapport},
         address = {Tingvoll},
          author = {Kristin S{\o}rheim and S{\o}ren Lund and Bj{\o}rn Steinar Skarb{\o}},
            year = {2021},
           pages = {1--42},
           month = {December},
        abstract = {I dette regionale forprosjektet har vi sett p{\r a} muligheter for enkle og rimelige ombygginger i melkeproduksjonsfj{\o}s for {\r a} gi mulighet for lengre tids samv{\ae}r mellom ku og kalv. 
Vi har studert samspillet mellom automatiske melke- og f{\^o}ringssystemer og f{\^o}ropptak, tilvekst, helse og atferd hos kalver som har f{\r a}tt g{\r a} sammen med m{\o}drene sine i en egen velferdsavdeling i en m{\r a}ned f{\o}r de ble separert og flytta over i en egen binge. Ett form{\r a}l var {\r a} se p{\r a} om kalvene har mulighet for {\r a} l{\ae}re seg automatiske f{\^o}ringsl{\o}sninger samtidig med at de har tilgang p{\r a} mora, uten for mye ekstra arbeid for bonden og uten at investeringene i fj{\o}sbygget blir for store.
Kalvene hadde god tilvekst i dieperioden (797 g/dag) men tapte seg mye ved separasjon (tilvekst p{\r a} 190 g/dag etter separasjon). 3 av 7 kalver l{\ae}rte seg {\r a} drikke av melkeautomaten med det minimale tidsforbruket vi brukte p{\r a} oppl{\ae}ring i dette fors{\o}ket, i gjennomsnitt drakk de 6,6 liter/dag. Bare 4 av 7 kalver begynte {\r a} ta til seg kraftf{\^o}r etter separasjonen. Selv om kalvene oppholdt seg mye av tida i kalvegjemme med automatf{\^o}ring ogs{\r a} i dieperioden, og selv om de var nysgjerrige og gikk inn i automaten flere ganger per dag, l{\ae}rte de ikke {\r a} drikke av den. Det b{\o}r derfor brukes mer tid og finnes andre l{\o}sninger for raskere oppl{\ae}ring til melkef{\^o}ring av kalvene hvis en ikke vil la dem g{\r a} sammen med mor til de avvennes gradvis ved 7-9 uker. 
For bondens {\o}konomi vil grad av fylling av melkekvoten v{\ae}re avgj{\o}rende for hvor lenge det l{\o}nner seg {\r a} la kalven die mora. Prisen p{\r a} melkeerstatning og melk til kalven er om lag det samme dersom kvoten ikke er fylt, mens det vil l{\o}nne seg {\r a} bruke egen melk, fersk eller syrna, dersom kvoten likevel blir fylt. 
Av 14 kyr som var med i fors{\o}ket var det 6 kyr som ikke leverte noe melk i roboten. 8 kyr leverte melk i roboten og gjennomsnittlig dagsleveranse varierte fra 6,7 til 35,8 liter. For 7 kyr som vi fulgte b{\r a}de i dieperioden og etter separasjon {\o}kte daglig melkeleveranse fra 20,6 liter i dieperioden til 33,1 liter etter separasjon. Gjennomsnittlig celletall varierte fra 37 til 335 og var generelt litt h{\o}yere mens kyrne gitt med kalv, men forskjellen var ikke signifikant. Vi registrerte lite feilmelkinger i roboten og en av 14 kyr ble behandlet for mastitt. 
I prosjektet har vi bes{\o}kt flere g{\r a}rder som har tenkt p{\r a} mulighet for {\r a} ha ku og kalv sammen, og vi har sett p{\r a} hvordan det kan tilrettelegges for et slikt driftsopplegg med enkle l{\o}sninger i eksisterende fj{\o}s. Vi legger fram fire forslag til l{\o}sninger med kostnadsoverslag som kan ha overf{\o}ringsverdi til andre. P{\r a} bakgrunn av dette fors{\o}ket ser vi at det er avgj{\o}rende med en god logistikk i fj{\o}set, der det f{\o}rst er en rein og t{\o}rr f{\o}debinge (eller fellesbinge for flere kyr) der mora og kalven f{\r a}r v{\ae}re i fred de f{\o}rste d{\o}gna, deretter kan de g{\r a} i en velferdsavdeling der kua har tilgang til melkerobot og kalvene samtidig har tilgang til eget kalvegjemme for tilleggsf{\^o}ring og hvile, f{\o}r de gradvis separeres og avvennes fra mora. Tidspunkt og metode for avvenning m{\r a} ogs{\r a} tilpasses bonden. Vi har l{\ae}rt mye ved sanntids kameraoverv{\r a}king av kyr og kalver i dette fors{\o}ket og mener det er viktig {\r a} fortsette arbeidet med {\r a} teste ut flere metoder og forbedringer til hvordan vi kan l{\ae}re kalvene {\r a} ta opp f{\^o}r fra automater slik at avvenning og separasjon kan g{\r a} enda bedre. Dette b{\o}r i tilfelle gj{\o}res i et st{\o}rre fors{\o}k med mulighet til {\r a} gjennomf{\o}re innl{\ae}ringsaktiviteter for kalvene.},
        keywords = {Cow-calf contact, animal welfare, dairy farming systems,
Ku-kalv, kalvef{\^o}ring, husdyrvelferd, mj{\o}lkeproduksjon},
             url = {https://orgprints.org/id/eprint/43314/}
