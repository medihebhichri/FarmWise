          number = {1},
          author = {Fernanda Melo Gomes and Andr{\'e}a Maria Teixeira Fortes and J{\'e}ssica da Silva and Thaliny Bonamigo and Tassiane Terezinha Pinto},
            year = {2013},
         journal = {Revista Brasileira de Agroecologia},
           title = {Efeito alelop{\'a}tico da fitomassa de Lupinus angustifolius (L.) sobre a germina{\c c}{\~a}o e desenvolvimento inicial de Zea mays (L.) e Bidens pilosa (L.).},
        keywords = {Aleloqu{\'i}micos, Fabaceae, Tremo{\c c}o azul, Extrato vegetal.
Allelochemicals, Fabaceae, Blue lupine, Vegetable extract.
},
        abstract = {O objetivo deste trabalho foi avaliar o efeito alelop{\'a}tico do extrato aquoso de folhas frescas de Lupinus angustifolius L. sobre alface (como planta indicadora), sobre a esp{\'e}cie cultivada milho e a invasora pic{\~a}o-preto. As sementes dessas esp{\'e}cies foram submetidas aos extratos aquosos preparados na propor{\c c}{\~a}o de 200g/L (20\% p/v) de {\'a}gua destilada e redilu{\'i}dos nas concentra{\c c}{\~o}es de 20\% (4\% p/v), 40\% (8\% p/v), 60\% (12\% p/v), 80\% (16\% p/v) e 100\% (20\% p/v), mais um tratamento testemunha com {\'a}gua destilada. Notou-se interfer{\^e}ncia alelop{\'a}tica negativa em alface e na invasora pic{\~a}o-preto enquanto que para o milho foi observada apenas uma pequena interfer{\^e}ncia negativa no comprimento m{\'e}dio de raiz, onde a diferen{\c c}a estat{\'i}stica apresenta-se na presen{\c c}a do extrato a 100\%. Conclui-se assim que o L. angustifolius possui potencial alelop{\'a}tico e pode ser usado na sucess{\~a}o de culturas do milho, al{\'e}m de reduzir o desenvolvimento do pic{\~a}o-preto como op{\c c}{\~a}o de herbicida natural.},
             url = {https://orgprints.org/id/eprint/24419/}
}

@misc{orgprints27591,
          number = {1},
           month = {May},
          volume = {3},
           pages = {78--84},
           title = {Compara{\c c}{\~a}o qu{\'i}mica do composto org{\^a}nico de esterco bovino e leguminosas: leucena (Leucaena lleucocephalla (Lam )de Wit) e sombreiro (Clitoria fairchildiana Haward).},
          author = {Josilda Junqueira Ayres Gomes and Cintia Viviane Ara{\'u}jo Costa and Ana Paula Rosa Teixeira Teixeira and Valdirene Socorro Dias},
            year = {2008},
         journal = {Revista Brasileira de Agroecologia},
        keywords = {Leucena, Sombreiro, Esterco bovino, Composto Org{\^a}nico.
Leucaena, Philippine pigeonwings, Bovine manure.},
        abstract = {O composto {\'e} utilizado na agricultura, como alternativa para melhorar as condi{\c c}{\~o}es f{\'i}sicas e a fertilidade do solo. Com objetivo de conhecer a composi{\c c}{\~a}o qu{\'i}mica do composto de esterco bovino e leguminosas; leucena (Leucaena leucocephala Lam de Wit), e sombreiro (Clitoria fairchildiana Haward) em diferentes propor{\c c}{\~o}es. O presente trabalho foi desenvolvido na Fazenda Escola da Universidade Estadual do Maranh{\~a}o (UEMA), no munic{\'i}pio de S{\~a}o Lu{\'i}s. O delineamento experimental foi em blocos ao acaso com sete tratamentos e quatro repeti{\c c}{\~o}es: T1 - 75 kg de esterco bovino + 25 kg de folhas de leucena; T2 - 50 kg de esterco bovino + 50 kg de folhas de leucena; T3 - 25 kg de esterco bovino + 75 kg de folhas de leucena; T4 - 75 kg de esterco bovino + 25 kg de folhas de sombreiro; T5 ? 50 kg de esterco bovino + 50 kg de folhas de sombreiro; T6 (testemunha) 100 kg de esterco bovino. Foram analisadas amostras do esterco bovino compostado, e dos compostos com leguminosas para determina{\c c}{\~a}o dos componentes qu{\'i}micos. Conforme resultados de an{\'a}lises, na maioria dos tratamentos os teores totais de MO, Ph, P, K e Ca, Mg e H+Al foram superiores {\`a} testemunha, constituindo fontes alternativas de fertilizantes para a agricultura.},
             url = {https://orgprints.org/id/eprint/27591/}
}

@misc{orgprints27581,
         journal = {Revista Brasileira de Agroecologia},
            year = {2008},
          author = {Josilda Junqueira Ayres Gomes and Ana Paula Rosa Teixeira and Valdirene S Dias and C{\'i}ntia V. Ara{\'u}jo Costa},
           title = {Composi{\c c}{\~a}o qu{\'i}mica de composto org{\^a}nico preparado com esterco de eq{\"u}ino e leucena. (Leucaena lleucocephalla Lam de Wit).},
           pages = {71--77},
          volume = {3},
           month = {February},
          number = {1},
        abstract = {A compostagem {\'e} um dos meios mais eficientes para a transforma{\c c}{\~a}o dos res{\'i}duos vegetais e animais em produtos org{\^a}nicos utiliz{\'a}veis na agricultura, constituindo alternativa para obten{\c c}{\~a}o de insumos, reduzindo a utiliza{\c c}{\~a}o de fertilizantes qu{\'i}micos. Este trabalho foi conduzido na Fazenda Escola da Universidade Estadual do Maranh{\~a}o ? UEMA, com o objetivo de avaliar a composi{\c c}{\~a}o qu{\'i}mica do composto de esterco eq{\"u}ino e leucena, em diferentes propor{\c c}{\~o}es. Os tratamentos foram as diferentes pilhas: T1, Testemunha (100kg de esterco de eq{\"u}ino), T2, 75kg de esterco de eq{\"u}ino + 25kg de leucena; T3, 50kg de esterco de eq{\"u}ino +50kg de leucena; T4, 25kg de esterco de eq{\"u}ino + 75kg de leucena. Aos 67 dias ap{\'o}s a montagem das pilhas de compostagem foram coletadas amostras dos compostos resultantes de cada tratamento, para an{\'a}lise da composi{\c c}{\~a}o qu{\'i}mica; teores de Mat{\'e}ria Org{\^a}nica (M.O), pH, F{\'o}sforo (P), Pot{\'a}ssio (K), C{\'a}lcio (Ca), Magn{\'e}sio (Mg), Hidrog{\^e}nio e Alum{\'i}nio (H+Al), Carbono e Nitrog{\^e}nio. De acordo com resultados obtidos, os tratamentos diferiram estatisticamente entre si, em rela{\c c}{\~a}o as caracter{\'i}sticas avaliadas. Os resultados das an{\'a}lises demonstraram que o T4 (75kg de esterco de eq{\"u}ino + 25kg de leucena) apresentou valores elevados em rela{\c c}{\~a}o {\`a} testemunha, T1 nos teores de M.O (14,9\%); P (857mg/dm3); e Mg (77,5mmol/dm3), associado a um pH de acidez baixa (6,5), resultando um composto de excelente qualidade. Todos os tratamentos apresentaram caracter{\'i}sticas qu{\'i}micas em n{\'i}veis satisfat{\'o}rios, em rela{\c c}{\~a}o {\`a} fertilidade do solo, exigidos pela maioria das esp{\'e}cies cultivadas no Brasil.},
        keywords = {Composto Org{\^a}nico, Leucena, Esterco eq{\"u}ino.
Organic compost, Leucaena, Equine manure.},
             url = {https://orgprints.org/id/eprint/27581/}
}

@mastersthesis{orgprints24844,
           pages = {1--183},
            year = {2003},
          author = {Alexandre Gon{\c c}alves},
          school = {Universidade Federal de Lavras},
           title = {Agroecologia, saber local e mercado - um estudo sobre a agricultura familiar de Po{\c c}o Fundo - MG.},
             url = {https://orgprints.org/id/eprint/24844/},
        abstract = {A produ{\c c}{\~a}o de alimentos org{\^a}nicos {\'e} uma atividade que vem sendo desenvolvida por agricultores familiares de diferentes regi{\~o}es do Brasil. A comercializa{\c c}{\~a}o desses produtos tem especificidades, {\'e} composta por diferentes canais e um sistema de certifica{\c c}{\~a}o dos produtos. A produ{\c c}{\~a}o e certifica{\c c}{\~a}o org{\^a}nica formam uma rede que articula pontos localizados em diferentes partes do mundo. Os pressupostos ligados a produ{\c c}{\~a}o org{\^a}nica envolvem aspectos econ{\^o}micos, ambientais e sociais. Atrav{\'e}s deste estudo procurou-se compreender como os agricultores de Po{\c c}o Fundo - Minas Gerias percebem e relacionam-se com a produ{\c c}{\~a}o e comercializa{\c c}{\~a}o de alimentos org{\^a}nicos. Para isto, utilizou-se um referencial te{\'o}rico que mant{\'e}m interfaces com diferentes disciplinas, como agroecologia, geografia, antropologia, sociologia e economia, analisando as rela{\c c}{\~o}es entre agricultura familiar, o espa{\c c}o geogr{\'a}fico e as rela{\c c}{\~o}es de troca com o mercado. Para os agricultores de Po{\c c}o Fundo a agricultura org{\^a}nica significa uma rea{\c c}{\~a}o {\`a} um processo de desestrutura{\c c}{\~a}o da realidade local causada pela agricultura moderna e pelo avan{\c c}o do sistema capitalista no campo. A agricultura org{\^a}nica em Po{\c c}o Fundo t{\^e}m um significado amplo, que envolvem aspectos pol{\'i}ticos, ideol{\'o}gicos, ambientais, culturais e econ{\^o}micos. Os espa{\c c}os de comercializa{\c c}{\~a}o formam territ{\'o}rios que comportam rela{\c c}{\~o}es de poder, envolvendo um sistema de certifica{\c c}{\~a}o dos produtos, no qual os agricultores s{\~a}o obrigados a adequar-se {\`a} um conjunto de normas e procedimentos para terem acesso a esse mercado. Os agricultores, ao mesmo tempo que adeguam-se {\`a}s normas definidas pelos agentes do mercado, constr{\'o}em arranjos que fortalecem o grupo e, que garantem um processo de desenvolvimento baseado na conviv{\^e}ncia com a natureza e calcado em seus sistemas de produ{\c c}{\~a}o e reprodu{\c c}{\~a}o do saber.

},
        keywords = {Alimentos org{\^a}nicos, Agricultura familiar, Mercados, Brazil.
Organic foods, Family farms, Markets, Brazil.}
}

