@misc{orgprints26517,
           pages = {240--247},
          volume = {9},
           month = {February},
          number = {1},
         journal = {Revista Brasileira de Agroecologia},
          author = {Marcelo Dotto and Kelli Pirola and Alessandro Jaquiel Waclawovsky and S{\'e}rgio Miguel Mazaro and Am{\'e}rico Wagner Junior},
            year = {2014},
           title = {Aplica{\c c}{\~a}o pr{\'e}-colheita de extratos vegetais em morangueiro.},
        keywords = {Fragaria x Ananassa, Controle alternativo de doen{\c c}as, Indutores de resist{\^e}ncia.
Fragaria x Ananassa, Alternative control of diseases, Resistance inductors.},
        abstract = {O objetivo deste trabalho foi avaliar o efeito de extratos vegetais de folhas de pitangueira (Eugenia uniflora), flores de cal{\^e}ndula (Calendula officinalis) e cap{\'i}tulo floral de cravo da {\'i}ndia (Syzygium aromaticum), al{\'e}m de calda bordalesa, sobre a indu{\c c}{\~a}o de resist{\^e}ncia a doen{\c c}as p{\'o}s-colheita de morango quando aplicados na pr{\'e}-colheita. O trabalho a campo foi realizado em uma propriedade rural, com sistema de cultivo agroecol{\'o}gico, localizada no munic{\'i}pio de P{\'e}rola D?Oeste, e os estudos laboratoriais foram desenvolvidos na Universidade Tecnol{\'o}gica Federal do Paran{\'a}, C{\^a}mpus Dois Vizinhos ? PR. O delineamento experimental foi em blocos casualizados, com quatro repeti{\c c}{\~o}es, utilizando-se a campo 20 plantas por parcela e 20 frutos por bandeja na p{\'o}s-colheita. Os extratos vegetais foram aplicados utilizando-se a concentra{\c c}{\~a}o de 10\% e a calda bordalesa a 1\%. As avalia{\c c}{\~o}es foram realizadas no momento da colheita e ap{\'o}s quatro dias de armazenamento dos frutos {\`a} temperatura de 24oC {$\pm$}2, sendo avaliados os par{\^a}metros f{\'i}sico-qu{\'i}micos de incid{\^e}ncia de podrid{\~o}es, firmeza de polpa, perda de massa, s{\'o}lidos sol{\'u}veis totais (SST) e acidez titul{\'a}vel; e os bioqu{\'i}micos de a{\c c}ucares totais e redutores, teor de prote{\'i}nas e atividade da enzima fenilalanina am{\^o}nialiase (FAL). Os extratos vegetais de folhas de pitangueira, flores de cal{\^e}ndula e cap{\'i}tulo floral de cravo da {\'i}ndia, al{\'e}m de calda bordalesa apresentaram efeito sobre aos SST e acidez titul{\'a}vel no momento da colheita, bem como nos n{\'i}veis de a{\c c}ucares totais na p{\'o}s-colheita, no entanto, os tratamentos n{\~a}o interferem nos par{\^a}metros de incid{\^e}ncia de podrid{\~o}es, perda de massa, firmeza de polpa e atividade da FAL.},
             url = {https://orgprints.org/id/eprint/26517/}
}

@misc{orgprints2841,
            year = {2001},
          author = {Lars Drake and Johanna Bj{\"o}rklund},
           month = {November},
           title = {Effekter av olika s{\"a}tt att producera livsmedel ? en inventering av j{\"a}mf{\"o}relser mellan ekologisk och konventionell produktion},
        keywords = {milj{\"o}effekter, ekologiskt lantbruk, konventionellt lantbruk, produktionssystem, j{\"a}mf{\"o}relse, uth{\r a}lligt},
        abstract = {Ekologisk produktion har totalt sett en mer positiv, eller mindre negativ, inverkan p{\r a} milj{\"o}n. Det pekar fyra omfattande inventeringar, avseende 18 europeiska l{\"a}nder (inklusive Sverige), som j{\"a}mf{\"o}r ekologisk och konventionell produktion p{\r a}. Att en {\"o}kad omfattning av den ekologiska produktionen utan tvivel skulle minska de negativa milj{\"o}effekterna och resursanv{\"a}ndningen i jordbruket {\"a}r den slutsats som dras i alla de fyra inventeringarna.

Bilden som ges av genomf{\"o}rda studier {\"a}r dock i n{\r a}gra avseenden inte entydig. Detta g{\"a}ller till exempel kv{\"a}vel{\"a}ckage, samt fosfor- och kaliumf{\"o}rs{\"o}rjning. Inom en del omr{\r a}den, framf{\"o}r allt livsmedelskvalitet, djurh{\"a}lsa och klimatf{\"o}r{\"a}ndringar, kr{\"a}vs ocks{\r a} mer forskning f{\"o}r att kunna g{\"o}ra en tillf{\"o}rlitlig j{\"a}mf{\"o}relse. F{\"o}r vissa faktorer {\"a}r dessutom skillnaderna sm{\r a}. En allm{\"a}n iakttagelse vad g{\"a}ller effektivitet {\"a}r att den ekologiska produktionen utnyttjar mer mark medan den konventionella produktionen utnyttjar mer energi och andra externa insatser.

Jordbruksproduktion {\"a}r en mycket komplex verksamhet som {\"a}r beroende av lika komplexa ekosystem. F{\"o}r{\"a}ndringar som g{\"o}rs i ett s{\r a}dant system medf{\"o}r ofelbart ett antal oavsedda effekter vid sidan om de avsedda.},
