        keywords = {Ordenamento de uso e ocupa{\c c}{\~a}o das terras, Planejamento agr{\'i}cola, Geom{\'a}tica, Geotecnologia.
Environmental planning, Land use, Geotechnologies, Geomatic.}
}

@misc{orgprints26341,
