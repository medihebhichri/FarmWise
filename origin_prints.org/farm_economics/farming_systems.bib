        abstract = {Testes para avaliar o consumo foliar de batata submetida a diferentes tinturas de Meli{\'a}ceas e {\'o}leo de nim por adultos de Diabrotica speciosa (Ger.) foram conduzidos no Laborat{\'o}rio de Entomologia da Embrapa Clima Temperado, Pelotas, Brasil. Discos foliares de plantas de batata, cultivadas em vaso, foram utilizados nos testes com e sem chance de escolha. No teste sem chance de escolha cada placa de Petri (90 mm) contendo um disco de determinado tratamento recebeu um casal de adultos, no teste com chance de escolha cada placa de Petri (140 mm) contendo um disco de cada tratamento recebeu tr{\^e}s casais de adultos. Utilizaram-se a mesma metodologia para os ensaios com {\'o}leo de nim. Os produtos a base de Trichilia claussenii C. DC. nas concentra{\c c}{\~o}es de 10 e 20\% obtiveram as menores {\'a}reas consumidas no teste sem chance de escolha. No ensaio com chance de escolha as menores {\'a}reas consumidas foram nos tratamento com T. claussenii e Cedrela fissilis L. No ensaio sem chance de escolha com {\'o}leo de nim, nenhum tratamento se diferenciou da testemunha com {\'a}gua. No ensaio com chance de escolha todos os tratamentos obtiveram valores de consumo menores do que a testemunha.}
}

@misc{orgprints24423,
           title = {Sustentabilidade ambiental de um sistema agroflorestal com bracatinga (Mimosa scabrella Bent.) atrav{\'e}s da analise emerg{\'e}tica.},
          author = {Maria da Penha Moreira Gon{\c c}alves and Ant{\^o}nio Rioyei Higa and Ivan Crespo Silva and Luciana Duque Silva and Rafael Kuster Oliveira},
            year = {2013},
         journal = {Revista Brasileira de Agroecologia},
          number = {1},
          volume = {8},
           pages = {18--27},
        keywords = {Sistema de produ{\c c}{\~a}o, Recursos naturais, Avalia{\c c}{\~a}o ambiental.
Production system, Natural resources, Environmental assessment.},
        abstract = {O sistema agroflorestal (SAF) com bracatinga (Mimosa scabrella Bent.) constitui-se em alternativa de renda e manuten{\c c}{\~a}o da qualidade ambiental, contribuindo para a fixa{\c c}{\~a}o do homem no campo. A an{\'a}lise emerg{\'e}tica permite a avalia{\c c}{\~a}o ambiental de um sistema sendo levadas em considera{\c c}{\~a}o todas {\`a}s contribui{\c c}{\~o}es da natureza na sua constru{\c c}{\~a}o. O presente estudo objetivou avaliar o desempenho ambiental de um SAF com bracatinga atrav{\'e}s do m{\'e}todo de an{\'a}lise emerg{\'e}tica. O SAF foi formado pela esp{\'e}cie florestal bracatinga e pelas esp{\'e}cies agr{\'i}colas milho e ab{\'o}bora. O estudo demonstrou que os recursos naturais renov{\'a}veis exercem grande participa{\c c}{\~a}o nos fluxos emerg{\'e}ticos do sistema, configurando ser o maior contribuidor energ{\'e}tico na constru{\c c}{\~a}o do SAF de bracatinga. Dentro dos recursos da economia, a m{\~a}o-de-obra familiar foi o servi{\c c}o mais demandante pelo SAF, mostrando ser esse um sistema eficaz na fixa{\c c}{\~a}o do homem no campo. O SAF com bracatinga nas condi{\c c}{\~o}es estudadas pode ser caracterizado como sustent{\'a}vel em m{\'e}dio prazo, mas fazem-se necess{\'a}rios ajustes no seu atual modelo produtivo para que possa adquirir sustentabilidade em longo prazo.},
             url = {https://orgprints.org/id/eprint/24423/}
}

