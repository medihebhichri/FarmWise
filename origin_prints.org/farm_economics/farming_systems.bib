         journal = {Revista Brasileira de Agroecologia},
           title = {Qualidade de compostos org{\^a}nicos preparados com diferentes propor{\c c}{\~o}es de ramos de gliric{\'i}dia (Gliricidia sepium).},
           pages = {187--198},
          volume = {8},
          number = {1},
        abstract = {O objetivo do presente trabalho foi avaliar a qualidade de compostos org{\^a}nicos produzidos com diferentes propor{\c c}{\~o}es de ramos triturados de gliric{\'i}dia e capim elefante. O delineamento experimental utilizado foi inteiramente casualizado, com tr{\^e}s repeti{\c c}{\~o}es. Foram avaliados cinco tratamentos, definidos por cinco propor{\c c}{\~o}es de biomassa de gliric{\'i}dia, misturadas com capim elefante (10, 20, 30, 40 e 50\%). Foram avaliados: carbono org{\^a}nico, rela{\c c}{\~a}o C/N, pH, e ac{\'u}mulo de nutrientes N, P, K, Ca, Mg e S. O aumento das propor{\c c}{\~o}es de gliric{\'i}dia aumentou de forma linear o teor de mat{\'e}ria org{\^a}nica, nitrog{\^e}nio, enxofre, c{\'a}lcio e magn{\'e}sio. Entretanto, houve redu{\c c}{\~a}o linear do pH, possivelmente devido ao favorecimento do processo de nitrifica{\c c}{\~a}o que libera H+ no meio. Os teores de P e K n{\~a}o foram alterados pelas propor{\c c}{\~o}es de misturas de gliric{\'i}dia e capim elefante. O estudo indica que a gliric{\'i}dia {\'e} uma alternativa para substituir ou reduzir a utiliza{\c c}{\~a}o do esterco na compostagem org{\^a}nica.},
        keywords = {Biomassa vegetal, Leguminosa, Macronutrientes, Mat{\'e}ria org{\^a}nica, Rela{\c c}{\~a}o C/N.
Vegetal biomass, Leguminous, Macronutrients, Organic matter, C/N ratio.},
             url = {https://orgprints.org/id/eprint/24474/}
}

