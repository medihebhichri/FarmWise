@unpublished{orgprints42176,
           title = {Nurse Cows For Young Calves: Analysis Of A Bottom-Up Innovation In Organic Dairy Farming},
          author = {Florence Hellec and Mathilde Belluz and Nathalie Bareille},
            year = {2021},
             url = {https://orgprints.org/id/eprint/42176/},
        keywords = {BOTTOM-UP DEVELOPMENT, cow-calf rearing, dairy production, qualitative study, rearing practices},
        abstract = {Technical groups of dairy farmers have developed autonomous and economical farming systems using bottom-up innovation processes. Nurse cow rearing, a new way to rear young calves, was designed within such groups. The aim of our research was to analyse how organic dairy farms implement these rearing systems. We conducted qualitative interviews with farmers to explore relations between rearing practices, farmers? objectives and farm characteristics. Our results indicate that this innovation is based on a few elements: (1) it is inseparable from a global rearing system based on pasture, (2) it fits with farmers? production and economic goals and (3) it can be adapted to each farm.}
}

