@mastersthesis{orgprints22752,
           title = {Sistemas de produ{\c c}{\~a}o org{\^a}nicos para a soca da cultura da cana-de-a{\c c}{\'u}car (Saccharum spp), consorciado com milho(Zea Mays), feij{\~a}o (Phaseolus Vulgaris) e mandioca (Manihot  Esculenta).},
