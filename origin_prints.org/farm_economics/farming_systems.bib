@misc{orgprints22546,
           title = {Agricultura apoiada pela comunidade: poderia a experi{\^e}ncia dos agricultores americanos ser {\'u}til para os agricultores urbanos brasileiros?},
         journal = {Horticultura Brasileira},
          author = {Marina Castelo  Branco and Ronaldo S de  Liz and Fl{\'a}via A  Alc{\^a}ntara and H{\'e}lio AG  Martins and James C  Hanson},
            year = {2011},
          number = {1},
             doi = {http://dx.doi.org/10.1590/S0102-05362011000100008  },
           pages = {43--49},
          volume = {29},
           month = {March},
        abstract = {O projeto "Agricultura Apoiada pela Comunidade" (AAC) {\'e} uma alternativa de mercado utilizada na Europa e nos EUA. Neste mercado alternativo, os produtores oferecem a sua produ{\c c}{\~a}o aos consumidores, sob a forma de cestas de alimentos, alguns meses antes de iniciarem a produ{\c c}{\~a}o de suas culturas. Algum tempo depois, durante alguns meses, os consumidores ir{\~a}o receber os produtos. Atualmente, diversos pa{\'i}ses t{\^e}m uma parcela significativa da sua popula{\c c}{\~a}o vivendo na pobreza. Por isso, diversas pol{\'i}ticas p{\'u}blicas foram desenhadas nesses pa{\'i}ses com o objetivo de reduzi-la. O est{\'i}mulo {\`a} agricultura urbana {\'e} uma dessas pol{\'i}ticas. No entanto, frequentemente, os produtores urbanos pobres t{\^e}m dificuldades para adquirir os insumos necess{\'a}rios para manter a sua produ{\c c}{\~a}o. O apoio cont{\'i}nuo do governo local associado ao apoio cont{\'i}nuo da comunidade local poderia ajudar esses produtores a manter o seu sistema de produ{\c c}{\~a}o. E, para esses agricultores urbanos pobres, o projeto AAC poderia ser um meio alternativo de organiza{\c c}{\~a}o. O objetivo desse trabalho foi obter algumas informa{\c c}{\~o}es sobre o projeto AAC nos EUA e avaliar se consumidores e produtores urbanos pobres de uma cidade brasileira de baixa renda aceitariam se engajar neste tipo de projeto. Os produtores americanos envolvidos no projeto AAC em geral cultivavam uma {\'a}rea inferior a 2 ha, ofereciam diferentes tipos de hortali{\c c}as aos consumidores por pelo menos quarto meses e geralmente usavam pr{\'a}ticas de produ{\c c}{\~a}o org{\^a}nica. No entanto, havia dificuldades para a manuten{\c c}{\~a}o do projeto, sendo o planejamento da produ{\c c}{\~a}o e a dificuldade de sua manuten{\c c}{\~a}o ao longo do tempo os maiores problemas. Alguns consumidores da cidade brasileira pesquisada se mostraram dispostos a participar deste tipo de projeto principalmente pela possibilidade de receberem alimentos frescos e produzidos organicamente. No entanto, o mesmo n{\~a}o ocorreu com os produtores urbanos pobres da cidade. E a raz{\~a}o principal para isso foi a impossibilidade deles garantirem a produ{\c c}{\~a}o. Pesquisas posteriores dever{\~a}o avaliar se a melhoria do sistema de produ{\c c}{\~a}o poderia levar esses agricultores a se engajarem neste tipo de projeto.},
        keywords = {Agricultura urbana, Redu{\c c}{\~a}o da pobreza, Organiza{\c c}{\~a}o alternativa.
Urban agriculture, Poverty reduction, Alternative organization.},
             url = {https://orgprints.org/id/eprint/22546/}
}

