           pages = {135--143},
          editor = {Erik Steen Kristensen and Stig Milan Thamsborg},
             url = {https://orgprints.org/id/eprint/13755/},
        abstract = {Den {\o}kologiske driftsform udspringer af en tankegang. Til denne tankegang knytter sig et s{\ae}t af overordnede intentioner og motiver. De overordnede tanker og m{\r a}l med {\o}kologisk drift er beskrevet i kapitel 1 i n{\ae}rv{\ae}rende rapport. Et af disse m{\r a}l er "at sikre alle husdyr gode forhold, der er i overensstemmelse med deres naturlige adf{\ae}rd og behov", og derved sikre en god dyrevelf{\ae}rd. Dette m{\r a}l har ikke en s{\ae}rlig fremtr{\ae}dende plads i de overordnede m{\r a}ls{\ae}tninger, og begrebet dyrevelf{\ae}rd er tilsyneladende ikke t{\ae}nkt tilstr{\ae}kkeligt igennem specifikt i forhold til den {\o}kologiske driftsform. I kapitel 9 er det beskrevet, hvorvidt husdyrvelf{\ae}rd i {\o}kologisk jordbrug er noget s{\ae}rligt. I n{\ae}rv{\ae}rende kapitel gives f{\o}rst en opsummering af, dels interviewene i kapitel 2 og 3, dels eksperternes beskrivelse af temaerne i kapitel 4-7. I forl{\ae}ngelse heraf gives et bud p{\r a} opfattelsen af dyrevelf{\ae}rd {\o}kologisk jordbrug og, der gives forslag til forbedringer omkring sundhed, velf{\ae}rd og medicinanvendelse inden for regulering, r{\r a}dgivning, forskning og udvikling.}
}

@unpublished{orgprints42173,
          author = {Mette Vaarst and Stephen Roderick and Guillaume Martin and Stefan Gunnarson and Anet Spengler Neff and Anne Grete Kongsted and Anna Bieber},
            year = {2021},
           title = {Strategies And Visions For The Future Of Organic Animal Farming},
        keywords = {Animal resilience, Breed diversity, home-grown protein feed, mother-infant contact, Multi-species farming, Sustainable foraging, Abacus, FiBL50090, FiBL50057},
        abstract = {Based on existing knowledge and current research projects, a number of innovative strategies and approaches are proposed that would enable organic animal farming to contribute to a more sustainable and resilient food systems that also impacts positively on our surrounding socio-ecological landscape. The focus is on 1) integrated, multispecies, diversified systems, 2) sustainable foraging strategies and efficient utilisation of natural resources, 3) home grown protein feed especially for monogastric animals, 4) resilience as the core of health development rather than just freedom of diseases, 5) breeding including multipurpose breeds, and 6) enhanced mother-infant contact.},
             url = {https://orgprints.org/id/eprint/42173/}
}

@misc{orgprints22641,
