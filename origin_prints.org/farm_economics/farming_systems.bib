@misc{orgprints24208,
          number = {2},
          volume = {12},
           pages = {89--114},
           month = {December},
           title = {Recomposi{\c c}{\~a}o socioambiental dos espa{\c c}os rurais e agricultura familiar na regi{\~a}o metropolitana de Curitiba.},
         journal = {Novos Cadernos NAEA},
            year = {2009},
          author = {Alfio Brandenburg and Cimone Rozendo de  Souza},
             url = {https://orgprints.org/id/eprint/24208/},
        abstract = {Esse texto analisa o processo de recomposi{\c c}{\~a}o dos espa{\c c}os rurais, considerando as pol{\'i}ticas ambientais e os diversos atores no meio rural em especial os agricultores familiares. A pesquisa foi realizada em dois universos diferenciados, localizados na Regi{\~a}o Metropolitana de Curitiba: comunidade de Santo Amaro, formada por agricultores modernizados e a comunidade de Postinho, formada por agricultores que vivem em um cen{\'a}rio de precariedade social. Levando em conta que a ideia de patrim{\^o}nio familiar faz parte da l{\'o}gica organizacional da agricultura familiar, constatou-se que em ambos os casos ocorre um processo de repatrimonializa{\c c}{\~a}o de parte dos recursos naturais. Na pr{\'a}tica, verificou-se que as pol{\'i}ticas de conserva{\c c}{\~a}o da natureza atuam na forma{\c c}{\~a}o de um patrim{\^o}nio natural comum, com fun{\c c}{\~o}es sociais, mas permitem que se desenvolvam pr{\'a}ticas sociais e produtivas que comprometem a sustentabilidade socioambiental dos agricultores. Nesse sentido, o espa{\c c}o se redefine muito mais segundo uma l{\'o}gica t{\'e}cnico-instrumental do que de acordo com um estilo socioambiental de desenvolvimento.},
        keywords = {Recomposi{\c c}{\~a}o socioambiental, Agricultura familiar, Meio ambiente, Pol{\'i}ticas ambientais.
Socio-environmental recomposition, Family farmers, Eenvironment, Environmental policies.}
}

