           month = {September},
             url = {https://orgprints.org/id/eprint/27603/},
        abstract = {Este trabalho foi realizado atrav{\'e}s de uma revis{\~a}o bibliogr{\'a}fica a respeito da aduba{\c c}{\~a}o verde e os benef{\'i}cios proporcionados com a ado{\c c}{\~a}o dessa pr{\'a}tica, com o objetivo de divulgar os principais resultados cient{\'i}ficos nessa {\'a}rea. Verificou-se que a aduba{\c c}{\~a}o verde proporciona melhorias significativas nas caracter{\'i}sticas qu{\'i}micas, f{\'i}sicas e biol{\'o}gicas do solo. As caracter{\'i}sticas qu{\'i}micas s{\~a}o beneficiadas pela libera{\c c}{\~a}o de nutrientes com a decomposi{\c c}{\~a}o dos res{\'i}duos. Essa pr{\'a}tica melhora as caracter{\'i}sticas f{\'i}sicas favorecendo a agrega{\c c}{\~a}o do solo, maior reten{\c c}{\~a}o de {\'a}gua e prevenindo a eros{\~a}o. A biomassa dos adubos verdes melhora as caracter{\'i}sticas biol{\'o}gicas do solo constituindo em alimento para esses microrganismos. Esp{\'e}cies utilizadas como adubos verdes s{\~a}o eficientes na fixa{\c c}{\~a}o biol{\'o}gica do nitrog{\^e}nio e podem efetuar supress{\~a}o de plantas espont{\^a}neas. Dessa forma, amaduba{\c c}{\~a}o verde {\'e} uma alternativa para diminuir o aporte de energia externa {\^a} propriedade rural, aumentando a sustentabilidade do sistema.},
        keywords = {Aduba{\c c}{\~a}o Verde, Agroecologia, Solo, Sustentabilidade.
Green manures, Agroecology, Soil, Sustainability}
}

@misc{orgprints27726,
          number = {3},
           month = {August},
           pages = {20--40},
          volume = {2},
           title = {Percep{\c c}{\~a}o agroambiental dos agricultores familiares no sudoeste do Paran{\'a}: o caso de Capanema.},
            year = {2007},
          author = {Humberto Miranda do Nascimento},
         journal = {Revista Brasileira de Agroecologia},
        abstract = {At{\'e} que ponto um maior discernimento dos agricultores familiares em rela{\c c}{\~a}o a quest{\~o}es ambientais contribui de fato para reduzir as vulnerabilidades ambientais? A percep{\c c}{\~a}o agroambiental do agricultor familiar acerca do recrudescimento do processo degrada{\c c}{\~a}o dos solos revela que, por causa do forte incentivo {\`a}s culturas de exporta{\c c}{\~a}o, foi gerada uma contradi{\c c}{\~a}o entre a necessidade de divisas para o pa{\'i}s e a sustentabilidade da agricultura familiar. O objetivo do artigo {\'e} analisar a percep{\c c}{\~a}o agroambiental do agricultor familiar por meio do estudo de caso do munic{\'i}pio de Capanema, vizinho ao Parque Nacional do Igua{\c c}u, situado na Regi{\~a}o Sudoeste do Estado do Paran{\'a}. Como metodologia, realizamos um Diagn{\'o}stico Agroambiental para captar a percep{\c c}{\~a}o de trinta agricultores familiares, a fim de verificar o n{\'i}vel de ado{\c c}{\~a}o de pr{\'a}ticas conservacionistas pelos agricultores em face das vulnerabilidades ambientais decorrentes do car{\'a}ter capital- intensivo do padr{\~a}o agr{\'i}cola. Como conclus{\~a}o, o estudo de caso nos permite afirmar que, mesmo numa regi{\~a}o com estrutura agr{\'a}ria mais democr{\'a}tica, as vulnerabilidades ambientais persistem porque as estrat{\'e}gias de reprodu{\c c}{\~a}o da agricultura familiar ainda est{\~a}o estreitamente associadas ao desempenho do mercado exportador de gr{\~a}os.},
        keywords = {Percep{\c c}{\~a}o agroambiental, agricultura familiar, vulnerabilidade ambiental.
Agro-environmental perception, family agriculture, environmental vulnerability.},
             url = {https://orgprints.org/id/eprint/27726/}
}

@misc{orgprints22705,
