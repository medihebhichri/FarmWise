@misc{orgprints26636,
           title = {Ado{\c c}{\~a}o de pr{\'a}ticas de manejo da agrobiodiversidade e estrat{\'e}gias de diversifica{\c c}{\~a}o dos meios de vida das comunidades rurais em Piren{\'o}polis - Goi{\'a}s.},
