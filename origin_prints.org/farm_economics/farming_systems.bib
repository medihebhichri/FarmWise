@book{orgprints21052,
          editor = {Borivoj Sarapatka},
         address = {Jiho{\v c}esk{\'a} univerzita v {\v C}esk{\'y}ch Bud{\v e}jovic{\'i}ch, Ekonomick{\'a} fakulta, edi{\v c}n{\'i} st{\v r}edisko},
            year = {2007},
          author = {Jan (Jr.) Moudry and Jan Moudry and Roman Rozsypal},
       publisher = {Jiho{\v c}esk{\'a} univerzita v {\v C}esk{\'y}ch Bud{\v e}jovic{\'i}ch, Zem{\v e}d{\v e}lsk{\'a} fakulta},
           title = {Anal{\'y}za ekologick{\'e}ho hospoda{\v r}en{\'i} na orn{\'e} p{\r u}d{\v e}},
        keywords = {ekologick{\'e} zem{\v e}d{\v e}lstv{\'i}, struktura, orn{\'a} p{\r u}da},
        abstract = {Studie je zam{\v e}{\v r}ena na posouzen{\'i} struktury ekologicky hospoda{\v r}{\'i}c{\'i}ch podnik{\r u} a na indikaci faktor{\r u} ovliv{\v n}uj{\'i}c{\'i}ch rozvoj ekologick{\'e}ho zem{\v e}d{\v e}lstv{\'i} na orn{\'e} p{\r u}d{\v e}. Anal{\'y}za struktury zem{\v e}d{\v e}lsk{\'y}ch podnik{\r u} byla provedena na dvou testovac{\'i}ch souborech. Bylo prok{\'a}z{\'a}no, {\v z}e s p{\v r}ib{\'y}vaj{\'i}c{\'i} nadmo{\v r}skou v{\'y}{\v s}kou roste extenzita hospoda{\v r}en{\'i}, v{\'y}robn{\'i} struktura podniku se zjednodu{\v s}uje (zatravn{\v e}n{\'i} v kombinaci s chovem skotu BTPM). P{\v r}i konverzi na ekologick{\'e} zem{\v e}d{\v e}lstv{\'i} p{\v r}istupuj{\'i} a{\v z} ke stoprocentn{\'i}mu zatravn{\v e}n{\'i}, co{\v z} vyvol{\'a}v{\'a} utlumen{\'i} produk{\v c}n{\'i}ch zem{\v e}d{\v e}lsk{\'y}ch funkc{\'i}. S rostouc{\'i} nadmo{\v r}skou v{\'y}{\v s}kou kles{\'a} pod{\'i}l orn{\'e} p{\r u}dy. V nedostate{\v c}n{\'e} m{\'i}{\v r}e jsou zastoupeny zlep{\v s}uj{\'i}c{\'i} plodiny (brambory, p{\'i}cniny na orn{\'e} p{\r u}d{\v e} a luskoviny). Existence {\v r}ady ekologicky hospoda{\v r}{\'i}c{\'i}ch podnik{\r u} z{\'a}vis{\'i} na dotac{\'i}ch tvo{\v r}{\'i}c{\'i}ch hlavn{\'i} p{\v r}{\'i}jem. Ekologick{\'e} zem{\v e}d{\v e}lstv{\'i} je dislokov{\'a}no p{\v r}edev{\v s}{\'i}m do horsk{\'y}ch a podhorsk{\'y}ch oblast{\'i}, kde relativn{\v e} dob{\v r}e pln{\'i} mimoproduk{\v c}n{\'i} environment{\'a}ln{\'i} funkci, zat{\'i}mco produk{\v c}n{\'i} funkce je pln{\v e}na naprosto nedostate{\v c}n{\v e}. Popt{\'a}vka po biopotravin{\'a}ch, a{\v c} dosud n{\'i}zk{\'a}, ani mo{\v z}nosti exportu, nejsou pokryty dom{\'a}c{\'i} produkc{\'i}. P{\v r}echod k multifunkcionalit{\v e}, vy{\v z}aduj{\'i}c{\'i} vysok{\'y} stupe{\v n} diverzifikace, je nedostate{\v c}n{\'y}. M{\'a}-li doj{\'i}t k roz{\v s}{\'i}{\v r}en{\'i} ekologick{\'e}ho zem{\v e}d{\v e}lstv{\'i} na orn{\'e} p{\r u}d{\v e}, je nezbytn{\'e} p{\v r}istoupit p{\v r}edev{\v s}{\'i}m k p{\v r}eklasifikov{\'a}n{\'i} podpor produk{\v c}n{\'i}ch a mimoproduk{\v c}n{\'i}ch funkc{\'i} ekologick{\'e}ho zem{\v e}d{\v e}lstv{\'i}. Druh{\'y}m limituj{\'i}c{\'i}m faktorem jsou nedostate{\v c}n{\'e} zpracovatelsk{\'e} kapacity pro bioprodukci. Na dobr{\'e} {\'u}rovni nen{\'i} ani kreativita zem{\v e}d{\v e}lc{\r u} a poradensk{\'y} servis v oblasti produkce, zpracov{\'a}n{\'i} a marketingu. Je nutn{\'e} vyhodnotit efektivnost dotac{\'i} na TTP a ornou p{\r u}du, podpo{\v r}it trh s biopotravinami, zkvalitnit poradenstv{\'i} pro oblast prvov{\'y}roby a zpracov{\'a}n{\'i} bioprodukt{\r u} zejm{\'e}na p{\v r}{\'i}mo na ekofarm{\v e}, zajistit informace t{\'y}kaj{\'i}c{\'i} se cen, v{\'y}roby, zpracov{\'a}n{\'i}, spot{\v r}eby a obchodu v oblasti biopotravin, kone{\v c}n{\v e} podpo{\v r}it u zem{\v e}d{\v e}lc{\r u} schopnost kombinovat produk{\v c}n{\'i} a environment{\'a}ln{\'i} {\v c}innosti podle komparativn{\'i}ch v{\'y}hod podniku, stanovi{\v s}t{\v e}, {\v c}i regionu. Ekologick{\'e} zem{\v e}d{\v e}lstv{\'i} mus{\'i} b{\'y}t nad{\'a}le vzorem rozvoje multifunk{\v c}n{\'i}ho zem{\v e}d{\v e}lstv{\'i}, respektive hospoda{\v r}en{\'i} v krajin{\v e}.},
             url = {https://orgprints.org/id/eprint/21052/}
}

