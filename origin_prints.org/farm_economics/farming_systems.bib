         journal = {Revista Brasileira de Agroecologia},
           title = {Atributos f{\'i}sicos e microbiol{\'o}gicos de um latossolo vermelho-amarelo distr{\'o}fico t{\'i}pico sob cerrado nativo e monocultivo de soja.},
          volume = {6},
