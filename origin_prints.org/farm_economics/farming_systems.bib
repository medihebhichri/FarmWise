          author = {Fernando Goulart and John Vandermeer and Ivette Perfecto and Rodrigo P. Matta-Machado},
            year = {2009},
           title = {An{\'a}lise agroecol{\'o}gica de dois paradigmas modernos.},
          volume = {4},
