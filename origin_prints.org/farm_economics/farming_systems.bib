@misc{orgprints54654,
           title = {Bio? Nein Danke! {\ensuremath{|}} FiBL Focus},
            year = {2024},
          author = {Franziska H{\"a}mmerli and Anke Beermann and Tim Schmid and Hansueli Dierauer},
