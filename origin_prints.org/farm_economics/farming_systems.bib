@misc{orgprints23086,
           title = {Altera{\c c}{\~a}o da carga de endoparasitas em ovinos submetidos a diferentes n{\'i}veis de folha de bananeira na alimenta{\c c}{\~a}o.},
          author = {Carla Lieda Cezimbra  Parra and Clair Jorge  Olivo and Carlos Alberto  Agnolin and Fernanda Flores  Vogel and Cleber Cassol  Pires and Anderson Michel Soares  Bolzan},
