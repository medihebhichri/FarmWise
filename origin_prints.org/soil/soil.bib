@misc{orgprints29479,
           title = {Coverage mobilization by different no-tillage in-line handling mechanisms.},
            year = {2015},
          author = {Evandro M. BRANDELERO and Augusto G. de ARA{\'U}JO and Ricardo RALISCH},
         journal = {Journal of the Brazilian Association of Agricultural Engineering},
