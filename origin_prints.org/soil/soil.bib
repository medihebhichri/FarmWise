@misc{orgprints34362,
       publisher = {Norsk senter for {\o}kologisk landbruk, NORS{\O}K},
           title = {Hvordan m{\r a}les innholdet av organisk materiale og karbon i norsk jord?},
          series = {NORS{\O}K FAGINFO},
         address = {Tingvoll},
          author = {Reidun Pommeresche and Randi Berland Fr{\o}seth and Hugh Riley},
            year = {2019},
           pages = {1--6},
        keywords = {Soil organic carbon, Soil organic matter, Organisk materiale, jordkarbon, karbon i jord, gl{\o}detap, Jordlappen, JordBio, HelJord, karbonlagring},
        abstract = {I Norge brukes oftest gl{\o}detap av t{\o}rr, siktet jord som m{\r a}l p{\r a} innhold av organisk materiale i dyrka jord. I mineraljord med {\o}kende innhold av leire, m{\r a} gl{\o}detapet korrigeres ned mer og mer for {\r a} finne det virkelige innholdet av organisk materiale i jorda. B{\r a}de organisk materiale, mold og humus inneholder foruten karbon ogs{\r a} flere andre grunnstoffer. Det er vanlig {\r a} beregne at organisk materiale i jord i snitt inneholder 50-58 \% karbon. 

Generelt gir gl{\o}detapet best m{\r a}l for innhold av organisk materiale i jordartsklassene mineralblandet moldjord og organisk jord og jordarter med lavt leirinnhold ({\ensuremath{<}} 9 \%). Det er ogs{\r a} der minst forskjell mellom gl{\o}detapet og moldinnhold (mer n{\o}ye innhold av organisk materiale). I slik jord er ogs{\r a} karboninnholdet n{\ae}rmest 50 \% av b{\r a}de gl{\o}detapsverdien og verdien for beregnet moldinnhold. 

I mineraljord med {\o}kende innhold av leire, vil gl{\o}detapet derimot m{\r a}tte korrigeres ned mer og mer for best mulig m{\r a}l for moldinnholdet (det reelle innholdet av organisk materiale) og i etterkant beregning av karboninnholdet. I slik jord blir det st{\o}rre forskjell mellom gl{\o}detapsverdien og moldinnholdet i analysene vi bruker. 

Det blir ogs{\r a} viktigere {\r a} beregne karboninnholdet mer n{\o}yaktig, og ikke dividere gl{\o}detapsverdien, men moldinnholdet med 2. Dette fordi leirinnholdet p{\r a}virker gl{\o}detapsverdien mye og gir i slik jord et langt lavere reelt innhold av organisk materiale enn gl{\o}detapsverdien viser. I lettleire og mellomleire er derfor ofte det reelle karboninnholdet veldig lavt, under 20 \% av gl{\o}detapstallverdien, og under 1 \% karbon av TS.},
             url = {https://orgprints.org/id/eprint/34362/}
}

