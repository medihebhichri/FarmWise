          author = {Adriana Fran{\c c}a  Figueira and Ricardo Luis Louro  Berbara and Jo{\~a}o Pedro  Pimentel},
            year = {2011},
           title = {Estrutura da popula{\c c}{\~a}o de nematoides do solo em uma unidade de produ{\c c}{\~a}o agroecol{\'o}gica no Estado do Rio de Janeiro, Brasil.},
           pages = {223--229},
          volume = {33},
           month = {April},
             doi = {http://dx.doi.org/10.4025/actasciagron.v33i2.4716  },
          number = {02},
             url = {https://orgprints.org/id/eprint/21915/},
        abstract = {Estudou-se a comunidade de nematoides do solo em quatro sistemas de manejo no Sistema Integrado de Produ{\c c}{\~a}o Agroecol{\'o}gica (SIPA): pasto, capoeira, figo com Paspalum notatum e horta. As amostras de solo foram coletadas a cada quatro meses durante um ano. A comunidade de nematoides foi identificada e descrita com os par{\^a}metros: riqueza de fam{\'i}lias, diversidade de Shannon-Weaner, de domin{\^a}ncia de Simpson, al{\'e}m da diversidade tr{\'o}fica e domin{\^a}ncia tr{\'o}fica.
Os fitoparasitas e bacteri{\'o}fagos dominaram as comunidades de nematoides nas {\'a}reas. Os
on{\'i}voros foram menos abundantes em todos os sistemas estudados. O sistema pasto apresentou
maior diversidade de fam{\'i}lias de nematoides enquanto que, no sistema capoeira se encontraram
valores de diversidade baixos, por{\'e}m constantes no tempo, indicando condi{\c c}{\~a}o de maior estabilidade. A a{\c c}{\~a}o antr{\'o}pica induzida pelo manejo modificou a distribui{\c c}{\~a}o tr{\'o}fica em benef{\'i}cio dos nematoides bacteri{\'o}fagos, normalmente associados {\`a} maior taxa de decomposi{\c c}{\~a}o da mat{\'e}ria org{\^a}nica. Nematoides fitoparasitas e bacteri{\'o}fagos foram dominantes nos sistemas estudados, o que sugere a influ{\^e}ncia de densidade radicular elevada no pasto e alta taxa de mineraliza{\c c}{\~a}o na horta. A distribui{\c c}{\~a}o relativa de grupos tr{\'o}ficos de nematoides do solo confirmou ser uma eficiente forma de se determinar o n{\'i}vel de perturba{\c c}{\~a}o de solos submetidos a distintos manejos.},
        keywords = {Microfauna, Sistemas de cultivo, Diversidade.
Microfauna, Crop system, Diversity.}
}

