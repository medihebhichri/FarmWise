@misc{orgprints31217,
           title = {Role of inoculation with multi-trait rhizobacteria
on strawberries under water deficit stress},
          author = {Ummugulsum Erdogan and Ramazan Cakmakci and Atafeh Varmazyar? and Metin Turan and Yasar Erdogan and Nurg{\"u}l K?t?r},
            year = {2016},
