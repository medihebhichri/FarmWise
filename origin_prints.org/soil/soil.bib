             doi = {DOI 10.1007/s11104-017-3249-2},
             url = {https://orgprints.org/id/eprint/31784/},
        abstract = {Background and aims While N2-fixation in diversified grasslands including forage legumes and non-legumes has been widely studied, N2-fixation in swards containing only forage legumes remains unclear. In this study, we investigated N2-fixation in pure stands and mixtures of three forage legumes. Methodology N2-fixation, dry matter (DM) and nitrogen (N) yields were quantified in a field experiment for red clover (Trifolium pratense L.), white clover (Trifolium repens L.) and lucerne (Medicago sativa L.) pure stands and mixtures using the isotope dilution method. Results All three forage legume species derived most (around 85\%) of their N from atmospheric N2-fixation (\%Ndfa). However, no positive effect of species diversity was found in any of the mixtures. Species composition of the forage legume mixtures affected the amount of N from N2-fixation by affecting DM production and N accumulation of the species, where the seasonal amount of N2-fixation ranged from 370 to 500 kg N ha?1; which was highest in the presence of red clover. Conclusions We found thatmixturesof thethree forage legumes were highly productive, but did not show positive advantages compared to the red clover pure stands in terms of DM, N yield and \%Ndfa.}
}

@misc{orgprints24977,
           title = {Monitoring land use and plant cover on an integrated agroecological production system through GIS.},
          author = {Jos{\'e} Eduardo Dias and Ant{\^o}nio Carlos de Souza Abboud and Jos{\'e} Guilherme Marinho  Guerra and Rodrigo dos Reis Salles and Vagner Gon{\c c}alves Ferreira and Jorge Xavier da Silva},
            year = {2010},
         journal = {Revista Brasileira de Agroecologia},
          number = {2},
           month = {November},
           pages = {275--287},
          volume = {5},
             url = {https://orgprints.org/id/eprint/24977/},
        abstract = {The objective of this paper is to study in detail the land use and plant cover of an Integrated Agroecological Production System (IAPS) from 2003 through 2005. Four quarterly updating visits were performed on the 26 land units of the System from January 2003 to December 2005. Cartographic documents and QuickBird satellite images were also used to generate the final index maps for agrodiversity, fallow intensity and green manure use intensity. A high diversity of crops was observed. In some land units up to 40 plant species were recorded. However, this diversity was not uniformly distributed throughout the terrain. A high intensity of land use, mostly with annuals was also observed in a large part of the area. In most cases, fallow periods were up to 3 months in 3 years. Since annual crops demand intense tillage, minimum or no tillage practices are recommended for those areas to improve soil conservation. The use of legumes was less frequent on the land units used for annual crops. They were not uniformly distributed throughout the terrain. The results of this research are useful not only for those who are interested in the system itself, but also to validate the hypothesis that through GIS it is possible to summarize complex agroecological information into a visually friendly format, allowing easy interpretation of systemic analyses.
},
        keywords = {Agroecology, Land use, GIS- Image processing, Species richness, Fallow, Green manure.
Agroecologia, Uso da terra, Riqueza de esp{\'e}cies, Pousio agr{\'i}cola, Aduba{\c c}{\~a}o verde, Processamento de imagens-GIS.
}
}

@misc{orgprints22930,
           title = {Distribui{\c c}{\~a}o espacial de Procornitermes sp. (Isoptera: Termitidae) em fun{\c c}{\~a}o das propriedades f{\'i}sicas do solo em {\'a}rea de pastagem no munic{\'i}pio de S{\~a}o Borja, Rio Grande do Sul.},
            year = {2012},
          author = {Naym{\~a} Pinto  Dias and Leticia Ramon  Medeiros and Juliano de Bastos  Pazini and Fernando Felisberto da  Silva},
         journal = {Revista Brasileira de Agroecologia},
          number = {2},
          volume = {7},
           pages = {104--111},
             url = {https://orgprints.org/id/eprint/22930/},
        keywords = {Cupim-de-mont{\'i}culo, Variabilidade espacial, Qualidade f{\'i}sica do solo, Indicador ecol{\'o}gico, S{\~a}o Borja, RS.
Termite-the-mound, Spatial variability, Soil physical quality, Ecological indicator, S{\~a}o Borja, RS.},
        abstract = {Os cupins pertencentes ao g{\^e}nero Procornitermes s{\~a}o comumente encontrados em {\'a}reas de pastagem e a sua presen{\c c}a t{\^e}m sido associada a situa{\c c}{\~o}es de desequil{\'i}brio ambiental e qualidade f{\'i}sica do solo. Um conhecimento importante sobre a ocorr{\^e}ncia destes cupins {\'e} o da distribui{\c c}{\~a}o espacial, essencial para o desenvolvimento de planos de amostragens e previs{\~o}es de ocorr{\^e}ncia. O objetivo deste trabalho foi verificar a dispers{\~a}o e distribui{\c c}{\~a}o espacial de cupinzeiros em {\'a}rea de pastagem na Fronteira Oeste, associando-as {\`a}s propriedades f{\'i}sicas do solo. Foram efetuadas amostragens para a determina{\c c}{\~a}o do n{\'u}mero total de cupinzeiros existentes na {\'a}rea. Realizou-se an{\'a}lise f{\'i}sica do solo, a determina{\c c}{\~a}o do g{\^e}nero de cupim predominante e o c{\'a}lculo da distribui{\c c}{\~a}o das freq{\"u}{\^e}ncias e dos {\'i}ndices de dispers{\~a}o. Como {\'i}ndices de dispers{\~a}o utilizou-se a raz{\~a}o vari{\^a}ncia/m{\'e}dia e o {\'i}ndice de Morisita. Utilizaram-se as distribui{\c c}{\~o}es de Poisson, Binominal e Binominal Negativa para avaliar a distribui{\c c}{\~a}o espacial da esp{\'e}cie observada. A associa{\c c}{\~a}o entre os dados da composi{\c c}{\~a}o granulom{\'e}trica e da estabilidade de agregados com o n{\'u}mero de cupinzeiro foi determinada atrav{\'e}s de an{\'a}lises de regress{\~a}o linear simples e m{\'u}ltipla. Como resultado, os cupins apresentaram distribui{\c c}{\~a}o espacial do tipo agregada, com bom ajuste para distribui{\c c}{\~a}o Binominal Negativa. Os {\'i}ndices de dispers{\~a}o confirmaram a distribui{\c c}{\~a}o agregada. As propriedades f{\'i}sicas do solo, quando correlacionadas ao n{\'u}mero total de cupinzeiros por quadrante, obtiveram correla{\c c}{\~o}es com a quantidade de areia, silte e o di{\^a}metro m{\'e}dio geom{\'e}trico. Desta forma, os dados obtidos mostram que as propriedades f{\'i}sicas do solo determinam a dispers{\~a}o e a distribui{\c c}{\~a}o espacial dos cupinzeiros na {\'a}rea.}
}

