@misc{orgprints26726,
       publisher = {Bioforsk },
           title = {U{\o}nskete stoffer i husdyrgj{\o}dsel. 
Konvensjonell husdyrgj{\o}dsel brukt i {\o}kologisk drift - er det problematisk?},
          series = {Bioforsk Rapport},
         address = {Tingvoll},
            year = {2012},
          author = {Grete Lene Serikstad and Kirsty McKinnon and Trine Eggen},
           month = {December},
           pages = {1--56},
             url = {https://orgprints.org/id/eprint/26726/},
        keywords = {manure, pesticides, trace elements, veterinary medicines, contamination},
        abstract = {Within certain restrictions, conventional animal manure can be used in organic farming when the farm?s own resources do not cover the demand for plant nutrients.
This review came forward as a request from The Norwegian Food Safety Authority (NFSA) and The Advisory Committee for Organic Farming Regulations to gain more knowledge concerning potential contamination of manure by unwanted elements, such as residues of veterinary medicines, pesticides or high levels of heavy metals.
Both a literature research on the topic and contact with relevant research and advisory institutions in Europe, show that there is a need to investigate the topic further. There is a lack of relevant analyses of animal manure. Methods for analysing need to be developed; and more research is needed on the uptake of harmful elements in plants.
We recommend that a Nordic seminar is held to discuss and coordinate future research on the topic. We also suggest that The NFSA take an active part in the process due to probable changes in current regulations.}
}

