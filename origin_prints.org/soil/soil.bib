@misc{orgprints29096,
          volume = {38},
           pages = {252--261},
             doi = {http://dx.doi.org/10.1590/S0100-06832014000100025 },
          number = {1},
         journal = {Revista Brasileira de Ci{\^e}ncia do Solo},
            year = {2014},
          author = {Wander Luis Barbosa  Borges and Rog{\'e}rio Soares de  Freitas and Gustavo Pavan  Mateus and Marco Eust{\'a}quio de  S{\'a} and Marlene Cristina  Alves},
           title = {Absor{\c c}{\~a}o de nutrientes e altera{\c c}{\~o}es qu{\'i}micas em Latossolos cultivados com plantas de cobertura em rota{\c c}{\~a}o com soja e milho.},
             url = {https://orgprints.org/id/eprint/29096/},
        abstract = {Os efeitos promovidos pelas plantas de cobertura nos atributos qu{\'i}micos do solo s{\~a}o bastante vari{\'a}veis, dependendo de fatores como esp{\'e}cie utilizada, manejo dado {\`a} biomassa, {\'e}poca de plantio e corte das plantas, tempo de perman{\^e}ncia dos res{\'i}duos no solo, condi{\c c}{\~o}es locais e intera{\c c}{\~a}o entre esses fatores. Este trabalho foi realizado com o objetivo de avaliar a absor{\c c}{\~a}o de nutrientes por cinco plantas de cobertura, que foram utilizadas para produ{\c c}{\~a}o de gr{\~a}os, sementes e forragem, em diferentes quantidades de sementes por hectare e pela vegeta{\c c}{\~a}o espont{\^a}nea, bem como para o efeito sobre as propriedades qu{\'i}micas de dois Latossolos, cultivadas em rota{\c c}{\~a}o com as culturas da soja e do milho. Os experimentos foram instalados em Votuporanga, SP, e Selv{\'i}ria, MS, em mar{\c c}o de 2008, ap{\'o}s o preparo convencional do solo. O delineamento experimental utilizado foi o de blocos casualizados com quatro repeti{\c c}{\~o}es, utilizando as seguintes plantas de cobertura em diferentes quantidades de sementes por hectare que constitu{\'i}ram os tratamentos: sorgo gran{\'i}fero: 6, 7 e 8 kg ha-1; milheto: 10, 15 e 20 kg ha-1; capim-sud{\~a}o: 12, 15 e 18 kg ha-1; h{\'i}brido de sorgo com capim-sud{\~a}o: 8, 9 e 10 kg ha-1; e Urochloa ruziziensis: 8, 12 e 16 kg ha-1. Tamb{\'e}m se utilizou um tratamento-controle com vegeta{\c c}{\~a}o espont{\^a}nea. Ap{\'o}s o manejo das coberturas, no primeiro ano de estudo, foi semeada a soja e, no segundo ano, semeou-se o milho, ambos em sistema de semeadura direta. Avaliaram-se a produtividade de mat{\'e}ria seca das diferentes coberturas, a absor{\c c}{\~a}o de nutrientes pelas coberturas e as altera{\c c}{\~o}es qu{\'i}micas no solo. Constatou-se que em solos argilosos, com elevado teor de alum{\'i}nio no solo, como em Selv{\'i}ria, o capim-sud{\~a}o, com 18 kg ha-1 de sementes, e o sorgo gran{\'i}fero, com 6 kg ha-1, em associa{\c c}{\~a}o a calagem, contribuem para redu{\c c}{\~a}o do teor de alum{\'i}nio e da acidez potencial e para eleva{\c c}{\~a}o da satura{\c c}{\~a}o por bases. As diferentes quantidades de sementes de cada planta de cobertura n{\~a}o influenciaram a produtividade de mat{\'e}ria seca da mesma planta de cobertura, mas interferiram na absor{\c c}{\~a}o de nitrog{\^e}nio do h{\'i}brido de sorgo com capim-sud{\~a}o, com 10 kg ha-1 de sementes, apresentando menor absor{\c c}{\~a}o que com 8 kg ha-1 de sementes, e tamb{\'e}m no teor de mat{\'e}ria org{\^a}nica no solo, com o capim-sud{\~a}o, com 15 kg ha-1 de sementes, propiciando maior teor que o de 18 kg ha-1 de sementes.},
        keywords = {Glycine max, Zea mays, Nutrient cycling, Sustainable soil management.

Glycine Max, Zea mays, Ciclagem de nutrientes, Manejo sustent{\'a}vel do solo.}
}

@misc{orgprints32610,
          series = {OK-Net Arable Practice abstract},
            year = {2018},
          author = {Stefano Bortolussi},
           title = {Cover crop mulches and no-till maize (OK-Net Arable Practice abstract)},
           month = {January},
           pages = {1--2},
             url = {https://orgprints.org/id/eprint/32610/},
        keywords = {cover plants, reduced tillage, soil fertility, soil quality, weed management, minimum tillage},
        abstract = {In a Northern Italian context, a field pea cover crop was tested as mulch for the maize crop. The roller crimper effectively terminated the cover crop (15 t per ha of wet biomass). However, the mulch did not last long enough, probably due to a narrow C/N-ratio and biologically active soil. The maize plants did not have enough time to develop and shade the entire soil surface. Weeds started to sprout through the decomposing mulch. For the test conditions, this cover crop has proven unsuitable as a long-lasting mulch to control weeds. No fertilization and no irrigation were applied. The maize yields amounted to about 5 t per ha.

Practical recommendations
? The cover crop needs to be sown as accurately as the main crop. Poor cover crop stands do not result in good mulches.
? The effectiveness of the mulch depends on the amount of mulch biomass. However, more biomass also means more difficulty for the planter.
? On soils with low organic matter content, additional nitrogen fertilisation should be considered.}
}

