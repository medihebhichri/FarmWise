@unpublished{orgprints24425,
           month = {October},
            year = {2013},
          author = {Egon Noe and Hugo Fjelsted Alr{\o}e},
           title = {Hvordan kan vi vide, om {\o}kologi bliver bedre? Workshop for Organic RDD projekterne m.fl.},
             url = {https://orgprints.org/id/eprint/24425/},
        abstract = {Der er nu over 50 tilmeldte deltagere til workshoppen fra forskellige steder i det {\o}kologiske vidensnetv{\ae}rk, og det lover rigtig godt for forl{\o}bet. Alle de tilmeldte arbejder for at udvikle {\o}kologien i en bedre retning, og i denne workshop stiller vi sp{\o}rgsm{\r a}let: hvordan vi kan vide, om {\o}kologien bliver bedre. Det kan v{\ae}re sv{\ae}rt at gennemskue for den enkelte, om det vi hver is{\ae}r arbejder med og arbejder for bidrager til, at {\o}kologien samlet set udvikler sig til det bedre i praksis. 
P{\r a} workshoppen skal vi i f{\ae}llesskab reflektere over, hvad sp{\o}rgsm{\r a}let, som er overskrift for workshoppen, betyder, og vi skal gerne n{\r a} frem til noget, vi hver is{\ae}r vil g{\o}re anderledes fremover - efter workshoppen. En foruds{\ae}tning for at vi kan n{\r a} til en s{\r a}dan afklaring er, at I som deltagere g{\o}r tre ting: 
?	Inden workshoppen forbereder jer ved at overveje de tre sp{\o}rgsm{\r a}l i den hjemmeopgave, I f{\r a}r stillet (se nedenfor)
?	P{\r a} workshoppen reflekterer over sp{\o}rgsm{\r a}let i en gruppe der s{\r a} vidt muligt h{\o}rer til samme sted i vidensnetv{\ae}rket (fx forsker, beslutningstager, r{\r a}dgiver/praktiker)
?	Bringer disse refleksioner ind i grupper der g{\r a}r p{\r a} tv{\ae}rs, i en f{\ae}lles diskussion af hvordan den enkeltes vurderinger kan bidrage til en samlet vurdering af om {\o}kologi bliver bedre, og hvordan vurderinger kan f{\o}re til {\ae}ndringer i praksis.
Hjemmeopgaven best{\r a}r i, at I forbereder jer ved at reflektere over f{\o}lgende tre sp{\o}rgsm{\r a}l ud fra jeres projekt eller ud fra jeres arbejde med {\o}kologiske forsknings- og udviklingsprojekter eller brug af resultater fra dem. Det er vigtigt for jeres eget og det samlede udbytte, at I har t{\ae}nkt over dem inden workshoppen. I har forskellige roller i {\o}kologien og forskellige vinkler p{\r a}, hvordan vi frembringer og bruger viden om, hvordan {\o}kologien kan blive bedre, og det er denne forskellighed, der vil give workshoppen v{\ae}rdi. 
Sp{\o}rgsm{\r a}l:
1.        Hvordan ved jeg, om min tilgang til {\o}kologiens udvikling bidrager til, at {\o}kologi bliver bedre? (Din tilgang kan fx v{\ae}re forskningsperspektivet i dit projekt, eller synsvinklen bag dine beslutninger om finansiering af projekter, eller dine forskningsbaserede beslutninger om tiltag i {\o}kologisk praksis).
2.        Hvilke v{\ae}rdier ligger der til grund for mine vurderinger af, om {\o}kologi bliver bedre?
3.        Hvordan bringer jeg mine vurderinger ind i en samlet vurdering af, om {\o}kologi bliver bedre?

Workshoppen kommer til at foreg{\r a} p{\r a} Hotel Opus i Horsens fra kl. 9:30 til 15:30.

Med venlig hilsen
Ilse A. Rasmussen
ICROFS}
}

@misc{orgprints24458,
           title = {Hvordan kan vi vide om {\o}kologi er godt? Offentlige foredrag i naturvidenskab, S{\o}auditorierne, Aarhus Universitet, 9., 10. og 11. april 2013},
            year = {2013},
           month = {April},
          author = {Egon Noe and Hugo Fjelsted Alr{\o}e},
             url = {https://orgprints.org/id/eprint/24458/},
        abstract = {Man st{\r a}r i supermarkedet foran k{\o}ledisken og r{\ae}kker h{\r a}nden ned for at gribe en {\o}kologisk kylling. Ud af {\o}jenkrogen ser man de p{\ae}nt emballerede kyllinger fra Danpo til under den halve pris. Tvivlen overv{\ae}lder en. Hvordan kan man nu ogs{\r a} vide om det er s{\r a} meget bedre at k{\o}be den {\o}kologiske kylling frem for den langt billigere almindelige kylling. Nogle siger at {\o}kologiske kyllinger smager bedre fordi de har bev{\ae}get sig mere og vokser langsomme. Ja, men p{\r a} den anden side er der jo ogs{\r a} nogle der p{\r a}st{\r a}r at {\o}kologien bidrager mere til den globale opvarmning fordi det koster mere foder at opfede en {\o}kologisk kylling. Jo, men de {\o}kologiske kyllinger har det bedre fordi de er opvokset under mere naturlige forhold. Det er m{\r a}ske meget godt men der er ogs{\r a} flere {\o}kologiske kyllinger der d{\o}r p{\r a} grund af rovdyr, og s{\r a}dan farer argumenterne rundt i hovedet p{\r a} en. Til sidst griber man desperat en tilf{\ae}ldig kylling og farer ud af supermarkedet.
Det er den samme uoverskuelige grad af kompleksitet som naturvidenskaben st{\r a}r overfor fx n{\r a}r den skal fors{\o}ge at bed{\o}mme om {\o}kologi er godt eller d{\r a}rligt. Ser man det ud fra {\'e}t videnskabeligt perspektiv, giver det et billede af hvad der er godt og ser man det ud fra et andet f{\r a}r man et helt andet billede. I et st{\o}rre forskningsprojekt ? kaldet MultiTrust ? om helhedsvurdering af {\o}kologisk jordbrug ved institut for Agro{\o}kologi, arbejder forskerne, med fokus p{\r a} {\o}kologisk jordbrug, p{\r a} hvordan man i en videnskabelig sammenh{\ae}ng kan h{\r a}ndtere de mange forskellige vinkler og perspektiver som naturvidenskaben giver mulighed for at anl{\ae}gge. Visualisering er et meget vigtigt redskab for at kunne h{\r a}ndtere de mange synsvinkler og formidle kompleksiteten, og derfor er videnskabsanimatorerne Peter Smith og Andreas Lykke med i projektet.
I foredraget inviterer vi dig som tilh{\o}rer med ind i de naturvidenskabelige diskussioner, og undervejs vil du blive bedt om aktivt at tage stilling til en r{\ae}kke dilemmaer og sp{\o}rgsm{\r a}l ved hj{\ae}lp af den lille tr{\r a}dl{\o}se afstemningssender du f{\r a}r udleveret.}
}

@misc{orgprints25416,
             doi = {10.1111/soru.12078},
          volume = {Early},
           pages = { View-- },
           title = {Knowledge Asymmetries Between Research and Practice: A Social Systems Approach to Implementation Barriers in Organic Arable Farming},
         journal = {Sociologia Ruralis},
          author = {Egon Noe and Hugo Fjelsted Alr{\o}e and Martin H. Thors{\o}e and Peter S{\o}rensen and Bo Melander and J{\o}rgen E. Olesen and Erik Fog},
            year = {2015},
        abstract = {Research in organic cropping systems has identified basic principles for increasing the robustness and production level of organic arable systems. But even though farmers know these principles, they have not implemented them in practice. This paper aims to improve our understanding of this ?knowledge gap? using the theory of knowledge asymmetries, and discusses the implications for research, extension and practice. The paper is based on interviews with 10 organic arable farms focused on how to understand the barriers from the perspective and context of the farming system. The findings show that some barriers are due to lack of knowledge of suggested actions, calling for more ordinary dissemination. But this cannot at all explain all the barriers. The farming systems face paradoxes of fragmented knowledge; if they try to improve one problem, this creates other problems. More scientific knowledge produces more paradoxes, and the farming systems lack tools to handle this. Systems research is meant to counteract this fragmentation, but has difficulties dealing with the plurality of practical contexts. And science has to deal with the dilemma of being superior and minor to practice at the same time. A perspectival understanding of science could be a step in that direction. },
             url = {https://orgprints.org/id/eprint/25416/}
}

@misc{orgprints43654,
           title = {Principes de la g{\'e}n{\'e}tique quantitative : Chapitre 5 : L'unique effet du hasard sur la structure all{\'e}lique. D{\'e}rive g{\'e}n{\'e}tique, {\'e}tranglement, consanguinit{\'e} et nombre minimum d'individus {\`a} s{\'e}lectionner},
           pages = {1--9},
