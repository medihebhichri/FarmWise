@inproceedings{orgprints24012,
           title = {BRIDGING FARMER EXPERIENCE AND SCIENCE: LEARNING FOR AGROECOLOGICAL DESIGN OF SUSTAINABLE FARMING SYSTEMS},
       publisher = {Johann Heinrich von Th{\"u}nen-Institut},
          author = {Charles Francis and Anna Marie Nicolaysen and Suzanne Morse and Tor Arvid Breland and Geir Lieblein},
