@book{orgprints7021,
          author = {Iris, Autorin; Lehmann},
            year = {2005},
          series = {Sozialwissenschaftliche Schriften zur Landnutzung und l{\"a}ndlichen Entwicklung},
           title = {Wissen und Wissensvermittlung im {\"o}kologischen Landbau in Baden-W{\"u}rttemberg in Geschichte und Gegenwart},
