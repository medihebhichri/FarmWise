@misc{orgprints24672,
            year = {2013},
          author = {Peter Smith and Andreas Lykke-Olesen and Erik Fog and Egon Noe and Hugo Fjelsted Alr{\o}e},
           month = {September},
       publisher = {MultiTrust, Tumblehead},
           title = {Et nyt redskab til kommunikation af multikriterielle vurderinger i den {\o}kologiske v{\ae}rdik{\ae}de - en animationsfilm },
        abstract = {T{\ae}nk nu hvis der er en platform som g{\o}r det muligt at diskutere {\o}kologi mellem alle parter i den {\o}kologiske f{\o}devarek{\ae}de ud fra hver deres kriterier.

I MultiTrust-projektet er der arbejdet med at finde frem til et redskab der kan l{\o}se udfordringen: Hvordan kan man lave helhedsvurderinger af effekterne af {\o}kologisk produktion p{\r a} en s{\r a}dan m{\r a}de at de reelt vil kunne g{\o}re en forskel i praksis. {\O}kologiske produkter skal klare sig i sammenligningen med konventionelle produkter, og ofte kommer {\o}kologiske produkter til at st{\r a} lidt svagt, n{\r a}r der m{\r a}les p{\r a} enkelte kriterier. {\O}kologiens s{\ae}rkende er den helhedsm{\ae}ssige betragtning ? at der er taget hensyn til mange forhold p{\r a} en gang ? og der er derfor brug for helhedsvurderinger. 

Helhedsvurderinger kan imidlertid blive meget komplekse, og dermed sv{\ae}re at kommunikere og forholde sig til. Og det kan v{\ae}re vanskeligt at afg{\o}re hvordan de kan forenkles, fordi en forenkling indeb{\ae}rer en r{\ae}kke valg om hvad der vigtigt og hvordan forskellige kriterier skal v{\ae}gtes. Samtidig er {\o}kologisk jordbrug en produktionsform der til stadighed {\o}nsker at udvikle sig. {\O}kologiske produkter er s{\r a}ledes ikke bare produkter, der er produceret efter de {\o}kologiske regler. Der er mange producenter der gerne vil g{\o}re en ekstra indsats ud over den standard som sikres gennem {\O}-m{\ae}rket. 

De {\o}kologiske produkter indeholder kvaliteter der er bestemt af en r{\ae}kke valg og handlinger som foretages i produktionen og i den efterf{\o}lgende forarbejdning, transport og handel. Der kan fx l{\ae}gges v{\ae}gt p{\r a} hvilke organiske g{\o}dninger der bliver anvendt, hvordan man indretter frugtplantagen for at fremme naturlige fjender, hvordan man sikrer h{\o}j velf{\ae}rd og trivsel hos husdyrene, hvordan man sparer p{\r a} energien i forarbejdningen, hvilken emballagetype man s{\ae}lger de {\o}kologiske produkter i osv. 

N{\r a}r forbrugeren st{\r a}r ved k{\o}ledisken, og n{\r a}r landmanden str{\o}r halm til sine kalve, ved de meget lidt om hvordan de andre led i f{\o}devarek{\ae}den agerer, og de har ikke store muligheder for at komme i kontakt med hinanden. Men hvis de initiativer der tages for at forbedre og udbrede {\o}kologien, skal lykkes, s{\r a} er der brug for at de andre led i k{\ae}den kan honorere den ekstra indsats. Der er brug for at kommunikere og koordinere p{\r a} tv{\ae}rs af k{\ae}den.

Disse udfordringer har MultiTrust-projektet belyst gennem en r{\ae}kke analyser, workshops og h{\o}ringer med repr{\ae}sentanter for de {\o}kologiske akt{\o}rer. Arbejdet er mundet ud i en skitse til et redskab der vil kunne etablere kontakt p{\r a} tv{\ae}rs af k{\ae}den og give brugerne overblik over hvad der er gjort i hele den {\o}kologiske v{\ae}rdik{\ae}de for at give produkterne deres s{\ae}rlige kvaliteter. Et redskab der udg{\o}r en platform hvor landm{\ae}nd, virksomheder, butikker og forbrugere sammen kan arbejde med at udvikle {\o}kologien ud fra mange kriterier p{\r a} en gang.

Projektet har produceret en videofilm der illustrerer problemstillingen og viser hvordan redskabet vil kunne fungere (se filmen p{\r a} YouTube via linket efter "Online at:"), samt et diagram der viser hvordan redskabet kan opbygges og hvordan forskellige akt{\o}rer t{\ae}nkes involveret (se http://zoom.it/P4IO - link i "Related links"). En mere detaljeret beskrivelse af prototypen og brugernes reaktioner p{\r a} et s{\r a}dant redskab er under udarbejdning.
 },
             url = {https://orgprints.org/id/eprint/24672/}
}

@inproceedings{orgprints34444,
            year = {2018},
          author = {Fernando Sousa and Gian L. Nicolay and Christoph Spurk and Noah Adamtey and Andreas Fliessbach},
         journal = {Program and abstract Book of the XXIII International Conference of the Society for Human Ecology "Navigating Complexity: Human-Environmental Solutions for a Challenging Future", July 7-10, 2018; Lisbon, Portugal},
           title = {Dissemination and adoption of bottom-up agriculture to improve soil fertility in Africa: An interdisciplinary approach},
             url = {https://orgprints.org/id/eprint/34444/},
        keywords = {Soil fertility, sub-Saharan Africa, Innovation Adoption, Transdisciplinary Research, Innovation Platforms},
        abstract = {Soil fertility is at stake at a global scale, putting pressure on food security, poverty alleviation and environmental protection, under scenarios of climate change that in most cases aggravate the threat. In sub-Saharan Africa, a combination of depleted soils and population growth adds particular pressure to smallholder farmers and society. Their capacity to innovate in a social, economic, political and cultural context is seen as decisive to reverse the trend of declining soil fertility. However, many technologies with a potential to protect, maintain and build up soil fertility are hardly used by small-scale farmers, triggering the urgent question on their reasoning not to do so.  Exploring and understanding the constraints and complexity of the social systems interacting with the implied institutional dynamics are essential steps in designing appropriate agricultural innovations that are scalable and adoptable. The focus of the inter- and transdisciplinary approach applied in the project ORM4Soil (Organic Resource Management for Soil Fertility; www.orm4soil.net) lies at the heart of this project. We are combining qualitative and quantitative methods from agronomy, sociology and communication sciences in order to bring soil-fertility-enhancing-technologies and their adoption to the center of the decision-making process of farmers? as well as local and regional institutions. At local and regional innovation platforms, stakeholders from business, government, academia and farmer organizations are discussing the outcomes of agronomic trials and sociological research. We are expecting to create bridges between the needs and concerns of farmers, relevant segments of society and policymaking, with the new common goal to enhance soil fertility.}
}

@misc{orgprints36332,
            year = {2018},
          author = {Ann-Kathrin Spiegel and Stephanie Fischinger and Susanne Padel and Bram Moeskops},
           title = {D.2.4: Recommendations for a common European research agenda},
        abstract = {Organic farming is knowledge intensive and in supporting farmers in enhancing their production systems, there is a need to improve how knowledge is shared. This is the overall aim of the OK-Net Arable project. Work Package 2 of the project is concerned with facilitating the testing of practical and educational materials with farmer innovation groups to improve knowledge provision in this sector. The work package adopts an interactive multi-actor approach, bringing together practitioners from regional innovation groups with each other, and with advisers and scientists.  
The aim of this report is to give a brief overview of the most important topics in organic arable farming for a common European research agenda. It identifies topics and open questions that are related to the main obstacles for increasing and stabilising yields in organic arable farming in Europe that should be considered in a common research agenda.  
It builds on the farmers? perspective of knowledge gaps and questions (Cullen et al 2016, D2.1) and the experience from testing knowledge exchange tools with farmers (Bliss et al 2018, D2.2). This is contrasted with the researchers? perspective from the Ok-Net Arable project on which knowledge is already there and which is still needed (Niggli et al 2016, D3.1). Perspectives from the project partners, the partner countries and national research agendas on approaches that bring together the relevant actors to discuss and shape research agendas were also considered.  
The ten recommendations for research topics are based on experiences made in the OK-Net Arable project. They include topics related to cropping systems and interactions, weed management, soil fertility and nutrient management and pest and disease control. Not all topics are equally relevant across the whole of Europe and in all areas both fundamental and applied research is needed. The farmers taking part in the OK-Net arable project were interested in a better understanding of the systemic aspects of organic cropping systems as well as in applied solutions to specific problems. There also is a need for further opportunities for knowledge exchange between farmers and farmers and advisors in Europe.},
             url = {https://orgprints.org/id/eprint/36332/}
}

