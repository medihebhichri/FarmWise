@misc{orgprints8977,
           pages = {5--7},
          volume = {7},
       publisher = {S{\"a}chsische Landesanstalt f{\"u}r Landwirtschaft},
           title = {Das PC-Programm BEFU, Teil {\"O}kologischer Landbau},
            year = {2006},
