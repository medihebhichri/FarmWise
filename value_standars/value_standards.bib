@unpublished{orgprints35398,
          author = {Daniel M{\"u}hlrath and Anna Lotterhos},
            year = {2015},
           title = {Das aktuelle und zuk{\"u}nftige {\"O}ko-Kontrollverfahren aus Sicht ausgew{\"a}hlter Branchenexperten},
