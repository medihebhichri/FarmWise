@misc{orgprints24886,
          editor = {Gorm Harste and Morten Knudsen},
           pages = {89--107},
       publisher = {Samfundslitteratur},
           title = {Jordbrug, strukturelle koblinger og b{\ae}redygtighed},
         address = {Frederiksberg},
          series = {Nyt fra samfundsvidenskaberne},
            year = {2014},
          author = {Egon Noe and Hugo F. Alr{\o}e},
         journal = {Systemteoretiske analyser ? at anvende Luhmann},
        abstract = {Artiklen analyserer udviklingen i landbruget med udgangspunkt i Luhmanns systemteori og med s{\ae}rlig fokus p{\r a} begreberne om afkobling og strukturelle koblinger. I et stadigt mere komplekst samfund er reduktion (eller rettere udgr{\ae}nsning) af kompleksitet en n{\o}dvendig men kontingent udvikling. I landbruget ser vi denne udvikling b{\r a}de i form af en uddifferentiering af semantikker og tilh{\o}rende funktionssystemer og i form af horisontale og vertikale specialiseringer og uddifferentieringer. De logiske konsekvenser heraf er for det f{\o}rste at de enkelte organisations- og funktionssystemers omverden (dvs. den verden de kan lade sig irritere af) bliver forenklet, og dermed sker der en r{\ae}kke afkoblinger. For det andet, og som kompensation for denne udvikling, er det n{\o}dvendigt at udvikle nye strukturelle koblinger mellem disse systemer for at kompensere for uddifferentieringen.  
En yderligere konsekvens af denne udvikling er, at der sker en r{\ae}kke reaktioner i omgivelserne som begynder at irritere tilbage i andre former, og der opst{\r a}r endnu flere specialiserede semantikker.  De semantikker der er uddifferentieret som egentlige funktioner, s{\o}ger at genintroducere irritationen af landbrugsbedrifterne via f.eks. lovgivne restriktioner eller i form af skatter og afgifter. I artiklen fokuserer vi analysen p{\r a} andre og nye muligheder for strukturelle (gen)koblinger, med udgangspunkt i en r{\ae}kke aktuelle tiltag og bev{\ae}gelser; koblinger der kan bidrage til at genintroducere omgivelsernes irritation til systemerne. Vi skelner mellem tre former for strukturelle koblinger: 1) De strukturelle koblinger som landbrugsvirksomheden producerer for at genintroducere omgivelser til omverden, dog i reduceret form via de generaliserede medier som funktionssystemerne tilbyder. 2) Strukturelle koblinger mellem organisationer der kan h{\r a}ndtere andre dimensioner end blot pris og kvantitet. Her skelner vi mellem de medierede koblingsmuligheder som de generaliserede medier, som f.eks. {\O}-m{\ae}rket, tilbyder, og koblingsmuligheder i bindende men dynamiske samarbejdsaftaler i netv{\ae}rket, som afh{\ae}nger af og giver mulighed for ko-evolution. 3) Anden ordens semantiske strukturelle koblinger af iagttagelser af iagttagelser, som f.eks. knytter sig til b{\ae}redygtighedssemantikken; semantikker der netop har deres styrke og udfordring ved at de forbliver ubestemte i deres form. 
Analysen viser at Luhmanns begreb om strukturelle koblinger ikke kun er st{\ae}rkt n{\r a}r det g{\ae}lder om at analysere og forst{\r a} udviklingsprocesserne i jordbruget, men at dette begrebsapparat ogs{\r a} kan danne grundlag for at arbejde konstruktivt med de udfordringer som uddifferentieringen producerer.  
},
             url = {https://orgprints.org/id/eprint/24886/}
}

