          author = {Mette Vaarst and Arthur Getz Escudero and M. Jahi Chappell and Catherine Brinkley and Ravic Nijbroek and Nilson A.M. Arraes and Lise Andreasen and Andreas Gattinger and Gustavo De Almeida and Deborah Bossio and Niels Halberg},
