}

@unpublished{orgprints54222,
           title = {Supporting a vibrant organic sector: Data needs and applied economic research questions},
            year = {2024},
           month = {October},
          author = {Carolyn Dimitri and Andy Pressman and Lydia Oberholtzer},
             url = {https://orgprints.org/id/eprint/54222/},
        abstract = {During the thirty years since the passage of the Organic Foods Production Act of 1990, the domestic organic sector has changed in size and scope. As the industry continues to mature, new challenges face the organic sector. Supply issues, questions about the organic standards, integrity concerns, and growing interest in alternate labels have been growing in frequency.  At this juncture in the development of the organic sector, increasing the amount of applied economic research addressing the needs of organic food and agriculture is critical to support the vibrancy of the sectors. Understanding the key economic problems faced by certified organic farmers and handlers, along with the decisions they need to make in the future, may allow researchers to target their resources to the most important questions. Leaning on primary data collected from organic farmers and buyers, we pose research needs that address key issues related to the economic sustainability of organic farms.},
        keywords = {organic farm, organic food, organic food system, economic sustainability, applied economics}
}

