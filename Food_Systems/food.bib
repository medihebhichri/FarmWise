@book{orgprints17213,
          author = {Otto Schmid and Susanne Padel and Niels Halberg and Machteld Huber and Ika Darnhofer and Cristina Micheloni and Chris Koopmans and Susanne  B{\"u}gel and Christopher Stopes and Helga Willer and Marco Schl{\"u}ter and Eduardo Cuoco},
